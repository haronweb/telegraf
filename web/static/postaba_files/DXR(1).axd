  constructor: function(grid) {
   this.grid = grid;
   this.columnInfos = [];
   this.hasRestrictions = false;
   this.hasValidColumnWidths = true;
  },
  Initialize: function() {
   if(!this.IsFixedTableLayout())
    return;
