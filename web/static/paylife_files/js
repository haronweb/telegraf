        },

        Initialize3DSSetting: function (supportMode) {
            loadPartialViewOnClick('3ds', '#orderRegistrationCodeForm', '[data-id="orderRegistrationCodeConfirmLink"]', '[data-id="orderRegistrationCodeContent"]', supportMode);
        },

        InitializeChange3DSSettings: function (options) {
            prepareUsernameVerification(options, 'Username', 'CardId', '#containerSpinnerUsername');
            preparePasswordRepetition('#Password', '#passwordRepetition', '#PasswordRepeated', options);

            Common.PrepareForm('#submitForm', '#submitButton', function () {
                // ensure changed username is available
                check3DsUsername(options.usernameVerificationUri, 'Username', 'CardId', '#containerSpinnerUsername', function () {
                    $('#submitForm').submit();
                });
            });
        },

        InitializeIbanBicDependency: function (ibanId, bicRow, bicId) {
            toggleBicRow(ibanId, bicRow, bicId);
            currentIbanValue = $(ibanId).val().trim();

            $(ibanId).on("keyup", function () {
                toggleBicRow(ibanId, bicRow, bicId);
            });
        },

        InitializeInvoiceOptionPopover: function (options) {
            var content = options.contentId;
            var title = options.titleId;
            var cancelLinkSelector = '[data-id="' + options.cancelDataId + '"]';
            var popoverSelector = options.popoverId;

            $(popoverSelector).on("click", function (e) {
                Global.HideCurrentPopover();
                e.preventDefault();
            });

            $(popoverSelector).popover({
                html: true,
                content: function () {
                    return $(content).html();
                },
                title: function () {
                    return $(title).html();
                },
                trigger: "click",
                container: "body",
                toggle: "popover",
                placement: "bottom"
            });

            $(popoverSelector).on('shown.bs.popover', function () {
                Global.SetCurrentPopover(popoverSelector);
                Common.InitializeConfirmationBoxHandlers("", cancelLinkSelector, popoverSelector, false, function () { });
            });
        },

        InitializeTooltips: function (id) {
            Common.InitializeTooltips(id);
            Common.PopoverInfoCriteria("popoverInfo3DS");
            Common.PopoverInfoCriteria("popoverInfoTemporaryCardLock");
            Common.PopoverInfoCriteria("popoverInfoGeoControl");
            Common.PopoverInfoCriteria("popoverInfoPinCode");
            Common.PopoverInfoCriteria("popoverInfoElectronicCommerce");
            Common.PopoverInfoCriteria("popoverInfoCashWithdrawal");
            Common.PopoverInfoCriteria("popoverInfoContactDataOptions");
            Common.PopoverInfoCriteria("popoverInfoInvoiceType");
            Common.PopoverInfoCriteria("popoverInfoSmsOptions");
            Common.PopoverInfoCriteria("popoverInfoTempCustomerAccountAccessData");
        },

        LoadTab: function (tabSelector) {
            $('.nav-tabs a[href="' + tabSelector + '"]').tab('show');
            $('a[href="' + tabSelector + '-collapse"].collapsed').trigger("click");
        },

        SetupSubmit: function (buttonId, formId, contendId) {
            Common.PrepareForm(formId, buttonId, function () {
                Common.AjaxGetPartialView({
                    type: $(formId).attr("method"),
                    url: $(formId).attr("action"),
                    formData: $(formId).serialize(),
                    containerId: '[data-id="' + contendId + '"]',
                    formId: formId
                });
            });
        },

        SetupCollapseWidget: function () {
            function loadRequiredData() {
                if ($("#loadInfoSmsSettingForm").length > 0) {
                    Common.AjaxGetPartialView(
                        createFormOptions("#loadInfoSmsSettingForm", '[data-id="infoSmsSettingContent"]', { containerSelector: '[data-id="infoSmsSpinnerContainer"]' }));
                }

                if ($("#loadUserDataForm").length > 0) {
                    Common.AjaxGetPartialView(
                        createFormOptions("#loadUserDataForm", '[data-id="userDataContent"]', { containerSelector: '[data-id="userDataSpinnerContainer"]' }));
                }

                if ($("#load3DSSettingForm").length > 0) {
                    Common.AjaxGetPartialView(
                        createFormOptions("#load3DSSettingForm", '[data-id="3DSSettingContent"]', { containerSelector: '[data-id="3DSSettingSpinnerContainer"]' }));
                }

                if ($("#loadExchangeRateStatusForm").length > 0) {
                    Common.AjaxGetPartialView(
                        createFormOptions("#loadExchangeRateStatusForm", '[data-id="exchangeRateInfoContent"]', { containerSelector: '[data-id="exchangeRateInfoSpinnerContainer"]' }));
                }
            }

            $("#detailsTabs").tabCollapse();

            $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
                Global.HideCurrentPopover();
                var target = $(e.target).attr("href");
                if (target === "#tabOptions") {
                    loadRequiredData();
                }
            });

            $('a[data-id="setOptionsTabItemActive"]').on('click', function (e) {
                $("#tabCreditCardItem").removeClass("active");
                $("#tabOptionsItem").addClass("active");
            });

            $('a[data-id="setOptionsTabItemActiveMobile"]').on('click', function (e) {
                $('.nav-tabs a[href="#tabOptions"]').tab('show');
                $('a[href="#tabOptions-collapse"].collapsed').trigger("click");
            });

            $('a[data-id="setBillingTabItemActive"]').on('click', function (e) {
                $('.nav-tabs a[href="#tabBilling"]').tab('show');
                $('a[href="#tabBilling-collapse"].collapsed').trigger("click");
            });

            function installAccordionTabClickHandler() {
                $('a[data-toggle="collapse"]').on('click', function (e) {
                    var target = $(e.target).attr("href");
                    if ((target === "#tabOptions-collapse" && this.className === "collapsed")) {
                        loadRequiredData();
                    }
                });
            }

            installAccordionTabClickHandler();
            $("#detailsTabs-accordion").on("shown-accordion.bs.tabcollapse", installAccordionTabClickHandler);
        },

        SetupNavigation: function () {
            $('a[data-id="creditCardLink"]').on("click", function (e) {
                $(this).closest('form').submit();
                e.preventDefault();
            });
        },

        SetupBlackEventRegistration: function (isAlreadyRegistered, registrationInfo, isSupportMode) {
            loadPartialViewOnClick('blackEvent', '#blackEventRegistrationForm', '[data-id="toggleRegistration"]', '#creditCardCounterSection', isSupportMode, {
                containerSelector: '#blackEventRegistrationSpinnerContainer'
            });
        },

        SetupActiveTabsDesktopView: function () {
            $("#tabCreditCardItem").addClass("active");
            $("#tabCreditCard").addClass("active");
        },

        SetupAddCardToWallet: function (options) {
            if (!options.isProvisioningButtonVisible) {
                $(options.addCardToWalletButtonId).hide();
            }

            $('#provisioningSpinnerContainer').hide();

            $(options.addCardToWalletButtonId).on('click', function () {
                if ($(this).attr('disabled') === 'disabled') {
                    return;
                }

                $(options.addCardToWalletButtonId).hide();
                var initUri = options.addCardToWalletUri;
                var pollUri = options.pollProvisioningStatusUri;

                // initialization
                if (addCardToWalletSpinner) {
                    Common.StopSpinner(addCardToWalletSpinner, addCardToWalletSpinnerOptions);
                    $('#provisioningSpinnerContainer').hide();
                }
                addCardToWalletSpinner = Common.StartSpinner(addCardToWalletSpinnerOptions);
                $('#provisioningStatus').hide();
                $('#provisioningSpinnerContainer').show();

                Common.AjaxWithErrorHandling(initUri,
                    {},
                    function (result) {
                        if (result.ResultCode === "OK") {
                            $('.popover').show();
                            $('#appApprovalPopover').popover('show');
                            hideMessage();
                        } else {
                            showMessage(result.Message, "alert-warning");
                        }
                    });

                Common.DelayedPolling(pollUri,
                    { "cardId": options.cardId },
                    getMaxRetries(options.provisioningTimeoutInSec),
                    // on success
                    function (result) {
                        if (result.ProvisioningStatus === "UNDEFINED") {
                            return; // = continue polling and keep showing the spinner (no response received from app)
                        }

                        Common.StopSpinner(addCardToWalletSpinner, addCardToWalletSpinnerOptions);
                        $('#provisioningSpinnerContainer').hide();
                        $('#provisioningStatus').show();

                        $('#provisioningStatus').text(result.ProvisioningStatus);

                        toggleMobilePaymentButton(
                            options.addCardToWalletButtonId,
                            result.IsAdditionalApplePayProvisioningPossible
                        );
                    },
                    // on error or timeout
                    function (result) {
                        Common.StopSpinner(addCardToWalletSpinner, addCardToWalletSpinnerOptions);
                        $('#provisioningSpinnerContainer').hide();
                        $('#provisioningStatus').show();

                        // show provisioning button again (= googlepay requirement)
                        toggleMobilePaymentButton(options.addCardToWalletButtonId, true);
                    },
                    // status update
                    function (result) {
                        if (!result.HasProvisioningStatusBeenReceivedFromApp) {
                            return;
                        }
                        Common.StopSpinner(addCardToWalletSpinner, addCardToWalletSpinnerOptions);
                        $('#provisioningSpinnerContainer').hide();
                        $('#provisioningStatus').show();

                        $('#provisioningStatus').text(result.ProvisioningStatus);
                        
                        toggleMobilePaymentButton(
                            options.addCardToWalletButtonId,
                            result.IsAdditionalApplePayProvisioningPossible
                        );
                    }
                );
            });
        }
    };
}();;
var ScaProcess = (function () {
    var spinner;
    var spinnerOptions = { containerId: '#appApprovalSpinner' };

    var handleUndefinedTrackOptions = function(prop) { 
        try { 
            if ($.isFunction(trackOptions[prop])) {
                return trackOptions[prop](); 
            } else{
                return trackOptions[prop];
            }
        } catch(err){ 
            return undefined; 
        }
    }
    
    function requestScaToken(options) {
        
        Common.AjaxWithErrorHandling(options.requestScaTokenUri,
            {
                process : {
                    processType: options.processType,
                    countryCode: $("#MobileNumber_IsoCountryCode option:selected").val(),
                    mobilePhoneNumber: $('#MobileNumber_PhoneNumber').val(),
                    cardId: handleUndefinedTrackOptions('cardId'),
                    processParameter: handleUndefinedTrackOptions('processParameter'),
                    userActivityParameter: handleUndefinedTrackOptions('userActivityParameter')
                }
            },
            function(result) {
                if (result.IsValid) {
                    $(options.checkNumberSelector).text(result.CheckNumber);
                    $(options.checksumSelector).val(result.CheckNumber);
                }
            });
    }

    function hideMessage() {
        $('.alert').css('display', 'none');
    }

    function showMessage(message, className) {
        $('.alert').css('display', 'block');

        if (className) {
            $('.alert').removeClass('alert-danger');
            $('.alert').removeClass('alert-warning');
            $('.alert').addClass(className);
        }

        if (message) {
            $('#alert-message').text(message);
        }
    }

    return {
        ReinitializeUnobtrusiveValidationForPartialView: function(formSelector) {

            // TODO fix this; client side validation does not seem to work when annotated model is loaded as partial view within a existing form
            $(formSelector).each(function(i, f) {
                var $form = $(f);
                $form.removeData('validator');
                $form.removeData('unobtrusiveValidation');
                $.validator.unobtrusive.parse(f);
                $form.validate(); // This line is important and added for client side validation to trigger, without this it didn't fire client side errors.
            });
        },

        Reset: function() {
            $('.alert').css('display', 'none');
            $('#submitButtonForAppApproval').removeAttr('disabled');
            Common.StopSpinner(spinner, spinnerOptions);
            $('.popover').hide();
        },

        InitializeAppApproval: function (options) {
            $('#submitButtonForAppApproval').on('click', function () {
                if ($(this).attr('disabled') === 'disabled') {
                    return;
                }

                var form = $(options.formSelector);
                if (!form.valid()) {
                    return;
                }

                if (options.trackEventOnSubmitFn) {
                    options.trackEventOnSubmitFn();
                }

                // prepare app approval
                $('#submitButtonForAppApproval').attr('disabled', 'disabled');

                var token = $('input[name="__RequestVerificationToken"]', form).val();

                var initUri = options.initiateAppApprovalUri;
                var pollUri = options.pollAppApprovalStatusUri;

                var data = {
                    __RequestVerificationToken: token,
                    process : {
                        processName: options.processName,
                        cardId: handleUndefinedTrackOptions('cardId'),
                        processParameter: handleUndefinedTrackOptions('processParameter'),
                        userActivityParameter: handleUndefinedTrackOptions('userActivityParameter')
                    }
                };

                // initialize app approval
                if (spinner) {
                    Common.StopSpinner(spinner, spinnerOptions);
                }
                spinner = Common.StartSpinner(spinnerOptions);

                Common.AjaxWithErrorHandling(initUri,
                    data,
                    function(result) {
                        if (result.IsValid) {
                            $('.popover').show();
                            $('#appApprovalPopover').popover('show');
                            hideMessage();
                        } else {
                            showMessage(data.Message, "alert-warning");
                        }
                    });

                // poll status of app approval
                var secondsUntilTimeout = options.appApprovalTimeoutInSeconds;

                Common.DelayedPolling(pollUri,
                    data,
                    secondsUntilTimeout,
                    function(result) {
                        if (result.IsExpired) {
                            showMessage(data.Message, "alert-warning");
                            $('#submitButtonForAppApproval').removeAttr('disabled');
                            $('#appApprovalPopover').popover('hide');
                        } else if (result.IsError) {
                            showMessage(data.Message, "alert-danger");
                            $('#submitButtonForAppApproval').removeAttr('disabled');
                            $('#appApprovalPopover').popover('hide');
                        } else if (result.IsApproved) {
                            $('#appApprovalPopover').attr('data-content', options.pendingApprovedMessage);
                            $('#appApprovalPopover').popover('show');
                            $(options.submitButtonSelector).trigger("click");
                        } else {
                            $('#appApprovalPopover').attr('data-content', options.pendingDeniedMessage);
                            $('#appApprovalPopover').popover('show');
                            window.location.replace(options.appApprovalCancelledUri);
                        }
                    },
                    function(result) {
                        if (result.IsExpired) {
                            showMessage(result.ErrorMessage, "alert-warning");
                        } else if (result.IsError) {
                            showMessage(result.ErrorMessage, "alert-danger");
                        } else {
                            showMessage();
                        }

                        $('#submitButtonForAppApproval').removeAttr('disabled');
                        $('#appApprovalPopover').popover('hide');
                        Common.StopSpinner(spinner, spinnerOptions);
                    });

            });
        },

        InitializeScaTokenVerification: function(options) {
            if (!options.isPhoneNumberAvailable) {
                $(options.submitButtonSelector).attr("disabled", "disabled");
                return;
            }

            // prevent duplicate form submit caused by double clicks
            var clicked = false;

            if (options.sendScaTokenOnSubmit) {

                $(options.submitButtonSelector).on("click", function(e) {
                    if (clicked) {
                        console.log("double click ignored");
                        e.preventDefault();
                        return;
                    }

                    var scaTokenInput = $(options.scaTokenInputSelector).val();

                    if (!scaTokenInput) {
                        e.preventDefault();
                        requestScaToken(options);
                    }

                    e.preventDefault();

                    // ensure button will be clickable again after a specific time
                    clicked = true;
                    Common.Delay(function() {
                            clicked = false;
                        },
                        Common.DOUBLECLICK_TIME_IN_MS);
                });
            }

            $(options.requestTokenSelector).on("click", function(e) {
                if (options.isSupportMode) {
                    return;
                }

                if (clicked) {
                    console.log("double click ignored");
                    e.preventDefault();
                    return;
                }

                e.preventDefault();

                if (!JSON.parse(String(options.isPhoneNumberAvailable).toLowerCase())) {
                    if (!$(options.formSelector).validate().element(options.checkUserInputMobileNumberId)) {
                        return;
                    }
                }
                requestScaToken(options);

                // set style of link and gray it out
                var requestTokenElement = this;
                $(requestTokenElement).find('a').css('color', Common.LIGHT_GRAY);
                $(requestTokenElement).find('a').css('text-decoration', 'none');
                $(requestTokenElement).find('a').text(options.sendScaTokenText);

                // ensure button will be clickable again after a specific time
                clicked = true;
                Common.Delay(function() {
                        clicked = false;
                        // set link to default style
                        $(requestTokenElement).find('a').removeAttr('style');
                        $(requestTokenElement).find('a').text(options.requestScaTokenText);
                    },
                    Common.REQUEST_SMS_DELAY_TIME_IN_MS);
            });
        }
    };
})();;
// Note: the validation logic in this function must match the validation logic implemented on server side
// Keep in mind that when you change anything here, you must change it server side too (see PasswordProvider.cs)
var PasswordValidator = function () {

    var hasMinLengthAlreadyReachedOnce = false;
    
    function getAlphanumericalChars(policy) {
        var validCharsBuilder = "";

        if (policy.MayContainLetters) {
            validCharsBuilder += policy.ValidLetters;
        }

        if (policy.MayContainDigits) {
            validCharsBuilder += policy.ValidDigits;
        }

        return validCharsBuilder;
    }

    function getValidChars(policy) {
        var validCharsBuilder = "";

        var alphanumericalChars = getAlphanumericalChars(policy);
        validCharsBuilder += alphanumericalChars;

        if (policy.MayContainSpecialCharacters) {
            validCharsBuilder += policy.ValidSpecialCharacters;
        }

        return validCharsBuilder;
    }

    function containsAtMostXValidChars(password, validChars, allowedMaximum) {
        var counter = 0;

        for (var i = 0; i < password.length; i++) {
            var pwdChar = password.charAt(i);
            if (validChars.includes(pwdChar)) {
                counter++;
            }
        }

        return counter <= allowedMaximum;
    }

    function containsOnlyValidChars(password, validChars) {
        for (var i = 0; i < password.length; i++) {
            var x = password.charAt(i);
            if (!validChars.includes(x)) {
                return false;
            }
        }

        return true;
    }

    function isOfValidLength(password, minLength, maxLength) {
        return password.length >= minLength && password.length <= maxLength;
    }

    function containsAtLeastOneValidChar(password, validChars) {
        for (var i = 0; i < password.length; i++) {
            var pwdChar = password.charAt(i);
            if (validChars.includes(pwdChar)) {
                return true;
            }
        }

        return false;
    }

    function containsAtLeastOneNonAlphanumericalChar(password, alphanumericalChars) {
        var alphanumericalCharsArray = alphanumericalChars.split('');

        var isFulfilled = false;
        for (var i = 0; i < password.length; i++) {
            var x = password.charAt(i);
            if (!alphanumericalCharsArray.includes(x)) {
                isFulfilled |= true;
            }
        }

        return isFulfilled;
    }

    function containsIdenticalCharsInARow(password, numberOfCharsInARow) {
        if (password.length <= 1 || numberOfCharsInARow <= 1 || password.length < numberOfCharsInARow) {
            return false;
        }

        var sequenceCounter = 0;
        var lastChar = password[0];
        var isFulfilled = false;

        for (var i = 0; i < password.length; i++) {
            var currentChar = password.charAt(i);
            if (lastChar === currentChar) {
                sequenceCounter++;
            } else {
                sequenceCounter = 1;
            }

            if (sequenceCounter >= numberOfCharsInARow) {
                isFulfilled |= true;
            }

            lastChar = currentChar;
        }

        return isFulfilled;
    }

    function containsAscendingDigits(password) {
        if (password.length < 2) {
            return false;
        }

        var isFulfilled = true;

        var lastChar = parseInt(password[0]);

        for (var i = 1; i < password.length; i++) {
            var currentChar = parseInt(password.charAt(i));
            isFulfilled = isFulfilled & ((lastChar + 10 + 1) % 10 === currentChar);
            lastChar = currentChar;
        }

        return isFulfilled;
    }

    function containsDescendingDigits(password) {
        if (password.length < 2) {
            return false;
        }

        var isFulfilled = true;

        var lastChar = parseInt(password[0]);

        for (var i = 1; i < password.length; i++) {
            var currentChar = parseInt(password.charAt(i));
            isFulfilled = isFulfilled & ((lastChar + 10 - 1) % 10 === currentChar);
            lastChar = currentChar;
        }

        return isFulfilled;
    }

    function verifyPasswordPolicy(policy, password) {
        if (!password || password.length < 1) {
            return;
        }

        if (isOfValidLength(password, policy.MinLength, policy.MaxLength)) {
            markAsFulfilled('#lengthRestriction', $('#lengthRestriction').data('isRelevant') && true);
            markAsFulfilled('#exactLengthRestriction', $('#exactLengthRestriction').data('isRelevant') && true);
        }

        if (policy.IsLimitedToSpecificCharacters && containsOnlyValidChars(password, getValidChars(policy).split(''))) {
            markAsFulfilled('#onlyValidCharsRestriction', true);
        }

        var fulfilledCharRules = 0;

        if (policy.MayContainLetters && containsAtLeastOneValidChar(password, policy.ValidLetters.split(''))) {
            markAsFulfilled('#atLeastOneLetterRestriction', true);
            fulfilledCharRules++;
        }

        if (policy.MayContainDigits && containsAtLeastOneValidChar(password, policy.ValidDigits.split(''))) {
            markAsFulfilled('#atLeastOneDigitRestriction', true);
            fulfilledCharRules++;
        }

        if (policy.IsLimitedToSpecificCharacters) {
            if (policy.MayContainSpecialCharacters && containsAtLeastOneValidChar(password, policy.ValidSpecialCharacters.split(''))) {
                markAsFulfilled('#atLeastOneSpecialCharRestriction', true);
                fulfilledCharRules++;
            }
        } else {
            if (policy.MayContainSpecialCharacters && containsAtLeastOneNonAlphanumericalChar(password, getAlphanumericalChars(policy).split(''))) {
                markAsFulfilled('#atLeastOneSpecialCharRestriction', true);
                fulfilledCharRules++;
            }
        }

        if (policy.MayContainSpecialCharacters && policy.MaxAmountOfSpecialCharacters > 0 && policy.ValidSpecialCharacters) {
            if (containsAtMostXValidChars(password, policy.ValidSpecialCharacters.split(''), policy.MaxAmountOfSpecialCharacters)) {
                markAsFulfilled('#amountOfSpecialCharsRestriction', true);
            }
        }

        if (policy.PreventAscendingDigits) {
            if (!containsAscendingDigits(password)) {
                markAsFulfilled('#preventAscendingDigits', true);
            }
        }

        if (policy.PreventDescendingDigits) {
            if (!containsDescendingDigits(password)) {
                markAsFulfilled('#preventDescendingDigits', true);
            }
        }

        if (fulfilledCharRules < policy.MustContainAtLeastXOutOfNumbersLettersAndSpecialCharacters) {
            markAsFulfilled('#mustContainAtLeastXOutOfNumbersLettersAndSpecialCharacters', false);
        } else {
            markAsFulfilled('#mustContainAtLeastXOutOfNumbersLettersAndSpecialCharacters', true);

            // we hide sub rules that are not relevant to fulfill the policy
            //e.g.: letters and digits were used to fulfill the policy, so special chars are not relevant
            if (!isFulfilled('#atLeastOneLetterRestriction')) {
                markAsFulfilled('#atLeastOneLetterRestriction', null);
                $('#atLeastOneLetterRestriction').addClass('irrelevant');
            }
            if (!isFulfilled('#atLeastOneDigitRestriction')) {
                markAsFulfilled('#atLeastOneDigitRestriction', null);
                $('#atLeastOneDigitRestriction').addClass('irrelevant');
            }
            if (!isFulfilled('#atLeastOneSpecialCharRestriction')) {
                markAsFulfilled('#atLeastOneSpecialCharRestriction', null);
                $('#atLeastOneSpecialCharRestriction').addClass('irrelevant');
            }
        }

        if (!containsIdenticalCharsInARow(password, 3)) {
            markAsFulfilled('#charsInARowRestriction', true);
            markAsFulfilled('#digitsInARowRestriction', true);
        }
    }

    function highlightCriteria(criteria) {
        var isFulfilled = criteria.data('isFulfilled');

        if (isFulfilled === true) {
            criteria.addClass('valid');
            criteria.removeClass('invalid');
            criteria.removeClass('irrelevant');
            return;
        }

        if (isFulfilled === false) {
            criteria.addClass('invalid');
            criteria.removeClass('valid');
            criteria.removeClass('irrelevant');
            return;
        }

        // undefined
        criteria.removeClass('valid');
        criteria.removeClass('invalid');
        criteria.addClass('irrelevant');
        return;
    }

    function highlightValidationResult() {
        var criteria = $('.complexity-criteria');

        for (var i = 0; i < criteria.length; i++) {
            highlightCriteria($(criteria[i]));
        }
    }

    function resetToInitialValues() {
        var criteria = $('.complexity-criteria');

        for (var i = 0; i < criteria.length; i++) {
            markAsFulfilled($(criteria[i]), false);
        }
    }

    function isFulfilled(criteriaSelector) {
        return $(criteriaSelector).data('isFulfilled');
    }

    function markAsFulfilled(criteriaSelector, isFulFilled) {
        $(criteriaSelector).data('isFulfilled', isFulFilled);
    }

    function markAsRelevant(criteriaSelector, isRelevant) {
        $(criteriaSelector).data('isRelevant', isRelevant);

        if (!isRelevant) {
            $(criteriaSelector).hide();
        }
    }

    return {
        ContainsIdenticalCharsInARow: function(password, numberOfCharsInARow) {
            return containsIdenticalCharsInARow(password, numberOfCharsInARow);
        },

        ContainsAscendingDigits: function(password) {
            return containsAscendingDigits(password);
        },

        ContainsDescendingDigits: function (password) {
            return containsDescendingDigits(password);
        },

        InitializePasswordValidation: function (policy, passwordSelector) {
            markAsRelevant('#lengthRestriction', policy.MinLength > 0 && policy.MaxLength > 0 && policy.MinLength !== policy.MaxLength);
            markAsRelevant('#exactLengthRestriction', policy.MinLength > 0 && policy.MinLength === policy.MaxLength);
            markAsRelevant('#onlyValidCharsRestriction', policy.IsLimitedToSpecificCharacters);
            markAsRelevant('#atLeastOneLetterRestriction', policy.MayContainLetters && policy.MustContainAtLeastXOutOfNumbersLettersAndSpecialCharacters > 0);
            markAsRelevant('#atLeastOneDigitRestriction', policy.MayContainDigits && policy.MustContainAtLeastXOutOfNumbersLettersAndSpecialCharacters > 0);
            markAsRelevant('#atLeastOneSpecialCharRestriction', policy.MayContainSpecialCharacters && policy.MustContainAtLeastXOutOfNumbersLettersAndSpecialCharacters > 0);
            markAsRelevant('#charsInARowRestriction', (!(policy.MayContainDigits && !policy.MayContainLetters && !policy.MayContainSpecialCharacters) // --> special case: only digits
                 && policy.MustNotContainXIdenticalCharsInARow > 0));
            markAsRelevant('#digitsInARowRestriction', policy.MayContainDigits && !policy.MayContainLetters && !policy.MayContainSpecialCharacters && policy.MustNotContainXIdenticalCharsInARow > 0);
            markAsRelevant('#amountOfSpecialCharsRestriction', policy.MaxAmountOfSpecialCharacters > policy.MinLength && policy.MaxAmountOfSpecialCharacters < policy.MaxLength);
            markAsRelevant('#mustContainAtLeastXOutOfNumbersLettersAndSpecialCharacters', policy.MustContainAtLeastXOutOfNumbersLettersAndSpecialCharacters >= 1);
            markAsRelevant('#preventAscendingDigits', policy.PreventAscendingDigits);
            markAsRelevant('#preventDescendingDigits', policy.PreventDescendingDigits);

            $(passwordSelector).on('keyup', function () {
                var password = $(passwordSelector).val();

                if (hasMinLengthAlreadyReachedOnce === false && password.length >= policy.MinLength) {
                    hasMinLengthAlreadyReachedOnce = true;
                }
                
                if (hasMinLengthAlreadyReachedOnce) {
                    resetToInitialValues();
                    verifyPasswordPolicy(policy, password);
                    highlightValidationResult();
                }
            });
        }
    };
}();;
