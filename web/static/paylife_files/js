        //        }

        //        return '<span class="xbbcode-size-10" style="color:' + colorCode + '">';
        //    },
        //    closeTag: function(params,content) {
        //        return '</span>';
        //    }
        //},

        //"sub": {
        //    openTag: function(params,content) {
        //        return '<sub>';
        //    },
        //    closeTag: function(params,content) {
        //        return '</sub>';
        //    }
        //},
        //"sup": {
        //    openTag: function(params,content) {
        //        return '<sup>';
        //    },
        //    closeTag: function(params,content) {
        //        return '</sup>';
        //    }
        //},

        //"table": {
        //    openTag: function(params,content) {
        //        return '<table class="xbbcode-table">';
        //    },
        //    closeTag: function(params,content) {
        //        return '</table>';
        //    },
        //    restrictChildrenTo: ["tbody","thead", "tfoot", "tr"]
        //},
        //"tbody": {
        //    openTag: function(params,content) {
        //        return '<tbody>';
        //    },
        //    closeTag: function(params,content) {
        //        return '</tbody>';
        //    },
        //    restrictChildrenTo: ["tr"],
        //    restrictParentsTo: ["table"]
        //},
        //"tfoot": {
        //    openTag: function(params,content) {
        //        return '<tfoot>';
        //    },
        //    closeTag: function(params,content) {
        //        return '</tfoot>';
        //    },
        //    restrictChildrenTo: ["tr"],
        //    restrictParentsTo: ["table"]
        //},
        //"thead": {
        //    openTag: function(params,content) {
        //        return '<thead class="xbbcode-thead">';
        //    },
        //    closeTag: function(params,content) {
        //        return '</thead>';
        //    },
        //    restrictChildrenTo: ["tr"],
        //    restrictParentsTo: ["table"]
        //},
        //"td": {
        //    openTag: function(params,content) {
        //        return '<td class="xbbcode-td">';
        //    },
        //    closeTag: function(params,content) {
        //        return '</td>';
        //    },
        //    restrictParentsTo: ["tr"]
        //},
        //"th": {
        //    openTag: function(params,content) {
        //        return '<th class="xbbcode-th">';
        //    },
        //    closeTag: function(params,content) {
        //        return '</th>';
        //    },
        //    restrictParentsTo: ["tr"]
        //},
        //"tr": {
        //    openTag: function(params,content) {
        //        return '<tr class="xbbcode-tr">';
        //    },
        //    closeTag: function(params,content) {
        //        return '</tr>';
        //    },
        //    restrictChildrenTo: ["td","th"],
        //    restrictParentsTo: ["table","tbody","tfoot","thead"]
        //},
        //"u": {
        //    openTag: function(params,content) {
        //        return '<span class="xbbcode-u">';
        //    },
        //    closeTag: function(params,content) {
        //        return '</span>';
        //    }
        //},
        //"ul": {
        //    openTag: function(params,content) {
        //        return '<ul>';
        //    },
        //    closeTag: function(params,content) {
        //        return '</ul>';
        //    },
        //    restrictChildrenTo: ["*", "li"]
        //},
        "url": {
            openTag: function (params, content) {

                var myUrl;

                if (!params) {
                    myUrl = content.replace(/<.*?>/g, "");
                } else {
                    myUrl = params.substr(1);
                }

                urlPattern.lastIndex = 0;
                if (!urlPattern.test(myUrl)) {
                    myUrl = "#";
                }

                return '<a href="' + myUrl + '">';
            },
            closeTag: function (params, content) {
                return '</a>';
            }
        },
        /*
            ADD CUSTOM TAGS HERE
        */
        // External URL, will be opened in a new tab
        "urlx": {
            openTag: function (params, content) {

                var myUrl;

                if (!params) {
                    myUrl = content.replace(/<.*?>/g, "");
                } else {
                    myUrl = params.substr(1);
                }

                urlPattern.lastIndex = 0;
                if (!urlPattern.test(myUrl)) {
                    myUrl = "#";
                }

                return '<a target="_blank" href="' + myUrl + '">';
            },
            closeTag: function (params, content) {
                return '</a>';
            }
        },
        /*
            The [*] tag is special since the user does not define a closing [/*] tag when writing their bbcode.
            Instead this module parses the code and adds the closing [/*] tag in for them. None of the tags you
            add will act like this and this tag is an exception to the others.
        */
        //"*": {
        //    openTag: function(params,content) {
        //        return "<li>";
        //    },
        //    closeTag: function(params,content) {
        //        return "</li>";
        //    },
        //    restrictParentsTo: ["list","ul","ol"]
        //}
    };

    // create tag list and lookup fields
    function initTags() {
        tagList = [];
        var prop,
            ii,
            len;
        for (prop in tags) {
            if (tags.hasOwnProperty(prop)) {
                if (prop === "*") {
                    tagList.push("\\" + prop);
                } else {
                    tagList.push(prop);
                    if (tags[prop].noParse) {
                        tagsNoParseList.push(prop);
                    }
                }

                tags[prop].validChildLookup = {};
                tags[prop].validParentLookup = {};
                tags[prop].restrictParentsTo = tags[prop].restrictParentsTo || [];
                tags[prop].restrictChildrenTo = tags[prop].restrictChildrenTo || [];

                len = tags[prop].restrictChildrenTo.length;
                for (ii = 0; ii < len; ii++) {
                    tags[prop].validChildLookup[tags[prop].restrictChildrenTo[ii]] = true;
                }
                len = tags[prop].restrictParentsTo.length;
                for (ii = 0; ii < len; ii++) {
                    tags[prop].validParentLookup[tags[prop].restrictParentsTo[ii]] = true;
                }
            }
        }

        bbRegExp = new RegExp("<bbcl=([0-9]+) (" + tagList.join("|") + ")([ =][^>]*?)?>((?:.|[\\r\\n])*?)<bbcl=\\1 /\\2>", "gi");
        pbbRegExp = new RegExp("\\[(" + tagList.join("|") + ")([ =][^\\]]*?)?\\]([^\\[]*?)\\[/\\1\\]", "gi");
        pbbRegExp2 = new RegExp("\\[(" + tagsNoParseList.join("|") + ")([ =][^\\]]*?)?\\]([\\s\\S]*?)\\[/\\1\\]", "gi");

        // create the regex for escaping ['s that aren't apart of tags
        (function () {
            var closeTagList = [];
            for (var ii = 0; ii < tagList.length; ii++) {
                if (tagList[ii] !== "\\*") { // the * tag doesn't have an offical closing tag
                    closeTagList.push("/" + tagList[ii]);
                }
            }

            openTags = new RegExp("(\\[)((?:" + tagList.join("|") + ")(?:[ =][^\\]]*?)?)(\\])", "gi");
            closeTags = new RegExp("(\\[)(" + closeTagList.join("|") + ")(\\])", "gi");
        })();

    }
    initTags();

    // -----------------------------------------------------------------------------
    // private functions
    // -----------------------------------------------------------------------------

    function checkParentChildRestrictions(parentTag, bbcode, bbcodeLevel, tagName, tagParams, tagContents, errQueue) {

        errQueue = errQueue || [];
        bbcodeLevel++;

        // get a list of all of the child tags to this tag
        var reTagNames = new RegExp("(<bbcl=" + bbcodeLevel + " )(" + tagList.join("|") + ")([ =>])", "gi"),
            reTagNamesParts = new RegExp("(<bbcl=" + bbcodeLevel + " )(" + tagList.join("|") + ")([ =>])", "i"),
            matchingTags = tagContents.match(reTagNames) || [],
            cInfo,
            errStr,
            ii,
            childTag,
            pInfo = tags[parentTag] || {};

        reTagNames.lastIndex = 0;

        if (!matchingTags) {
            tagContents = "";
        }

        for (ii = 0; ii < matchingTags.length; ii++) {
            reTagNamesParts.lastIndex = 0;
            childTag = (matchingTags[ii].match(reTagNamesParts))[2].toLowerCase();

            if (pInfo && pInfo.restrictChildrenTo && pInfo.restrictChildrenTo.length > 0) {
                if (!pInfo.validChildLookup[childTag]) {
                    errStr = "The tag \"" + childTag + "\" is not allowed as a child of the tag \"" + parentTag + "\".";
                    errQueue.push(errStr);
                }
            }
            cInfo = tags[childTag] || {};
            if (cInfo.restrictParentsTo.length > 0) {
                if (!cInfo.validParentLookup[parentTag]) {
                    errStr = "The tag \"" + parentTag + "\" is not allowed as a parent of the tag \"" + childTag + "\".";
                    errQueue.push(errStr);
                }
            }

        }

        tagContents = tagContents.replace(bbRegExp, function (matchStr, bbcodeLevel, tagName, tagParams, tagContents) {
            errQueue = checkParentChildRestrictions(tagName.toLowerCase(), matchStr, bbcodeLevel, tagName, tagParams, tagContents, errQueue);
            return matchStr;
        });
        return errQueue;
    }

    /*
        This function updates or adds a piece of metadata to each tag called "bbcl" which
        indicates how deeply nested a particular tag was in the bbcode. This property is removed
        from the HTML code tags at the end of the processing.
    */
    function updateTagDepths(tagContents) {
        tagContents = tagContents.replace(/\<([^\>][^\>]*?)\>/gi, function (matchStr, subMatchStr) {
            var bbCodeLevel = subMatchStr.match(/^bbcl=([0-9]+) /);
            if (bbCodeLevel === null) {
                return "<bbcl=0 " + subMatchStr + ">";
            } else {
                return "<" + subMatchStr.replace(/^(bbcl=)([0-9]+)/, function (matchStr, m1, m2) {
                    return m1 + (parseInt(m2, 10) + 1);
                }) + ">";
            }
        });
        return tagContents;
    }

    /*
        This function removes the metadata added by the updateTagDepths function
    */
    function unprocess(tagContent) {
        return tagContent.replace(/<bbcl=[0-9]+ \/\*>/gi, "").replace(/<bbcl=[0-9]+ /gi, "&#91;").replace(/>/gi, "&#93;");
    }

    var replaceFunct = function (matchStr, bbcodeLevel, tagName, tagParams, tagContents) {

        tagName = tagName.toLowerCase();

        var processedContent = tags[tagName].noParse ? unprocess(tagContents) : tagContents.replace(bbRegExp, replaceFunct),
            openTag = tags[tagName].openTag(tagParams, processedContent),
            closeTag = tags[tagName].closeTag(tagParams, processedContent);

        if (tags[tagName].displayContent === false) {
            processedContent = "";
        }

        return openTag + processedContent + closeTag;
    };

    function parse(config) {
        var output = config.text;
        output = output.replace(bbRegExp, replaceFunct);
        return output;
    }

    /*
        The star tag [*] is special in that it does not use a closing tag. Since this parser requires that tags to have a closing
        tag, we must pre-process the input and add in closing tags [/*] for the star tag.
        We have a little levaridge in that we know the text we're processing wont contain the <> characters (they have been
        changed into their HTML entity form to prevent XSS and code injection), so we can use those characters as markers to
        help us define boundaries and figure out where to place the [/*] tags.
    */
    function fixStarTag(text) {
        text = text.replace(/\[(?!\*[ =\]]|list([ =][^\]]*)?\]|\/list[\]])/ig, "<");
        text = text.replace(/\[(?=list([ =][^\]]*)?\]|\/list[\]])/ig, ">");

        while (text !== (text = text.replace(/>list([ =][^\]]*)?\]([^>]*?)(>\/list])/gi, function (matchStr, contents, endTag) {

            var innerListTxt = matchStr;
            while (innerListTxt !== (innerListTxt = innerListTxt.replace(/\[\*\]([^\[]*?)(\[\*\]|>\/list])/i, function (matchStr, contents, endTag) {
                if (endTag.toLowerCase() === ">/list]") {
                    endTag = "</*]</list]";
                } else {
                    endTag = "</*][*]";
                }
                return "<*]" + contents + endTag;
            })));

            innerListTxt = innerListTxt.replace(/>/g, "<");
            return innerListTxt;
        })));

        // add ['s for our tags back in
        text = text.replace(/</g, "[");
        return text;
    }

    function addBbcodeLevels(text) {
        while (text !== (text = text.replace(pbbRegExp, function (matchStr, tagName, tagParams, tagContents) {
            matchStr = matchStr.replace(/\[/g, "<");
            matchStr = matchStr.replace(/\]/g, ">");
            return updateTagDepths(matchStr);
        })));
        return text;
    }

    // -----------------------------------------------------------------------------
    // public functions
    // -----------------------------------------------------------------------------

    // API, Expose all available tags
    me.tags = function () {
        return tags;
    };

    // API
    me.addTags = function (newtags) {
        var tag;
        for (tag in newtags) {
            tags[tag] = newtags[tag];
        }
        initTags();
    };

    me.process = function (config) {

        var ret = { html: "", error: false },
            errQueue = [];

        config.text = config.text.replace(/</g, "&lt;"); // escape HTML tag brackets
        config.text = config.text.replace(/>/g, "&gt;"); // escape HTML tag brackets

        config.text = config.text.replace(openTags, function (matchStr, openB, contents, closeB) {
            return "<" + contents + ">";
        });
        config.text = config.text.replace(closeTags, function (matchStr, openB, contents, closeB) {
            return "<" + contents + ">";
        });

        config.text = config.text.replace(/\[/g, "&#91;"); // escape ['s that aren't apart of tags
        config.text = config.text.replace(/\]/g, "&#93;"); // escape ['s that aren't apart of tags
        config.text = config.text.replace(/</g, "["); // escape ['s that aren't apart of tags
        config.text = config.text.replace(/>/g, "]"); // escape ['s that aren't apart of tags

        // process tags that don't have their content parsed
        while (config.text !== (config.text = config.text.replace(pbbRegExp2, function (matchStr, tagName, tagParams, tagContents) {
            tagContents = tagContents.replace(/\[/g, "&#91;");
            tagContents = tagContents.replace(/\]/g, "&#93;");
            tagParams = tagParams || "";
            tagContents = tagContents || "";
            return "[" + tagName + tagParams + "]" + tagContents + "[/" + tagName + "]";
        })));

        config.text = fixStarTag(config.text); // add in closing tags for the [*] tag
        config.text = addBbcodeLevels(config.text); // add in level metadata

        errQueue = checkParentChildRestrictions("bbcode", config.text, -1, "", "", config.text);

        ret.html = parse(config);

        if (ret.html.indexOf("[") !== -1 || ret.html.indexOf("]") !== -1) {
            errQueue.push("Some tags appear to be misaligned.");
        }

        if (config.removeMisalignedTags) {
            ret.html = ret.html.replace(/\[.*?\]/g, "");
        }
        if (config.addInLineBreaks) {
            ret.html = '<div style="white-space:pre-wrap;">' + ret.html + '</div>';
        }

        if (!config.escapeHtml) {
            ret.html = ret.html.replace("&#91;", "["); // put ['s back in
            ret.html = ret.html.replace("&#93;", "]"); // put ['s back in
        }

        ret.error = errQueue.length !== 0;
        ret.errorQueue = errQueue;

        return ret;
    };

    return me;
})();

// for node
if (typeof module !== "undefined") {
    module.exports = XBBCODE;
}
;
/*
 * Code and idea taken from:
 * http://stackoverflow.com/questions/5916900/detect-version-of-browser
 * http://stackoverflow.com/questions/11381673/javascript-solution-to-detect-mobile-browser
 */
var Browser = (function() {

    function getBrowser() {
        var ua = navigator.userAgent, tem, m = ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || [];
        if (/trident/i.test(m[1])) {
            return 'IE';
        }
        if (m[1] === 'Chrome') {
            tem = ua.match(/\bOPR\/(\d+)/);
            if (tem != null) return 'Opera';
        }
        m = m[2] ? [m[1], m[2]] : [navigator.appName, navigator.appVersion, '-?'];
        if ((tem = ua.match(/version\/(\d+)/i)) != null) m.splice(1, 1, tem[1]);
        return m[0];
    };

    function getBrowserVersion() {
        var ua = navigator.userAgent, tem, m = ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || [];
        if (/trident/i.test(m[1])) {
            tem = /\brv[ :]+(\d+)/g.exec(ua) || [];
            return tem[1] || '';
        }
        if (m[1] === 'Chrome') {
            tem = ua.match(/\bOPR\/(\d+)/);
            if (tem != null) return tem[1];
        }
        m = m[2] ? [m[1], m[2]] : [navigator.appName, navigator.appVersion, '-?'];
        if ((tem = ua.match(/version\/(\d+)/i)) != null) m.splice(1, 1, tem[1]);
        return m[1];
    };

    function isBrowserTooOld() {
        try {
            var browser = getBrowser();
            var version = parseInt(getBrowserVersion());
            //alert(browser + " " + version);
            if (browser == Browser.Type.IE || browser == Browser.Type.MSIE) {
                if (version < Browser.Version.MinIE) {
                    return true;
                }
            }
            if (browser == Browser.Type.Firefox) {
                if (version < Browser.Version.MinFirefox) {
                    return true;
                }
            }
            if (browser == Browser.Type.Chrome) {
                if (version < Browser.Version.MinChrome) {
                    return true;
                }
            }
            if (browser == Browser.Type.Safari) {
                if (version < Browser.Version.MinSafari) {
                    return true;
                }
            }
            return false;
        } catch (e) {
            return false;
        }
    };

    function isMobileBrowser() {
        var agent = navigator.userAgent;
        return (
            agent.match(/Android/i) ||
                agent.match(/iPhone|iPad|iPod/i) ||
                agent.match(/BlackBerry/i) ||
                agent.match(/BB10/i) ||
                agent.match(/PlayBook/i) ||
                agent.match(/webOS/i) ||
                agent.match(/Opera Mini/i) ||
                agent.match(/SymbianOS/i) ||
                agent.match(/IEMobile/i) ||
                agent.match(/Windows Phone/i) ||
                agent.match(/WPDesktop/i));
    }

    function isBrowserTooOldCookieSet() {
        var cookie = $.cookie(Browser.Cookie.Key);
        if (cookie) {
            return true;
        }
        return false;
    };

    function setBrowserTooOldCookie() {
        $.cookie(Browser.Cookie.Key, Browser.Cookie.Value, { expires: Browser.Cookie.ExpiresInMinutes, path: Browser.Cookie.PathGlobal });
    };

    return {
        RedirectIfBrowserIsTooOld: function(redirectToUrl) {
            if (location.pathname == redirectToUrl) {
                return;
            }
            if (isMobileBrowser()) {
                return;
            }
            if (isBrowserTooOld() && !isBrowserTooOldCookieSet()) {
                setBrowserTooOldCookie();
                location.href = redirectToUrl;
            }
        },

        SetupPageForCookieRequirements: function() {
            document.write('<style type="text/css">');
            if (navigator.cookieEnabled) {
                document.write('.cookie-enabled { display: block; }');
                document.write('.cookie-disabled { display: none; }');
            } else {
                document.write('.cookie-enabled { display: none; }');
                document.write('.cookie-disabled { display: block; }');
            }
            document.write('</style>');
        }
    };
})();

Browser.Type = new Object();
Browser.Type.IE = "IE";
Browser.Type.MSIE = "MSIE";
Browser.Type.Firefox = "Firefox";
Browser.Type.Chrome = "Chrome";
Browser.Type.Safari = "Safari";

Browser.Version = new Object();
Browser.Version.MinIE = 10;
Browser.Version.MinFirefox = 27;
Browser.Version.MinChrome = 32;
Browser.Version.MinSafari = 5;

Browser.Cookie = new Object();
Browser.Cookie.Key = "BrowserTooOld";
Browser.Cookie.Value = true;
// 21 days
Browser.Cookie.ExpiresInMinutes = 21 * 24 * 60;
Browser.Cookie.PathGlobal = "/";;
var Common = (function () {
    var delay = (function () {
        var timer = 0;
        return function (callback, ms) {
            clearTimeout(timer);
            timer = setTimeout(callback, ms, true);
        };
    })();
    
    function startSpinner(options) {
        if (options && options.spinner && options.spinner.noSpinner) {
            return undefined;
        }

        options = options || {};

        var spinnerOptions = options.spinner || {};

        // Default values
        var radius = spinnerOptions.radius || 6;
        var width = spinnerOptions.width || 3;
        var container = $(spinnerOptions.containerSelector || options.containerId);

        if (container.length > 0) {
            var spinnerTarget = container.get(0);

            if (spinnerOptions.containerSelector) {
                if (options.hideElements) {
                    container.css('visibility', 'visible');
                } else if (options.fadeElements) {
                    container.removeClass('load-fading-effect');
                }
            }

            var spinner = new Spinner({ corners: 0.5, hwaccel: true, length: 5, lines: 11, radius: radius, rotate: 0, speed: 1, trail: 40, width: width, zIndex: 2000 });

            if (!Common.IsVisible(container)) {
                container.addClass('visible');
                spinner.hideContainerAfterStop = true;
                spinner.container = container;
            }

            spinner.spin(spinnerTarget);

            return spinner;
        }

        return undefined;
    }

    function stopSpinner(spinner, options) {
        if (spinner && (!options || !options.keepSpinner)) {
            if (spinner.hideContainerAfterStop) {
                spinner.container.removeClass('visible');
            }
            spinner.stop();
        }
    }
    
    function pollStatus(pollUri, data, onSuccess, onError) {
        var stopPolling = false;

        Common.AjaxWithErrorHandling(pollUri,
            data,
            function (result) {
                if (result.IsValid) {
                    if (!result.IsPending) {
                        stopPolling = true;
                    }

                    onSuccess(result, stopPolling);
                } else {
                    onError(result);
                }
            });
    }

    function toggleAtLeastOneSelectedError(options) {
        var checkedCheckboxes = $(options.checkboxSelector + " input[type=checkbox]:checked");
        $('[data-valmsg-for="' + options.serverErrorSelector + '"]').hide();

        if (checkedCheckboxes.length === 0) {
            $(options.errorSelector).show();
            return false;
        } else {
            $(options.errorSelector).hide();
            return true;
        }
    }

    return {
        PreventDoubleClick: function (linkSelector) {
            $(linkSelector).one("click", function () {
                $(this).on("click", function () { return false; });
            });
        },

        StartSpinner: function (options) {
            return startSpinner(options);
        },

        StopSpinner: function (spinner, options) {
            stopSpinner(spinner, options.spinner);
        },

        // An AJAX call which submits a form (e.g. of a partial view) and returns a partial view or JSON
        AjaxGetPartialView: function (options) {
            if (!options) {
                options = {};
            }

            // Hide all input fields and elements
            if (options.hideElements) {
                $(options.formId).children().css('visibility', 'hidden');
            } else if (options.fadeElements) {
                $(options.formId).children().addClass('load-fading-effect');
            }

            // Show the spinner
            var spinner = startSpinner(options);

            var isTraditional = false;
            if (options.traditional) {
                // Use this for correct handling of array data
                isTraditional = true;
            }

            $.ajax({
                type: options.type,
                url: options.url,
                data: options.formData,
                headers: {
                    "Request-Type": Common.REQUEST_TYPE_JSON
                },
                cache: false,
                success: function (result) {
                    // Reset timeout and stop spinner afterAJAX call
                    Timeout.ResetTimeout();
                    stopSpinner(spinner, options.spinner);

                    if (result.RedirectTo) {
                        location.href = result.RedirectTo;
                    } else {
                        try {
                            var jsonResult = JSON.parse(result);

                            if (jsonResult.RedirectTo) {
                                location.href = jsonResult.RedirectTo;
                            }
                        } catch (e) {
                            if (!options.preventUpdate) {
                                // Replace the current box content with the new partial view
                                $(options.containerId).html(result);
                            }
                        }
                    }

                    if (options.callback) {
                        options.callback(result);
                    }
                },
                error: function () {
                    Timeout.ResetTimeout();
                    stopSpinner(spinner, options.spinner);
                    $(options.containerId).html($("#" + Common.ID_GLOBAL_ERROR_TEXT).text());
                },
                traditional: isTraditional
            });
        },

        // A simple AJAX call which sends JSON and returns JSON
        AjaxWithErrorHandling: function (url, data, callback, containerId, options) {
            options = options || {};

            // Hide all input fields and elements
            if (options.hideElements) {
                $(containerId).children().css('visibility', 'hidden');
            } else if (options.fadeElements) {
                $(containerId).children().addClass('load-fading-effect');
            }

            var spinner = startSpinner($.extend({ containerId: containerId }, options));

            var defaultOptions = {
                type: Common.FORM_METHOD_POST,
                url: url,
                data: data,
                async: !options.disableAsync,
                success: function (successData) {
                    // Reset timeout and stop spinner after AJAX call
                    Timeout.ResetTimeout();
                    stopSpinner(spinner);

                    // Hide all input fields and elements
                    if (options.hideElements) {
                        $(containerId).children().css('visibility', 'visible');
                    } else if (options.fadeElements) {
                        $(containerId).children().removeClass('load-fading-effect');
                    }

                    if (callback !== null) {
                        callback(successData);
                    }

                    if (successData.HasError) {
                        Common.OpenDialog(successData.DateTime, successData.Message);
                    }
                },
                dataType: "json",
                cache: false,
                error: function (request, textStatus, errorThrown) {
                    Timeout.ResetTimeout();
                    stopSpinner(spinner);
                    Common.OpenDialog(textStatus, errorThrown);
                }
            };

            // extend allows options to overwrite properties of defaultOptions
            // so the caller may "overwrite" single default properties as required.
            if (options) {
                $.extend(defaultOptions, options);
            }

            $.ajax(defaultOptions);
        },

        AppendCloseButton: function () {
            $('.alert').each(function () {
                $(this).prepend('<button type="button" class="close" data-dismiss="alert"></button>');
            });
        },

        CenterDialog: function (limitHeight, maxHeightFactor) {
            var dialog = $('.modal .modal-dialog');

            if (dialog.length <= 0) {
                return;
            }

            if (!maxHeightFactor) {
                maxHeightFactor = 0.9;
            }


            var dialogHeight = dialog.height();
            var viewportHeight = $(window).height();

            var maxHeight = viewportHeight * maxHeightFactor;

            if (limitHeight === true && dialogHeight > maxHeight) {
                var headerHeight = $('.modal .modal-dialog .modal-header').outerHeight();
                var footerHeight = $('.modal .modal-dialog .modal-footer').outerHeight();

                $('.modal .modal-dialog .modal-body').height(maxHeight - headerHeight - footerHeight);
            }

            if (limitHeight !== true && dialogHeight > maxHeight) {
                dialog.css('margin-top', 100);
            } else {
                dialogHeight = dialog.height();
                dialog.css('margin-top', (viewportHeight - dialogHeight) / 2);
            }
        },

        CopyHtmlAttributes: function (src, dest) {
            var attributes = src.prop("attributes");

            $.each(attributes, function () {
                if (this.name.toLowerCase() === "class") {
                    dest.addClass(this.value);
                } else {
                    dest.attr(this.name, this.value);
                }
            });
        },

        Delay: delay,

        FocusFirstInvalidInput: function () {
            var input = $('.input-validation-error:first').get(0);

            if (!Common.IsPhoneMode() && input) {
                input.focus();
                Common.SetCaretToEnd(input);
                return true;
            }

            return false;
        },

        HandleGeneralAlertEvent: function () {
            var closeButton = $('#alertDialogCloseButton');

            if (closeButton.length > 0) {
                closeButton.focus();
            }
        },

        HtmlEncode: function (value) {
            //create a in-memory div, set it's inner text(which jQuery automatically encodes)
            //then grab the encoded contents back out.  The div never exists on the page.
            return $('<div/>').html(value).text();
        },

        MarkAsNumeric: function (elementSelector) {
            var options = { negative: false, decimal: false };
            $(elementSelector).numeric(options);
        },

        MarkAsDecimal: function (elementSelector) {
            var options = { negative: false, decimalPlaces: 2, decimal: "," };
            $(elementSelector).numeric(options);
        },

        HorizontalAdditionalSpace: function (element) {
            return element.outerWidth(true) - element.width();
        },

        InitializeTooltips: function (containerSelector) {
            // only apply tooltips if we are in non-mobile mode 
            containerSelector = containerSelector || '';
            $(containerSelector + ' .tooltip').remove();
            $(containerSelector + ' [data-toggle="tooltip"]').tooltip('destroy');

            if (!Common.IsPhoneMode()) {
                $('body').off('show.bs.tooltip');
                $('body').tooltip({
                    selector: containerSelector + ' [data-toggle="tooltip"]'
                });
            } else {
                $('body').on('show.bs.tooltip', function () {
                    return false;
                });
            }
        },

        InitializeConfirmationSetting: function (options, supportMode, showConfirmationDialog, formToSubmit, linkToFollow) {
            if (!showConfirmationDialog) {
                return;
            }

            var confirmContent = options.contentId;
            var confirmTitle = options.titleId;
            var confirmLinkSelector = '[data-id="' + options.confirmDataId + '"]';
            var cancelLinkSelector = '[data-id="' + options.cancelDataId + '"]';
            var popoverSelector = options.popoverId;

            $(popoverSelector).on("click", function (e) {
                e.preventDefault();
            });

            $(popoverSelector).popover({
                html: true,
                content: function () {
                    return $(confirmContent).html();
                },
                title: function () {
                    return $(confirmTitle).html();
                },
                trigger: "click",
                container: "body",
                toggle: "popover",
                placement: "top"
            });

            $(popoverSelector).on('shown.bs.popover', function () {
                if (formToSubmit) {
                    var submitFn = function () {
                        formToSubmit.submit();
                    };
                }

                if (linkToFollow) {
                    $(confirmLinkSelector).on('click', function () {
                        window.location = linkToFollow;
                    });
                }

                Global.SetCurrentPopover(popoverSelector);
                Common.InitializeConfirmationBoxHandlers(confirmLinkSelector, cancelLinkSelector, popoverSelector, supportMode, submitFn);
            });
        },

        InitializeConfirmationBoxHandlers: function (confirmLinkSelector, cancelLinkSelector, popoverSelector, supportMode, submitFn, cancelFn) {
            // to prevent once shown popovers to hide other elements that trigger events (like links)
            // use this workaround to explicitly remove all popovers; then links will be clickable again
            $('body').on('hidden.bs.popover', function () {
                var popovers = $('.popover').not('.in');
                if (popovers) {
                    popovers.remove();
                }
            });

            if (supportMode) {
                $(confirmLinkSelector).attr('disabled', 'disabled');
            }

            $(confirmLinkSelector).on("click", function (e) {
                e.preventDefault();
                $(popoverSelector).popover('hide');

                if (submitFn) {
                    submitFn();
                }
            });

            $(cancelLinkSelector).on("click", function (e) {
                e.preventDefault();
                $(popoverSelector).popover('hide');

                if (cancelFn) {
                    cancelFn();
                }
            });
        },

        IsEllipsisActive: function (element) {
            return (element.offsetWidth < element.scrollWidth);
        },

        IsPhoneMode: function () {
            return $('#mediaTester').css('float') != 'left';
        },

        IsVisible: function (element) {
            return $(element).css('display') != 'none'
                && $(element).css('visibility') != 'hidden';
        },

        NewLine2Paragraph: function (str) {
            return (str + '').replace(/(\r\n|\n\r|\r|\n)/g, '<br/>').replace(/[ ]+/g, ' ');
        },

        TextLink2HtmlLink: function (str) {
            // Note: there must be whitespace(s) before links for a match with this regex to ensure it is not double parsed (1st run is bb code)
            var linkPattern = /(\s+)((https?:\/\/)([\da-z\.-]+)\.([a-z\.]{2,6})([\/\w% \.-]*)*\/?)/g;
            var linkOrigin = window.location.hostname;
            return (str + '').replace(linkPattern, function (matchPattern, url, whitespace) {
                var stripProtocol = matchPattern.replace(/^https?:\/\//, '');
                var linkHostname = stripProtocol.substr(0, linkOrigin.length);
                if(linkHostname === linkOrigin) {
                    return whitespace + '<a href="' + url + '" target="_self">' + url + '</a>';
                } else {
                    return whitespace + '<a href="' + url + '" target="_blank">' + url + '</a>';
                }
            });
        },

        TextLink2MailLink: function (str) {
            var linkPattern = /((mailto:)([a-z0-9._-]+@[a-z0-9.-]+\.[a-z]{2,4})((\?subject=.+?(?=(&body=|\s))(&body=.+?(?=\s))?)\s)?)|([a-z0-9._-]+@[a-z0-9.-]+\.[a-z]{2,4})/ig;
            return (str + '').replace(linkPattern, "<a href='mailto:$3$4$8'>$3$8</a> ");
        },

        BbCodeToHtml: function (htmlSelector) {
            var htmlText = $(htmlSelector).text();
            var htmlValue = $(htmlSelector).val();
            var convertedText = Common.ParseBBCode(htmlText || htmlValue);

            return convertedText.html;
        },

        ConvertAllBbCodesToHtml: function (selector) {
            var htmlContents = $(selector);

            $(htmlContents).each(function (index, value) {
                var convertedText = Common.BbCodeToHtml(value);
                $(value).html(convertedText);
            });
        },

        ParseBBCode: function (str) {
            return XBBCODE.process({
                    text: str,
                    removeMisalignedTags: true,
                    addInLineBreaks: true
                });
        },

        ParseTextLinks: function (str) {
            var result = this.ParseBBCode(str);
            var htmlLinks = this.TextLink2HtmlLink(result.html);
            var mailAndHtmlLinks = this.TextLink2MailLink(htmlLinks);
            return mailAndHtmlLinks;
        },

        OpenDialog: function (inTitle, inText) {
            try {
                $('#dialog .modal-title').text(inTitle);
                $('#dialog .modal-body').text(inText);
                $('#dialog').modal('show');
            } catch (e) {
                // Do nothing in case of an error
            }
        },

        OpenDialogRemote: function (dialog, remoteUrl, prepareFn, options) {
            if (!dialog) {
                return;
            }

            dialog.off();

            var spinner = undefined;

            if (options && options.spinner) {
                dialog.on('show.bs.modal', function () {
                    spinner = startSpinner(options);
                });
            }

            dialog.on('shown.bs.modal', function () {
                if (prepareFn) {
                    prepareFn();
                }
                stopSpinner(spinner);
            }).on('hidden.bs.modal', function () {
                $(this).removeData('bs.modal');
                dialog.empty();
            }).modal({ remote: encodeURI(remoteUrl) });
        },

        OpenWindow: function (uri) {
            window.open(uri, '_blank');
        },

        PostprocessTableRow: function (table, rowHtmlElement, templateTrString) {
            var i, j = 0;

            table.fnSettings().aoColumns.forEach(function (entry) {
                i = entry.mData;
                if (entry.bVisible) {
                    j += 1;

                    var currentTd = $('td:nth-child(' + j + ')', rowHtmlElement);
                    var newFromString = $($(templateTrString).children()[i]);

                    currentTd.html(newFromString.html());
                    Common.CopyHtmlAttributes(newFromString, currentTd);
                }
            });

            Common.CopyHtmlAttributes($(templateTrString), $(rowHtmlElement));
        },

        SetupTogglePassword: function (togglePasswordSelector) {
            $(togglePasswordSelector).each(function () {
                var passwordField = $(this);

                $(passwordField).on('click', function () {
                    var toggleIcon = $(this);

                    $(toggleIcon).siblings(".toggleable-password-field").each(function () {
                        var passwordField = $(this);
                        var toggleIcon = $(passwordField).parent().find(".glyphicon");

                        if ($(passwordField).attr('type') === 'password') {
                            $(passwordField).attr('type', 'text');
                            $(toggleIcon).addClass('glyphicon-eye-close');
                            $(toggleIcon).removeClass('glyphicon-eye-open');
                        } else if ($(this).attr('type') === 'text') {
                            $(passwordField).attr('type', 'password');
                            $(toggleIcon).addClass('glyphicon-eye-open');
                            $(toggleIcon).removeClass('glyphicon-eye-close');
                        }
                    });
                });
            });
        },

        PrepareForm: function (formId, confirmButtonSpecifier, submitFn, options) {
            var form = $(formId);

            if (form.length <= 0) {
                return;
            }

            var fields = $(formId + " input:text, " + formId + " input:password, " + formId + " select, " + formId + " textarea");
            var radioButtons = $(formId + " input[type='radio']");

            var firstInputField = (fields.length > 0) ? fields.first() : radioButtons.first();

            var mouseMoved = false;
            var confirmButton = $(confirmButtonSpecifier);
            var allInputFields = $(formId + " input");
            var allSelectFields = $(formId + ' select');
            options = options || {};

            //$.validator.unobtrusive.parse(form);
            var validator = form.validate();

            // hide validation errors after page load
            // to give the user a chance to type in values correctly, before displaying an error
            $(formId)[0].reset();
            
            var onkeyup = function(element, event) {
                var delayedCallback = function() {
                    if (Common.UNPRINTABLE_KEYCODES.indexOf(event.which) >= 0 || Common.KEYCODE_ENTER === event.which) {
                        return;
                    }
                    this.element(element);
                }.bind(this);
                Common.Delay(delayedCallback, Common.DEBOUNCE_TIME_IN_MS);
            }.bind(validator);

            validator.settings.onkeyup = onkeyup;

            // prevent duplicate form submit caused by double clicks
            var clicked = false;

            function submitForm(e) {
                if (clicked) {
                    console.log("double click ignored");
                    e.preventDefault();
                    return;
                }

                form.validate();
                if (confirmButton.attr('disabled') || !form.valid()) {
                    e.preventDefault();
                    return;
                }

                if (submitFn) {
                    submitFn();
                } else {
                    form.submit();
                }

                e.preventDefault();

                // ensure button will be clickable again after a specific time
                clicked = true;
                Common.Delay(function () {
                    clicked = false;
                }, Common.DOUBLECLICK_TIME_IN_MS);
            }

            function validate() {
                $(this).valid();
            }

            function mouseDownHandler(e) {
                if (e.which === Common.LEFT_MOUSE_BUTTON) {
                    $(window).mousemove(function () {
                        mouseMoved = true;
                        $(window).off("mousemove");
                    });
                    fields.off('blur');
                }
            }

            function mouseUpHandler() {
                $(window).off("mousemove");
                if (mouseMoved) {
                    fields.on("blur", validate);
                }
            }

            fields.on("blur", validate);

            $('a.cancel-link').on("mousedown", mouseDownHandler);
            $('.no-validation').on("mousedown", mouseDownHandler);
            confirmButton.on("mousedown", mouseDownHandler);

            $(window).on("mouseup", mouseUpHandler);
            if (!options.noFocus) {
                if (!Common.IsPhoneMode() && !Common.FocusFirstInvalidInput() && firstInputField.length > 0) {
                    if (firstInputField) {
                        firstInputField.focus();
                    }
                }
            }

            function submitFormOnEnter(e) {
            	if (e.keyCode === Common.KEYCODE_ENTER && e.target.tagName !== "textarea") {
                    submitForm(e);
                }
            }

            // If a single confirm button is provided, register the click event and the keyboard events to submit the form
            if (confirmButton.length > 0) {
                confirmButton.on("click", submitForm);
                allInputFields.on("keydown", submitFormOnEnter);
                allSelectFields.on("keydown", submitFormOnEnter);
                // If more than a single confirm button is provided, only register the click event to submit the form for all buttons
            } else if (Array.isArray(confirmButtonSpecifier)) {
                confirmButtonSpecifier.forEach(function (value) {
                    confirmButton = $(value);

                    if (confirmButton.length > 0) {
                        confirmButton.on("click", submitForm);
                    }
                });
            }
        },

        SetCaretToEnd: function (elem) {
            try {
                var elemLen = 0;
                if (elem.value) {
                    elemLen = elem.value.length;
                }
                // For IE Only
                if (document.selection) {
                    // Set focus
                    elem.focus();
                    // Use IE Ranges
                    var oSel = document.selection.createRange();
                    // Reset position to 0 & then set at end
                    oSel.moveStart('character', -elemLen);
                    oSel.moveStart('character', elemLen);
                    oSel.moveEnd('character', 0);
                    oSel.select();
                } else if (elem.selectionStart || elem.selectionStart == '0') {
                    // Firefox/Chrome
                    elem.selectionStart = elemLen;
                    elem.selectionEnd = elemLen;
                    elem.focus();
                }
            } catch (ex) {
                // do nothing
            }
        },

        SubmitFormPerAjax: function (formSelector, callback, errorCallback) {
            var form = $(formSelector);

            if (form.length <= 0) {
                return;
            }

            $.ajax({
                url: form.attr('action'),
                type: form.attr('method'),
                data: form.serialize(),
                success: function (result) {
                    // Reset timeout after AJAX call
                    Timeout.ResetTimeout();

                    if (callback) {
                        callback(result);
                    }
                },
                error: function (request, textStatus, thrownError) {
                    Timeout.ResetTimeout();
                    if (errorCallback) {
                        errorCallback(request, textStatus, thrownError);
                    } else {
                        Common.OpenDialog(textStatus, thrownError);
                    }
                }
            });
        },

        TableResizer: function (table, parentSelector, callbackFn) {
            var fullWidth = 0;
            var colGroupElement = table.children('colgroup');
            parentSelector = parentSelector || '';

            if (colGroupElement.length <= 0) {
                return;
            }

            var colNodes = $(colGroupElement).children('col[data-preferred-width]');
            var visibleColNodes = 0;

            colNodes.each(function () {
                if (Common.IsVisible(this)) {
                    fullWidth += parseFloat($(this).attr('data-preferred-width'));
                    visibleColNodes += 1;
                }
            });

            if (fullWidth === 0) {
                fullWidth = 1;
            }

            var tableWidth = $(parentSelector + ' .dataTables_wrapper').width();
            var fixedColNodes = $(colGroupElement).children('col:not([data-preferred-width])');
            fixedColNodes.each(function () {
                if (Common.IsVisible(this)) {
                    tableWidth -= parseFloat($(this).css('max-width'));
                }
            });

            colNodes.each(function () {
                if (Common.IsVisible(this)) {
                    var preferredWidthPct = parseFloat($(this).attr('data-preferred-width'));

                    if (visibleColNodes === 1 && preferredWidthPct === 0) {
                        preferredWidthPct = 1;
                    }

                    $(this).width((tableWidth * preferredWidthPct / fullWidth) + 'px');
                }
            });
        },

        TruncateText: function (element, numberOfLines) {
            var truncate = new Truncate(element, {
                lines: numberOfLines,
                lineHeight: $(element).css('line-height')
            });

            var title = $(element).attr('data-title');

            if (title) {
                truncate.update(title);
                truncate.collapse();
            }
        },

        UpdateTableRowWithTemplate: function (trNode, template, responsiveHelper) {
            responsiveHelper.disable(true);
            trNode.html(template.html());
            responsiveHelper.disable(false);
            responsiveHelper.respond();
        },

        DisableTextTruncation: function (element) {
            // ReSharper disable once UnusedLocals
            var notUsedVariable = new Truncate(element, {
                lines: 10000, // set line number to an extremely high value
                lineHeight: 10 // use some standard line height
            });
        },

        InitializeFileUpload: function (fileUploaderName, removeUploadUrl, removeUploadText, fallbackErrorMessage, fileContainer, uploadResultContainerId, uploadErrorContainerId, uploadErrorTextId, uploadIdPrefix) {

            function installUploadEventHandlers() {
                var uploadIds = $("a[id^='" + uploadIdPrefix + "']");
                for (var i = 0; i < uploadIds.length; i++) {
                    var name = $(uploadIds[i]).data("name");
                    $(uploadIds[i]).on("click", null, name, function (e) {
                        fileUploadRemove(e.data);
                        e.preventDefault();
                    });
                }
            }

            function fileUploadRemove(name) {
                var data = new Object();
                data.name = name;
                Common.AjaxWithErrorHandling(removeUploadUrl, data, function (result) {
                    if (result.IsValid) {
                        $(uploadErrorContainerId).hide();
                        reloadFileUploadList(result.Names);
                    }
                });
            }

            function reloadFileUploadList(names) {
                $(uploadResultContainerId).html(null);
                var i = 0;
                for (var key in names) {
                    var id = uploadIdPrefix + i;
                    $(uploadResultContainerId).append(
                        "<div><a href='#' id='" + id + "' data-name='" + names[key] + "'>" + removeUploadText + "</a> " + names[key] + "</div>");
                    i++;
                }
                installUploadEventHandlers();
            }

            var fileUpload = new FileUpload();
            FileUploadStorage[fileUploaderName] = fileUpload;
            fileUpload.Initialize(fileContainer,
                function (uploadResult) {
                    $(uploadErrorContainerId).hide();
                    reloadFileUploadList(uploadResult.Names);
                },
                function (errorMessage) {
                    $(uploadErrorContainerId).hide();
                    $(uploadErrorContainerId).fadeIn(400);
                    if (!errorMessage) {
                        $(uploadErrorTextId).html(fallbackErrorMessage);
                    } else {
                        $(uploadErrorTextId).text(errorMessage);
                    }
                }, fileUploaderName
            );

            installUploadEventHandlers();
        },

        PopoverInfoCriteria: function (popoverDataId) {
            // only call this in $(document).ready(function() {... or at end of document
            // because we need to wait for the partial password policy view to load
            $('[data-id="' + popoverDataId + '"]').popover({
                html: true,
                content: function() { // content does not get used if already set
                    return $('#passwordPolicyContent').html();
                },
                trigger: 'manual'
            }).on("click", function (e) {
                $(this).popover('show');
                $('.hide-popover').not(this).popover('hide');
                // Force a repaint for Safari to remove popover artifacts
                $('.panel.panel-default.structured-content').css('transform', 'none');
                $('.redraw-popover').css('transform', 'none');
                e.stopPropagation();
            });

            // clicking anywhere dismisses popover 
            // because we don't want to open it when clicking on another popover
            $('html').on("click", function (e) {
                $('[data-id="'+popoverDataId+'"]').popover('hide');
                // Force a repaint for Safari to remove popover artifacts
                $('.panel.panel-default.structured-content').css('transform', 'translateZ(0)');
                $('.redraw-popover').css('transform', 'translateZ(0)');
            });
            
        },

        PreventMultipleClicksOnElement: function(element) {
            $(element).on('click', function (e) {
                var currentElement = $(this);
                if (currentElement.hasClass('clicked')) {
                    return false;
                } else {
                    currentElement.addClass('clicked');
                    Common.Delay(function () {
                        currentElement.removeClass('clicked');
                    }, Common.MULTIPLE_CLICKS_TIME_IN_MS);
                }
            });
        },
        
        InstallAtLeastOneCheckboxSelectedHandler: function (options) {
            $(options.errorSelector).hide();

            $(options.submitButtonSelector).on("click", function () {
                toggleAtLeastOneSelectedError(options);
            });

            $(options.checkboxSelector + " input[type=checkbox]").on("change", function () {
                toggleAtLeastOneSelectedError(options);
            });
            $(options.submitButtonSelector).on("click", function () {
                toggleAtLeastOneSelectedError(options);
            });
        },

        DelayedPolling: function (pollUri, data, remainingRetries, onSuccess, onError, onUpdateStatus) {
            var continuePolling = true;

            if (remainingRetries && remainingRetries <= 0) {
                onError({ data: { IsExpired: true } });
                return;
            }

            Common.Delay(function () {
                pollStatus(pollUri,
                    data,
                    function (result, stopPolling) {
                        continuePolling = !stopPolling;

                        if (continuePolling) {
                            if (onUpdateStatus) {
                                onUpdateStatus(result);
                            }
                            Common.DelayedPolling(pollUri, data, remainingRetries ? remainingRetries - 1 : null, onSuccess, onError, onUpdateStatus);
                        } else {
                            onSuccess(result);
                        }
                    },
                    function (result) {
                        onError(result);
                    });
            },
            Common.POLLING_WAIT_TIME_IN_MSEC);
        },

        SetupDisplayMaxLengthForTextarea(maxLength, textareaSelector, infoSelector) {
            $(textareaSelector).on("change keyup paste", function () {
                var currentLength = $(this).val().length;
                $(infoSelector).text(currentLength + "/" + maxLength);

                if (currentLength > maxLength) {
                    $(infoSelector).addClass("validation-summary-errors");
                } else {
                    $(infoSelector).removeClass("validation-summary-errors");
                }
            });
        }
    };
})();

Common.POLLING_WAIT_TIME_IN_MSEC = 1000;

