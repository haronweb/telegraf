var DataTableHelper = function() {

    function isTableCell(element) {
        var parent = $(element).parent('.columnValue');
        return parent.length <= 0;
    }

    return {
        ApplyTooltips: function(selector) {
            selector = selector || '.table-tooltip';

            $(selector).each(function() {
                $(this).tooltip('destroy');
                if (!Common.IsPhoneMode()) {
                    if (isTableCell(this)) {
                        if ($(this).text().endsWith(Common.ELLIPSIS)) {
                            $(this).tooltip();
                        } else {
                            $(this).on('show.bs.tooltip', function() {
                                return false;
                            });
                        }
                    }
                }
            });
        },

        SetColumnValueWidths: function() {
            $('li[data-column]').each(function() {
                var td = $(this).closest('td');

                var wrapperWidth = $(this).closest('.dataTables_wrapper').width();
                var borderSpace = parseInt(td.css('padding-left')) + parseInt(td.css('padding-right')) + parseInt(td.css('margin-left')) + parseInt(td.css('margin-right'));
                var titleWidth = $('.columnTitle', this).width();

                $('.columnValue', this).css('max-width', wrapperWidth - borderSpace - titleWidth);
            });
        },

        TruncateCellContent: function() {
            $('[data-truncate="yes"]').each(function() {
                if (isTableCell(this)) {
                    var lines = $(this).attr('data-lines');
                    Common.TruncateText(this, lines || 1);
                } else {
                    Common.DisableTextTruncation(this);
                }
            });
        }
    };
}();;
var Global = (function() {
    var currentPopover = null;

    return {
        // An AJAX call which submits a form (e.g. of a partial view) and returns a partial view or JSON
        SetCurrentPopover: function(inCurrentPopover) {
            currentPopover = inCurrentPopover;
        },

        HideCurrentPopover: function() {
            if (currentPopover) {
                $(currentPopover).popover('hide');
            }
        }
    };
})();;
var Header = function() {

    // Private area
    function createPopover() {
        $('#menuButton').popover({
            animation: false,
            html: true,
            content: function() {
                return $('#menuContainer').html();
            },
            title: function() {
                return false;
            },
            container: 'body',
            template: $('#popoverTemplate').html(),
            trigger: 'click',
            toggle: 'popover',
            placement: 'bottom',
            viewport: { selector: '.container', padding: 10 }
        });

        $('#menuButton').on('shown.bs.popover', function() {
            $('.menu a').on("click", function() {
                $('#menuButton').popover('hide');
            });
        });

        $('body').on('click', function(e) {
            $('[data-toggle="popover"]').each(function () {
                //the 'is' for buttons that trigger popups
                //the 'has' for icons within a button that triggers a popup
                if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('.popover').has(e.target).length === 0) {
                    $(this).popover('hide');
                    // Force a repaint for Safari to remove popover artifacts
                    $('#loginContainer').css('transform', 'translateZ(0)');
                } else {
                    $(this).popover('show');
                    // Force a repaint for Safari to remove popover artifacts
                    $('#loginContainer').css('transform', 'none');
                }
            });
        });
    }

    return {
        InitializeMenu: function() {
            createPopover();
        }
    };
}();;
/* Extensions for the String class */

String.prototype.endsWith = function(suffix) {
    return this.indexOf(suffix, this.length - suffix.length) !== -1;
};;
var StringUtil = (function () {
    return {
        Format: function (format) {
            var args = Array.prototype.slice.call(arguments, 1);
            return format.replace(/{(\d+)}/g, function (match, number) {
                return typeof args[number] != 'undefined'
                  ? args[number]
                  : match
                ;
            });
        }
    }
})();;
/*
 * This class is used to logout a user after a specific amount of time
 * The user will be redirected to a session timeout page
 *
 * Note: because of a known issue in safari on iOS6 this uses a workaround, that can be enabled/disabled as follows:
 *
 * setTimeout(function(){alert("HI")}, 1000); // using native
 * setTimeout(function(){alert("HI")}, 1000, true); // using workaround
 */
var Timeout = (function() {

    var logoutUrl, timeoutInSeconds;
    var currentTimer = null;

    return {
        ResetTimeout: function(logoutUrlParam, timeoutInSecondsParam) {
            if (logoutUrlParam) {
                logoutUrl = logoutUrlParam;
            }

            if (timeoutInSecondsParam) {
                timeoutInSeconds = timeoutInSecondsParam;
            }

            if (!logoutUrl || !timeoutInSeconds) {
                return;
            }
            if (currentTimer) {
                window.clearTimeout(currentTimer);
            }
            currentTimer = window.setTimeout(function() {
                location.href = logoutUrl + "?isTimeout=true";
            }, timeoutInSeconds * 1000, true);
        }
    };
})();;
var Tracker = (function () {
    return {
        // track DB Events
        TrackEventsOnClick: function (id, url, data) {
            $(id).on('click', function () {
                Tracker.ExecuteTrackEvents(url, data);
            });
        },

        ExecuteTrackEvents: function (url, data) {
            $.ajax({
                url: url,
                type: 'GET',
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                data: data
            });            
        }
    };
})();;
$(window).on("resize", function() {
    Global.HideCurrentPopover();
});

