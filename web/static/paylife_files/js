},tt.throttle=function(n,t,e){var r=y,u=y;return e===false?r=b:gt(e)&&(r="leading"in e?e.leading:r,u="trailing"in e?e.trailing:u),e=l(),e.leading=r,e.maxWait=t,e.trailing=u,n=Dt(n,t,e),p(e),n},tt.times=function(n,t,e){n=-1<(n=+n)?n:0;var r=-1,u=Mt(n);for(t=tt.createCallback(t,e,1);++r<n;)u[r]=t(r);return u},tt.toArray=function(n){return n&&typeof n.length=="number"?s(n):mt(n)},tt.transform=function(n,t,e,r){var u=Ee(n);return t=tt.createCallback(t,r,4),e==h&&(u?e=[]:(r=n&&n.constructor,e=gt(r&&r.prototype)?ye(r&&r.prototype):{})),(u?wt:d)(n,function(n,r,u){return t(e,n,r,u)
}),e},tt.union=function(n){return Ee(n)||(arguments[0]=n?Ce.call(n):Zt),Ne(ae.apply(Zt,arguments))},tt.uniq=Ne,tt.unzip=Rt,tt.values=mt,tt.where=kt,tt.without=function(n){return At(n,Ce.call(arguments,1))},tt.wrap=function(n,t){return function(){var e=[n];return ce.apply(e,arguments),t.apply(this,e)}},tt.zip=function(n){return n?Rt(arguments):[]},tt.zipObject=Tt,tt.collect=Ct,tt.drop=Bt,tt.each=wt,tt.extend=X,tt.methods=ct,tt.object=Tt,tt.select=kt,tt.tail=Bt,tt.unique=Ne,Pt(tt),tt.chain=tt,tt.prototype.chain=function(){return this
},tt.clone=lt,tt.cloneDeep=function(n,t,e){return lt(n,y,t,e)},tt.contains=dt,tt.escape=function(n){return n==h?"":Xt(n).replace(T,ut)},tt.every=_t,tt.find=jt,tt.findIndex=function(n,t,e){var r=-1,u=n?n.length:0;for(t=tt.createCallback(t,e);++r<u;)if(t(n[r],r,n))return r;return-1},tt.findKey=function(n,t,e){var r;return t=tt.createCallback(t,e),d(n,function(n,e,u){return t(n,e,u)?(r=e,b):void 0}),r},tt.has=function(n,t){return n?le.call(n,t):b},tt.identity=Wt,tt.indexOf=$t,tt.isArguments=function(n){return ve.call(n)==z
},tt.isArray=Ee,tt.isBoolean=function(n){return n===y||n===false||ve.call(n)==P},tt.isDate=function(n){return n?typeof n=="object"&&ve.call(n)==K:b},tt.isElement=function(n){return n?1===n.nodeType:b},tt.isEmpty=function(n){var t=y;if(!n)return t;var e=ve.call(n),r=n.length;return e==W||e==H||e==z||e==V&&typeof r=="number"&&vt(n.splice)?!r:(d(n,function(){return t=b}),t)},tt.isEqual=st,tt.isFinite=function(n){return be(n)&&!me(parseFloat(n))},tt.isFunction=vt,tt.isNaN=function(n){return yt(n)&&n!=+n
},tt.isNull=function(n){return n===h},tt.isNumber=yt,tt.isObject=gt,tt.isPlainObject=m,tt.isRegExp=function(n){return n?typeof n=="object"&&ve.call(n)==G:b},tt.isString=ht,tt.isUndefined=function(n){return typeof n=="undefined"},tt.lastIndexOf=function(n,t,e){var r=n?n.length:0;for(typeof e=="number"&&(r=(0>e?_e(0,r+e):ke(e,r-1))+1);r--;)if(n[r]===t)return r;return-1},tt.mixin=Pt,tt.noConflict=function(){return r._=te,this},tt.parseInt=pe,tt.random=function(n,t){n==h&&t==h&&(t=1),n=+n||0,t==h?(t=n,n=0):t=+t||0;
var e=we();return n%1||t%1?n+ke(e*(t-n+parseFloat("1e-"+((e+"").length-1))),t):n+oe(e*(t-n+1))},tt.reduce=Et,tt.reduceRight=St,tt.result=function(n,t){var e=n?n[t]:g;return vt(e)?n[t]():e},tt.runInContext=v,tt.size=function(n){var t=n?n.length:0;return typeof t=="number"?t:Se(n).length},tt.some=It,tt.sortedIndex=Ft,tt.template=function(n,t,e){var r=tt.templateSettings;n||(n=""),e=Q({},e,r);var u,a=Q({},e.imports,r.imports),r=Se(a),a=mt(a),o=0,f=e.interpolate||R,l="__p+='",f=Qt((e.escape||R).source+"|"+f.source+"|"+(f===N?I:R).source+"|"+(e.evaluate||R).source+"|$","g");
n.replace(f,function(t,e,r,a,f,c){return r||(r=a),l+=n.slice(o,c).replace(q,i),e&&(l+="'+__e("+e+")+'"),f&&(u=y,l+="';"+f+";__p+='"),r&&(l+="'+((__t=("+r+"))==null?'':__t)+'"),o=c+t.length,t}),l+="';\n",f=e=e.variable,f||(e="obj",l="with("+e+"){"+l+"}"),l=(u?l.replace(x,""):l).replace(O,"$1").replace(E,"$1;"),l="function("+e+"){"+(f?"":e+"||("+e+"={});")+"var __t,__p='',__e=_.escape"+(u?",__j=Array.prototype.join;function print(){__p+=__j.call(arguments,'')}":";")+l+"return __p}";try{var c=Gt(r,"return "+l).apply(g,a)
}catch(p){throw p.source=l,p}return t?c(t):(c.source=l,c)},tt.unescape=function(n){return n==h?"":Xt(n).replace(S,ft)},tt.uniqueId=function(n){var t=++_;return Xt(n==h?"":n)+t},tt.all=_t,tt.any=It,tt.detect=jt,tt.findWhere=jt,tt.foldl=Et,tt.foldr=St,tt.include=dt,tt.inject=Et,d(tt,function(n,t){tt.prototype[t]||(tt.prototype[t]=function(){var t=[this.__wrapped__];return ce.apply(t,arguments),n.apply(tt,t)})}),tt.first=Nt,tt.last=function(n,t,e){if(n){var r=0,u=n.length;if(typeof t!="number"&&t!=h){var a=u;
for(t=tt.createCallback(t,e);a--&&t(n[a],a,n);)r++}else if(r=t,r==h||e)return n[u-1];return s(n,_e(0,u-r))}},tt.take=Nt,tt.head=Nt,d(tt,function(n,t){tt.prototype[t]||(tt.prototype[t]=function(t,e){var r=n(this.__wrapped__,t,e);return t==h||e&&typeof t!="function"?r:new et(r)})}),tt.VERSION="1.3.1",tt.prototype.toString=function(){return Xt(this.__wrapped__)},tt.prototype.value=Kt,tt.prototype.valueOf=Kt,wt(["join","pop","shift"],function(n){var t=Zt[n];tt.prototype[n]=function(){return t.apply(this.__wrapped__,arguments)
}}),wt(["push","reverse","sort","unshift"],function(n){var t=Zt[n];tt.prototype[n]=function(){return t.apply(this.__wrapped__,arguments),this}}),wt(["concat","slice","splice"],function(n){var t=Zt[n];tt.prototype[n]=function(){return new et(t.apply(this.__wrapped__,arguments))}}),tt}var g,y=!0,h=null,b=!1,m=[],d=[],_=0,k={},j=+new Date+"",w=75,C=40,x=/\b__p\+='';/g,O=/\b(__p\+=)''\+/g,E=/(__e\(.*?\)|\b__t\))\+'';/g,S=/&(?:amp|lt|gt|quot|#39);/g,I=/\$\{([^\\}]*(?:\\.[^\\}]*)*)\}/g,A=/\w*$/,N=/<%=([\s\S]+?)%>/g,$=($=/\bthis\b/)&&$.test(v)&&$,B=" \t\x0B\f\xa0\ufeff\n\r\u2028\u2029\u1680\u180e\u2000\u2001\u2002\u2003\u2004\u2005\u2006\u2007\u2008\u2009\u200a\u202f\u205f\u3000",F=RegExp("^["+B+"]*0+(?=.$)"),R=/($^)/,T=/[&<>"']/g,q=/['\n\r\t\u2028\u2029\\]/g,D="Array Boolean Date Function Math Number Object RegExp String _ attachEvent clearTimeout isFinite isNaN parseInt setImmediate setTimeout".split(" "),z="[object Arguments]",W="[object Array]",P="[object Boolean]",K="[object Date]",M="[object Function]",U="[object Number]",V="[object Object]",G="[object RegExp]",H="[object String]",J={};
J[M]=b,J[z]=J[W]=J[P]=J[K]=J[U]=J[V]=J[G]=J[H]=y;var L={"boolean":b,"function":y,object:y,number:b,string:b,undefined:b},Q={"\\":"\\","'":"'","\n":"n","\r":"r","\t":"t","\u2028":"u2028","\u2029":"u2029"},X=L[typeof exports]&&exports,Y=L[typeof module]&&module&&module.exports==X&&module,Z=L[typeof global]&&global;!Z||Z.global!==Z&&Z.window!==Z||(n=Z);var nt=v();typeof define=="function"&&typeof define.amd=="object"&&define.amd?(n._=nt, define(function(){return nt})):X&&!X.nodeType?Y?(Y.exports=nt)._=nt:X._=nt:n._=nt
}(this);;
/**
 * File:        datatables.responsive.js
 * Version:     0.1.3
 * Author:      Seen Sai Yang
 * Info:        https://github.com/Comanche/datatables-responsive
 *
 * Copyright 2013 Seen Sai Yang, all rights reserved.
 *
 * This source file is free software, under either the GPL v2 license or a
 * BSD style license.
 *
 * This source file is distributed in the hope that it will be useful, but
 * WITHOUT ANY WARRANTY; without even the implied warranty of MERCHANTABILITY
 * or FITNESS FOR A PARTICULAR PURPOSE. See the license files for details.
 *
 * You should have received a copy of the GNU General Public License and the
 * BSD license along with this program.  These licenses are also available at:
 *     https://raw.github.com/Comanche/datatables-responsive/master/license-gpl2.txt
 *     https://raw.github.com/Comanche/datatables-responsive/master/license-bsd.txt
 */

'use strict';

/**
 * Constructor for responsive datables helper.
 *
 * This helper class makes datatables responsive to the window size.
 *
 * The parameter, breakpoints, is an object for each breakpoint key/value pair
 * with the following format: { breakpoint_name: pixel_width_at_breakpoint }.
 *
 * An example is as follows:
 *
 *     {
 *         tablet: 1024,
 *         phone: 480
 *     }
 *
 * These breakpoint name may be used as possible values for the data-hide
 * attribute.  The data-hide attribute is optional and may be defined for each
 * th element in the table header.
 *
 * @param {Object|string} tableSelector jQuery wrapped set or selector for
 *                                      datatables container element.
 * @param {Object} breakpoints          Object defining the responsive
 *                                      breakpoint for datatables.
 */
function ResponsiveDatatablesHelper(tableSelector, breakpoints, expandAllCallback) {
    if (typeof tableSelector === 'string') {
        this.tableElement = $(tableSelector);
    } else {
        this.tableElement = tableSelector;
    }

    // State of column indexes and which are shown or hidden.
    this.columnIndexes = [];
    this.columnsShownIndexes = [];
    this.columnsHiddenIndexes = [];

    this.expandAllCallback = expandAllCallback;
    this.respondCallback = null;

    // Index of the th in the header tr that stores where the attribute
    //     data-class="expand"
    // is defined.
    this.expandColumn = undefined;

    // Stores the break points defined in the table header.
    // Each th in the header tr may contain an optional attribute like
    //     data-hide="phone,tablet"
    // These attributes and the breakpoints object will be used to create this
    // object.
    this.breakpoints = {
        /**
         * We will be generating data in the following format:
         *     phone : {
         *         lowerLimit   : undefined,
         *         upperLimit   : 320,
         *         columnsToHide: []
         *     },
         *     tablet: {
         *         lowerLimit   : 320,
         *         upperLimit   : 724,
         *         columnsToHide: []
         *     }
         */

    };

    // Expand icon template
    this.expandIconTemplate = '<span class="responsiveExpander"></span>';

    // Row template
    this.rowTemplate = '<tr class="row-detail"><td><ul><!--column item--></ul></td></tr>';
    this.rowLiTemplate = '<li><div class="columnTitle"><!--column title--></div> <div class="columnValue"><!--column value--></div></li>';

    // Responsive behavior on/off flag
    this.disabled = true;

    // Skip next windows width change flag
    this.skipNextWindowsWidthChange = false;

    // Initialize settings
    this.init(breakpoints);
}

/**
 * Responsive datatables helper init function.  Builds breakpoint limits
 * for columns and begins to listen to window resize event.
 *
 * See constructor for the breakpoints parameter.
 *
 * @param {Object} breakpoints
 */
ResponsiveDatatablesHelper.prototype.init = function (breakpoints) {
    // Add the 'always' breakpoint
    breakpoints['always'] = Infinity;

    /** Generate breakpoints in the format we need. ***************************/
    // First, we need to create a sorted array of the breakpoints given.
    var breakpointsSorted = [];
    _.each(breakpoints, function (value, key) {
        breakpointsSorted.push({
            name: key,
            upperLimit: value,
            columnsToHide: []
        });
    });
    breakpointsSorted = _.sortBy(breakpointsSorted, 'upperLimit');

    // Set lower and upper limits for each breakpoint.
    var lowerLimit = undefined;
    _.each(breakpointsSorted, function (value) {
        value.lowerLimit = lowerLimit;
        lowerLimit = value.upperLimit;
    });

    // Add the default breakpoint which shows all (has no upper limit).
    breakpointsSorted.push({
        name: 'default',
        lowerLimit: lowerLimit,
        upperLimit: undefined,
        columnsToHide: []
    });

    var i, l;

    // Copy the sorted breakpoint array into the breakpoints object using the
    // name as the key.
    for (i = 0, l = breakpointsSorted.length; i < l; i++) {
        this.breakpoints[breakpointsSorted[i].name] = breakpointsSorted[i];
    }

    /** Create range of possible column indexes *******************************/
    // Get all visible column indexes
    var columns = this.tableElement.fnSettings().aoColumns;
    for (i = 0, l = columns.length; i < l; i++) {
        if (columns[i].bVisible) {
            this.columnIndexes.push(i);
        }
    }

    // We need the range of possible column indexes to calculate the columns
    // to show:
    //     Columns to show = all columns - columns to hide
    var headerElements = $('thead th', this.tableElement);

    /** Add columns into breakpoints respectively *****************************/
    // Read column headers' attributes and get needed info
    _.each(headerElements, function (element, index) {
        // Get the column with the attribute data-class="expand" so we know
        // where to display the expand icon.
        if ($(element).attr('data-class') === 'expand') {
            this.expandColumn = index;
        }

        // The data-hide attribute has the breakpoints that this column
        // is associated with.
        // If it's defined, get the data-hide attribute and sort this
        // column into the appropriate breakpoint's columnsToHide array.
        var dataHide = $(element).attr('data-hide');
        if (dataHide !== undefined && dataHide !== null) {
            var splitBreakingPoints = dataHide.split(/,\s*/);
            _.each(splitBreakingPoints, function (e) {
                if (this.breakpoints[e] !== undefined) {
                    // Translate visible column index to internal column index.
                    this.breakpoints[e].columnsToHide.push(this.columnIndexes[index]);
                }
            }, this);
        }
    }, this);

    // Enable responsive behavior.
    this.disable(false);
};

ResponsiveDatatablesHelper.prototype.setWindowsResizeHandler = function (bindFlag) {
    if (bindFlag === undefined) {
        bindFlag = true;
    }

    if (bindFlag) {
        var that = this;
        $(window).on("resize", function () {
            that.respond();
        });
    } else {
        $(window).off("resize");
    }
}; /**
 * Respond window size change.  This helps make datatables responsive.
 */
ResponsiveDatatablesHelper.prototype.respond = function () {
    if (this.disabled) {
        return;
    }
    var that = this;

    // Get new windows width
    var newWindowWidth = viewportSize.getWidth();

    // Loop through breakpoints to see which columns need to be shown/hidden.
    var newColumnsToHide = [];
    _.each(this.breakpoints, function (element) {
        if ((!element.lowerLimit || newWindowWidth > element.lowerLimit) && (!element.upperLimit || newWindowWidth <= element.upperLimit)) {
            newColumnsToHide = element.columnsToHide;
        }
    }, this);

    // Find out if a column show/hide should happen.
    // Skip column show/hide if this window width change follows immediately
    // after a previous column show/hide.  This will help prevent a loop.
    var columnShowHide = false;
    if (!this.skipNextWindowsWidthChange) {
        // Check difference in length
        if (this.columnsHiddenIndexes.length !== newColumnsToHide.length) {
            // Difference in length
            columnShowHide = true;
        } else {
            // Same length but check difference in values
            var d1 = _.difference(this.columnsHiddenIndexes, newColumnsToHide).length;
            var d2 = _.difference(newColumnsToHide, this.columnsHiddenIndexes).length;
            columnShowHide = d1 + d2 > 0;
        }
    }

    if (columnShowHide) {
        // Showing/hiding a column at breakpoint may cause a windows width
        // change.  Let's flag to skip the column show/hide that may be
        // caused by the next windows width change.
        this.skipNextWindowsWidthChange = true;
        this.columnsHiddenIndexes = newColumnsToHide;
        this.columnsShownIndexes = _.difference(this.columnIndexes, this.columnsHiddenIndexes);
        this.showHideColumns();
        this.skipNextWindowsWidthChange = false;
    }


    // We don't skip this part.
    // If one or more columns have been hidden, add the has-columns-hidden class to table.
    // This class will show what state the table is in.
    if (this.columnsHiddenIndexes.length) {
        this.tableElement.addClass('has-columns-hidden');

        // Show details for each row that is tagged with the class .detail-show.
        $('tr.detail-show', this.tableElement).each(function (index, element) {
            var tr = $(element);
            if (tr.next('.row-detail').length === 0) {
                ResponsiveDatatablesHelper.prototype.showRowDetail(that, tr);
            }
        });

        if (this.expandAllCallback && typeof (this.expandAllCallback) === "function") {
            this.expandAllCallback();
        }
    } else {
        this.tableElement.removeClass('has-columns-hidden');
        $('tr.row-detail', this.tableElement).each(function () {
            ResponsiveDatatablesHelper.prototype.hideRowDetail(that, $(this).prev());
        });
    }

    Common.TableResizer(this.tableElement, this.parentSelector);
    if (this.respondCallback) {
        this.respondCallback();
    }
};

/**
 * Show/hide datatables columns.
 */
ResponsiveDatatablesHelper.prototype.showHideColumns = function () {
    // INDIV: select all <col> elements in the <colgroup> section
    var cols = this.tableElement.children('colgroup').children();
    var i, l;

    // Calculate the columns to show
    // Show columns that may have been previously hidden.
    for (i = 0, l = this.columnsShownIndexes.length; i < l; i++) {
        var col = $(cols[this.columnsShownIndexes[i]]);
        if (col.length > 0 && !col.attr['data-hide="always"']) {
            $(cols[this.columnsShownIndexes[i]]).show();
        }

        this.tableElement.fnSetColumnVis(this.columnsShownIndexes[i], true, false);
    }

    // Hide columns that may have been previously shown.
    for (i = 0, l = this.columnsHiddenIndexes.length; i < l; i++) {
        if ($(cols[this.columnsHiddenIndexes[i]])) {
            $(cols[this.columnsHiddenIndexes[i]]).hide();
        }

        this.tableElement.fnSetColumnVis(this.columnsHiddenIndexes[i], false, false);
    }

    // Rebuild details to reflect shown/hidden column changes.
    var that = this;
    $('tr.row-detail', this.tableElement).each(function () {
        ResponsiveDatatablesHelper.prototype.hideRowDetail(that, $(this).prev());
    });
    if (this.tableElement.hasClass('has-columns-hidden')) {
        $('tr.detail-show', this.tableElement).each(function (index, element) {
            ResponsiveDatatablesHelper.prototype.showRowDetail(that, $(element));
        });
    }
};

/**
 * Create the expand icon on the column with the data-class="expand" attribute
 * defined for it's header.
 *
 * @param {Object} tr table row object
 */
ResponsiveDatatablesHelper.prototype.createExpandIcon = function (tr) {
    if (this.disabled) {
        return;
    }

    // INDIV: Find the new index of the column with 'data-class="expand"'
    var headerElements = $('thead th', this.tableElement);
    var that = this;

    /** Add columns into breakpoints respectively *****************************/
    // Read column headers' attributes and get needed info
    _.each(headerElements, function (element, index) {
        // Get the column with the attribute data-class="expand" so we know
        // where to display the expand icon.
        if ($(element).attr('data-class') === 'expand') {
            that.expandColumn = index;
        }
    });
    // End INDIV changes

    // Get the td for tr with the same index as the th in the header tr
    // that has the data-class="expand" attribute defined.
    var tds = $('td', tr);

    // Loop through tds and create an expand icon on the td that has a column
    // index equal to the expand column given.
    for (var i = 0, l = tds.length; i < l; i++) {
        var td = tds[i];
        var tdIndex = that.tableElement.fnGetPosition(td)[1];
        td = $(td);
        if (tdIndex === that.expandColumn) {
            // Create expand icon if there isn't one already.
            if ($('span.responsiveExpander', td).length == 0) {
                td.prepend(that.expandIconTemplate);

                // Respond to click event on expander icon.
                td.on('click', 'span.responsiveExpander', { responsiveDatatablesHelperInstance: that }, that.showRowDetailEventHandler);
            }
            break;
        }
    }
};

/**
 * Show row detail event handler.
 *
 * This handler is used to handle the click event of the expand icon defined in
 * the table row data element.
 *
 * @param {Object} event jQuery event object
 */
ResponsiveDatatablesHelper.prototype.showRowDetailEventHandler = function (event) {
    if (this.disabled) {
        return;
    }

    // Get the parent tr of which this td belongs to.
    var tr = $(this).closest('tr');

    // Show/hide row details
    if (tr.hasClass('detail-show')) {
        ResponsiveDatatablesHelper.prototype.hideRowDetail(event.data.responsiveDatatablesHelperInstance, tr);
    } else {
        ResponsiveDatatablesHelper.prototype.showRowDetail(event.data.responsiveDatatablesHelperInstance, tr);
    }

    tr.toggleClass('detail-show');

    // Prevent click event from bubbling up to higher-level DOM elements.
    event.stopPropagation();
};

/**
 * Show row details.
 *
 * @param {ResponsiveDatatablesHelper} responsiveDatatablesHelperInstance instance of ResponsiveDatatablesHelper
 * @param {Object}                     tr                                 jQuery wrapped set
 */
ResponsiveDatatablesHelper.prototype.showRowDetail = function (responsiveDatatablesHelperInstance, tr) {
    // Get column because we need their titles.
    var tableContainer = responsiveDatatablesHelperInstance.tableElement;
    var columns = tableContainer.fnSettings().aoColumns;

    // Create the new tr.
    var newTr = $(responsiveDatatablesHelperInstance.rowTemplate);

    Common.CopyHtmlAttributes(tr, newTr);

    newTr.removeClass('detail-show');

    // Get the ul that we'll insert li's into.
    var ul = $('ul', newTr);

    // Loop through hidden columns and create an li for each of them.
    _.each(responsiveDatatablesHelperInstance.columnsHiddenIndexes, function (index) {
        var li = $(responsiveDatatablesHelperInstance.rowLiTemplate);
        $('.columnTitle', li).html(columns[index].sTitle);

        var templateTr = tableContainer.fnGetData(tr[0])[0];
        var td = $('td:nth-child(' + (index + 1) + ')', templateTr);
        var rowHtml = $(td).contents().clone();

        $('.columnValue', li).html(rowHtml);

        // Copy index to data attribute, so we'll know where to put the value when the tr.row-detail is removed
        li.attr('data-column', index);

        // Copy td class to new li
        var tdClass = $(td).attr('class');
        if (tdClass !== 'undefined' && tdClass !== false && tdClass !== '') {
            li.addClass(tdClass);
        }

        ul.append(li);
    });

    // Create tr colspan attribute
    var colspan = responsiveDatatablesHelperInstance.columnIndexes.length - responsiveDatatablesHelperInstance.columnsHiddenIndexes.length;
    newTr.find('* > td').attr('colspan', colspan);

    // Append the new tr after the current tr.
    tr.after(newTr);
};

/**
 * Hide row details.
 *
 * @param {ResponsiveDatatablesHelper} responsiveDatatablesHelperInstance instance of ResponsiveDatatablesHelper
 * @param {Object}                     tr                                 jQuery wrapped set
 */
ResponsiveDatatablesHelper.prototype.hideRowDetail = function (responsiveDatatablesHelperInstance, tr) {

    // If the value of an input has changed, we need to copy its state back to the DataTables object
    // so that value will persist when the tr.row-detail is removed
    tr.next('.row-detail').find('li').each(function () {
        var tableContainer = responsiveDatatablesHelperInstance.tableElement;
        var aoData = tableContainer.fnSettings().aoData;
        var rowIndex = tableContainer.fnGetPosition(tr[0]);
        var column = $(this).attr('data-column');
        var td = $(this).find('div.columnValue').contents();
        aoData[rowIndex]._anHidden[column] = $(aoData[rowIndex]._anHidden[column]).empty().append(td)[0];
    });
    tr.next('.row-detail').remove();
};

/**
 * Enable/disable responsive behavior and restores changes made.
 *
 * @param {Boolean} disable, default is true
 */
ResponsiveDatatablesHelper.prototype.disable = function (disable) {
    this.disabled = (disable === undefined) || disable;

    if (this.disabled) {
        // Remove windows resize handler
        this.setWindowsResizeHandler(false);

        // Remove all trs that have row details.
        $('tbody tr.row-detail', this.tableElement).remove();

        // Remove all trs that are marked to have row details shown.
        $('tbody tr', this.tableElement).removeClass('detail-show');

        // Remove all expander icons
        $('tbody tr span.responsiveExpander', this.tableElement).remove();

        this.columnsHiddenIndexes = [];
        this.columnsShownIndexes = this.columnIndexes;
        this.showHideColumns();
        this.tableElement.removeClass('has-columns-hidden');

        this.tableElement.off('click', 'span.responsiveExpander', this.showRowDetailEventHandler);
    } else {
        // Add windows resize handler
        this.setWindowsResizeHandler();
    }
};

ResponsiveDatatablesHelper.prototype.expandAll = function () {
    if (this.disabled || this.expandColumn === undefined) {
        return;
    }

    $('span.responsiveExpander', this.tableElement).trigger("click");
};

/**
* Get an array of TD nodes from DataTables for a given row, including any column elements which are hidden.
*
* Author: Allan Jardine
* http://datatables.net/plug-ins/api
*
* @param {Object} oSettings DataTables settings object
* @param {node}   mTr       TR node or aoData index
*/
$.fn.dataTableExt.oApi.fnGetTds = function (oSettings, mTr) {
    var anTds = [];
    var anVisibleTds = [];
    var iCorrector = 0;
    var nTd, iColumn, iColumns;

    /* Take either a TR node or aoData index as the mTr property */
    var iRow = (typeof mTr == 'object') ?
        oSettings.oApi._fnNodeToDataIndex(oSettings, mTr) : mTr;
    var nTr = oSettings.aoData[iRow].nTr;

    /* Get an array of the visible TD elements */
    for (iColumn = 0, iColumns = nTr.childNodes.length; iColumn < iColumns; iColumn++) {
        nTd = nTr.childNodes[iColumn];
        if (nTd.nodeName.toUpperCase() == "TD") {
            anVisibleTds.push(nTd);
        }
    }

    /* Construct and array of the combined elements */
    for (iColumn = 0, iColumns = oSettings.aoColumns.length; iColumn < iColumns; iColumn++) {
        if (oSettings.aoColumns[iColumn].bVisible) {
            anTds.push(anVisibleTds[iColumn - iCorrector]);
        } else {
            anTds.push(oSettings.aoData[iRow]._anHidden[iColumn]);
            iCorrector++;
        }
    }

    return anTds;
};;
var DataTablePlugins = function() {

	/*private*/
	function installDisplayRow() {
		$.fn.dataTableExt.oApi.fnDisplayRow = function(oSettings, nRow) {
			// Account for the "display" all case - row is already displayed
			if (oSettings._iDisplayLength == -1) {
				return;
			}

			// Find the node in the table
			var iPos = -1;
			for (var i = 0, iLen = oSettings.aiDisplay.length; i < iLen; i++) {
				if (oSettings.aoData[oSettings.aiDisplay[i]].nTr == nRow) {
					iPos = i;
					break;
				}
			}

			// Alter the start point of the paging display
			if (iPos >= 0) {
				oSettings._iDisplayStart = (Math.floor(i / oSettings._iDisplayLength)) * oSettings._iDisplayLength;
				this.oApi._fnCalculateEnd(oSettings);
			}

			this.oApi._fnDraw(oSettings);
		};
	};

	/*private*/
	function installFindCellRowIndexes() {
		$.fn.dataTableExt.oApi.fnFindCellRowIndexes = function(oSettings, sSearch, iColumn) {
			var
				i,
				iLen,
				j,
				jLen,
				aOut = [],
				aData;

			for (i = 0, iLen = oSettings.aoData.length; i < iLen; i++) {
				aData = oSettings.aoData[i]._aData;

				if (typeof iColumn == 'undefined') {
					for (j = 0, jLen = aData.length; j < jLen; j++) {
						if (aData[j] == sSearch) {
							aOut.push(i);
						}
					}
				} else if (aData[iColumn] == sSearch) {
					aOut.push(i);
				}
			}

			return aOut;
		};
	};

	/*private */
	function installFindCellRowNodes() {
		$.fn.dataTableExt.oApi.fnFindCellRowNodes = function(oSettings, sSearch, iColumn) {
			var
				i,
				iLen,
				j,
				jLen,
				aOut = [],
				aData;

			for (i = 0, iLen = oSettings.aoData.length; i < iLen; i++) {
				aData = oSettings.aoData[i]._aData;

				if (typeof iColumn == 'undefined') {
					for (j = 0, jLen = aData.length; j < jLen; j++) {
						if (aData[j] == sSearch) {
							aOut.push(oSettings.aoData[i].nTr);
						}
					}
				} else if (aData[iColumn] == sSearch) {
					aOut.push(oSettings.aoData[i].nTr);
				}
			}

			return aOut;
		};
	};

	/*private*/
	function installHiddenTitleStringSorting() {
		jQuery.extend(jQuery.fn.dataTableExt.oSort, {
			"title-string-pre": function(a) {
				return a.match(/title="(.*?)"/)[1].toLowerCase();
			},

			"title-string-asc": function(a, b) {
				return ((a < b) ? -1 : ((a > b) ? 1 : 0));
			},

			"title-string-desc": function(a, b) {
				return ((a < b) ? 1 : ((a > b) ? -1 : 0));
			}
		});
	};

	/*private*/
	function installPagingInfo() {
		$.fn.dataTableExt.oApi.fnPagingInfo = function(oSettings) {
			return {
				iStart: oSettings._iDisplayStart,
				iEnd: oSettings.fnDisplayEnd(),
				iLength: oSettings._iDisplayLength,
				iTotal: oSettings.fnRecordsTotal(),
				iFilteredTotal: oSettings.fnRecordsDisplay(),
				iPage: oSettings._iDisplayLength === -1 ? 0 : Math.ceil(oSettings._iDisplayStart / oSettings._iDisplayLength),
				iTotalPages: oSettings._iDisplayLength === -1 ? 0 : Math.ceil(oSettings.fnRecordsDisplay() / oSettings._iDisplayLength)
			};
		};
	};

	return {
		Init: function() {
			installDisplayRow();
			installFindCellRowIndexes();
			installFindCellRowNodes();
			installPagingInfo();
			installHiddenTitleStringSorting();
		}
	};
}();;
var MobileSorting = function(tableContainerSelector, templateContainerSelector) {
    var table = null;
    var initialized = false;

    tableContainerSelector = tableContainerSelector || '';
    templateContainerSelector = templateContainerSelector || '';

    function sortTable() {
        if (!Common.IsVisible($(tableContainerSelector + ' .dataTables_sort'))) {
            return;
        }

        var sortDirection = $(tableContainerSelector + ' [data-id="sortDirectionButton"]').hasClass('mobile-sort-direction-ascending') ? 'asc' : 'desc';
        var sortArray = [$(tableContainerSelector + ' [data-id="sortColumnDropDown"]').val(), sortDirection];
        table.fnSettings().aaSorting = [sortArray];
        table.fnSort([sortArray]);

        var directionButton = $(tableContainerSelector + ' [data-id="sortDirectionButton"]');
        directionButton.prop('disabled', false);
    }

    function toggleSort() {
        var sortDirectionButton = $(tableContainerSelector + ' [data-id="sortDirectionButton"]');

        if (sortDirectionButton.hasClass('mobile-sort-direction-ascending')) {
            sortDirectionButton.removeClass();
            sortDirectionButton.addClass('mobile-sort-direction-descending');
        } else {
            sortDirectionButton.removeClass();
            sortDirectionButton.addClass('mobile-sort-direction-ascending');
        }

        sortTable();
    }

    return {
        InitializeMobileSorting: function(dataTable, callback) {
            table = dataTable;

            var template = $(templateContainerSelector + ' [data-id="sortTemplate"]');

            if (template.length <= 0) {
                return;
            }
            var sortTemplate = template.html();
            template.empty();
            $(tableContainerSelector + ' .dataTables_sort').html(sortTemplate);

            var dropdown = $(tableContainerSelector + ' [data-id="sortColumnDropDown"]');
            var directionButton = $(tableContainerSelector + ' [data-id="sortDirectionButton"]');

            dropdown.on("change", sortTable);
            directionButton.on('click', function (e) {
                e.preventDefault();
                toggleSort();
            });

            if (callback) {
                dropdown.on("change", callback);
                directionButton.on("click", callback);
            }

            initialized = true;
            this.UpdateMobileSorting();
        },

        UpdateMobileSorting: function() {
            if (!initialized) {
                return;
            }

            var sortArray = table.fnSettings().aaSorting;
            var dropdown = $(tableContainerSelector + ' [data-id="sortColumnDropDown"]');
            
            // remove duplicates in sort order drop down
            var uniqueElements = new Array();
            var duplicates = new Array();
            $("#SortedColumnIndex option").each(function (element) {
                var currentIndex = uniqueElements.indexOf((this).innerText);
                if (currentIndex === -1) {
                    uniqueElements.push((this).innerText);
                } else {
                    duplicates.push(element);
                }
            });
            duplicates.forEach(function (element) {
                dropdown[0].remove(element);
            });

            var directionButton = $(tableContainerSelector + ' [data-id="sortDirectionButton"]');

            if (sortArray && sortArray[0]) {
                dropdown.val(sortArray[0][0]);
                directionButton.removeClass();

                if (sortArray[0][1] === 'asc') {
                    directionButton.addClass('mobile-sort-direction-ascending');
                } else if (sortArray[0][1] === 'desc') {
                    directionButton.addClass('mobile-sort-direction-descending');
                }

                if (!dropdown.val()) {
                    directionButton.prop('disabled', true);
                } else {
                    directionButton.prop('disabled', false);
                }
            }
        }
    };
};;
var Account = (function() {

    function updateDropDownList() {
        if ($("#austrianCitizen input[value='Yes']").is(':checked')) {
            $('#idTypes select').val('');

            // show all
            $("#idTypes option[value='DrivingLicenseLimitedFor15Years']").show();
            $("#idTypes option[value='DrivingLicenseWithoutExpiryDate']").show();
            $("#idTypes option[value='IdentityCard']").show();
            $("#idTypes option[value='Passport']").show();
        } else if ($("#europeanUnionCitizen input[value='Yes']").is(':checked')) {
            $('#idTypes select').val('');

            // hide driver licence drop down item
            $("#idTypes option[value='DrivingLicenseLimitedFor15Years']").hide();
            $("#idTypes option[value='DrivingLicenseWithoutExpiryDate']").hide();
            $("#idTypes option[value='IdentityCard']").show();
            $("#idTypes option[value='Passport']").show();
        } else {
            $('#idTypes select').val('');

            // only show passport
            $("#idTypes option[value='DrivingLicenseLimitedFor15Years']").hide();
            $("#idTypes option[value='DrivingLicenseWithoutExpiryDate']").hide();
            $("#idTypes option[value='IdentityCard']").hide();
            $("#idTypes option[value='Passport']").show();
        }
    }

    function setupAustrianEuropeanCitizenEventHandlers() {
        $("#austrianCitizen input[value='Yes']").on("change", function () {
            if (this.checked) {
                $("#europeanUnionCitizen input[value='Yes']").trigger("click");
            }
            updateDropDownList();
        });

        $("#europeanUnionCitizen input[value='No']").on("change", function () {
            if (this.checked) {
                $("#austrianCitizen input[value='No']").trigger("click");
            }
            updateDropDownList();
        });

        $("#austrianCitizen input[value='No']").on("change", function () {
            updateDropDownList();
        });

        $("#europeanUnionCitizen input[value='Yes']").on("change", function () {
            updateDropDownList();
        });
    }

    function setupDatePickers(options) {
        if (!options.datePickerSelectors) {
            return;
        }

        var form = $(options.formSelector);

        var datePickerOptions = {
            language: options.cultureCode,
            autoclose: true,
            forceParse: false,
            todayHighlight: true,
            todayBtn: "linked",
            clearBtn: true,
            startDate: "01/01/1900",
            keyboardNavigation: false,
            format: options.dateFormat.replace(/M/g, "m")
        };

        options.datePickerSelectors.forEach(element => $(element).datepicker(datePickerOptions));

        if (options.isMobileAppContext) {
            datePickerOptions["orientation"] = "right";
        }

        $('[data-type="date"]').off();
        $('[data-type="date"]').on("click", function(e) {
            e.preventDefault();
        });

        $('[data-type="date"]').closest(".date").datepicker().on("changeDate",
            function() {
                var input = $(this).find("input");

                if (!input.is(":focus")) {
                    form.validate().element(input);
                }
            }
        );
    }

    return {
        InitializeLogin: function(loginFormId, confirmButtonId) {
            Common.PrepareForm(loginFormId,
                confirmButtonId,
                function() {
                    Common.AjaxGetPartialView({
                        type: Common.FORM_METHOD_POST,
                        url: $(loginFormId).attr("action"),
                        formData: $(loginFormId).serialize(),
                        containerId: "#loginContainer",
                        formId: loginFormId,
                        hideElements: true,
                        spinner: {
                            keepSpinner: true
                        }
                    });
                });
            Common.MarkAsNumeric("#loginCustomerNumberInput");

            $("#loginCustomerNumberInput").off("blur");
            $("#loginPasswordInput").off("blur");

            if (Common.IsPhoneMode) {
                $("body").scrollTop(0);
            }
        },

        InitializeCredentialsForgotten: function (options) {
            Common.PrepareForm(options.formSelector, options.submitButtonSelector);
            $('[data-toggle="tooltip"]').tooltip();
            $('[data-toggle="popover"]').popover();
            setupDatePickers(options);
        },

        InitializeAmlQuestionnaire: function (options) {
            $('#FixedDate').hide();

            ProductDetails.InitializeOrderWizardStepOne(true, options.confirmationCheckboxSelector, options.formSelector, options.submitButtonSelector);
            Common.MarkAsDecimal('#MonthlyIncome');
            options.dateSelectors.forEach(element => DateFormatter.SetupDate(element));
            $(options.confirmationCheckboxSelector).on("click", function () {
                $('#IsConfirmedByUser').val($(options.confirmationCheckboxSelector).is(':checked'));
            });
            setupDatePickers(options);

            $(options.submitButtonSelector).on("click", function() {
                if ($("#uploadResultContainer").html().trim().length <= 0) {
                    $("#uploadErrorContainer").show();
                }
            });

            if ($('#IdTypeSelected').val() === "DrivingLicenseWithoutExpiryDate") {
                // todo duplicate code; put this into a private method
                $('#ExpiryDate').val(options.fixedDate);
                $('#FixedDate').val(options.fixedDate);
                $('#ExpiryDate').prop("disabled", true);
                $('#expiryDateDatePicker').hide();
                $('#FixedDate').show();
            }

            $('#IdTypeSelected').on("change", function () {
                var selectedIdType = $('#IdTypeSelected').val();

                if (selectedIdType === "DrivingLicenseWithoutExpiryDate") {
                    // todo duplicate code; put this into a private method
                    $('#ExpiryDate').val(options.fixedDate);
                    $('#FixedDate').val(options.fixedDate);
                    $('#ExpiryDate').prop("disabled", true);
                    $('#expiryDateDatePicker').hide();
                    $('#FixedDate').show();
                } else {
                    $('#ExpiryDate').val("");
                    $('#ExpiryDate').prop("disabled", false);
                    $('#expiryDateDatePicker').show();
                    $('#FixedDate').hide();
                }
            });

            Common.InstallAtLeastOneCheckboxSelectedHandler({
                submitButtonSelector: options.submitButtonSelector, 
                checkboxSelector: "#OriginOfIncome",
                errorSelector: "#OriginOfIncomeError",
                serverErrorSelector: "OriginOfIncomeSelected"
            });
            Common.InstallAtLeastOneCheckboxSelectedHandler({
                submitButtonSelector: options.submitButtonSelector,
                checkboxSelector: "#PurposeOfUsage",
                errorSelector: "#PurposeOfUsageError",
                serverErrorSelector: "PurposeOfUsageSelected"
            });

            setupAustrianEuropeanCitizenEventHandlers();
        }
    };
})();;
var CurrencyTranslation = (function () {
    var responsiveHelper = null;
    var table = null;
    var mobileSorting = new MobileSorting();
    var paging;
    var entriesPerPage = Common.DISPLAY_LENGTH;
    var keyDownElement = null;
    var selectedRowId = null;

    function setSearchParametersFn() {
        var transactionDescription = $('#currencyNameInput').val();

        if (transactionDescription.length < 2) {
            transactionDescription = '';
        }

        table.fnSettings().aoPreSearchCols[1].sSearch = transactionDescription;
    }

    function applySearchFn() {
        setSearchParametersFn();

        table.fnFilter('');
    }

    function scrollToTop() {
        $('html, body').animate({
            scrollTop: $('#currencyTableContainer').offset().top
        }, 500);
    }

    function handleScrollToTop() {
        var currentEntriesPerPage = table.fnSettings()._iDisplayLength;
        if (entriesPerPage !== currentEntriesPerPage) {
            entriesPerPage = currentEntriesPerPage;
            scrollToTop();
        }
    }

    var setupDataTable = function (options) {
        table = $('#currencyTable').dataTable({
            sDom: "<'row clearfix'<'col-xs-12 visible-xs dataTables_sort'>>t<'row clearfix'<'col-md-4'l><'col-md-6'p><'col-md-2'<'spinner-container'>>'>",
            iDisplayLength: Common.DISPLAY_LENGTH,
            bStateSave: false,
            bFilter: true,
            bProcessing: false,
            bAutoWidth: false,
            sPaginationType: 'full_numbers',
            bServerSide: true,
            sAjaxSource: options.filterUri,
            aLengthMenu: [[15, -1], [15, options.allEntriesLabel]],
            aaSorting: options.sorting,
            aoColumns: [
                { bSortable: false, bVisible: false }, // Complete table row
                { bSortable: true }, // Currency name
                { bSortable: true }, // Valid-on date
                { bSortable: true }, // Rate to use
                { bSortable: false }, // Deviation
                { bSortable: false } // Column used as main column in collapsed state
            ],
            oLanguage: {
                "sUrl": Common.DataTable.LOCALIZATION_URL_PREFIX + options.twoLetterLanguageCode + Common.DataTable.LOCALIZATION_URL_POSTFIX
            },
            fnPreDrawCallback: function () {
                setSearchParametersFn();
                if (!responsiveHelper) {
                    responsiveHelper = new ResponsiveDatatablesHelper(table, Common.DataTable.BREAKPOINT_DEFINITION);
                    responsiveHelper.expandAllCallback = CurrencyTranslation.InstallEventHandlers;
                }
                $('.spinner-container').show();
            },
            fnRowCallback: function (nRow) {
                responsiveHelper.createExpandIcon(nRow);
            },
            fnDrawCallback: function () {
                responsiveHelper.expandAll();
                responsiveHelper.respond();
                Common.TableResizer(table);
                mobileSorting.UpdateMobileSorting();
                CurrencyTranslation.InstallEventHandlers();
                handleScrollToTop();
                table.show();
            },
            fnServerData: function (url, data, callback) {
                $('#currencyTableContainer').removeClass('hidden');

                Common.AjaxWithErrorHandling(url, data, function (result) {
                    if (result.RedirectTo) {
                        location.href = result.RedirectTo;
                    }

                    callback(result);
                    $('.spinner-container').hide();

                    if (paging) {
                        paging = false;
                        scrollToTop();
                    }
                }, '.spinner-container');
            },
            fnCreatedRow: function (nRow, aData) {
                Common.PostprocessTableRow(table, nRow, aData[0]);
                CurrencyTranslation.MarkSelectedRow(nRow);
            },
            fnInitComplete: function () {
                $('#currencyTable th div').on("click", function (e) {
                    $(this).closest('th').trigger("click");
                    e.stopPropagation();
                });
                mobileSorting.InitializeMobileSorting(table);
                table.on('page', function () {
                    paging = true;
                });
            }
        });
    };


    function updateCalculatorValues(fromInput, toInput, multiply) {
        var value = fromInput.autoNumeric("get");

        if (value === "") {
            toInput.val("");
        } else {
            var rate = parseFloat($("#exchangeRateSpan").attr("data-value"));
            toInput.autoNumeric("set", multiply ? value * rate : value / rate);
        }
    };

    return {
        InitializeFilter: function () {
            $('#currencyNameInput').on("keyup", function () {
                Common.Delay(function () {
                    applySearchFn();
                }, Common.DEBOUNCE_TIME_IN_MS);
            });
        },

        InitializeCalculator: function (options) {
            calculatorOptions = options;
            var amountInput = $("#amountInput");
            var foreignAmountInput = $("#foreignAmountInput");
            var autoNumericOptions = {
                aSep: options.groupSeparator,
                aDec: options.decimalSeparator
            };

            amountInput.autoNumeric("init", autoNumericOptions);
            foreignAmountInput.autoNumeric("init", autoNumericOptions);

            foreignAmountInput.on("keyup", function () {
                if (keyDownElement.attr("id") === $(this).attr("id")) {
                    updateCalculatorValues(foreignAmountInput, amountInput, false);
                }
            });

            foreignAmountInput.on("keydown", function () {
                keyDownElement = $(this);
            });

            amountInput.on("keyup", function () {
                if (keyDownElement.attr("id") === $(this).attr("id")) {
                    updateCalculatorValues(amountInput, foreignAmountInput, true);
                }
            });

            amountInput.on("keydown", function () {
                keyDownElement = $(this);
            });
        },

        CalculateForeignCurrencyAmount: function () {
            $("#initialCalculatorContent").addClass("hidden");
            $("#selectedCalculatorContent").removeClass("hidden");

            var tr = $(this).closest("tr");
            $('table tr').removeClass("selected");
            tr.addClass("selected");
            selectedRowId = tr.attr("data-id");

            $("#currencyNameSpan").text(tr.attr("data-currency-name"));
            $("#exchangeRateSpan").text(tr.attr("data-rate-to-use"));
            $("#exchangeRateSpan").attr("data-value", tr.attr("data-rate-to-use-value"));
            $("#validOnDateSpan").text(tr.attr("data-valid-on-date"));
            $("#amountInput").autoNumeric("set", 1);
            updateCalculatorValues($("#amountInput"), $("#foreignAmountInput"), true);
        },

        HideEmptyExchangeRates: function () {
            var referenceRates = $('.reference-rate.hidden-xs');

            for (var i = 0; i < referenceRates.length; i++) {
                var row = $(referenceRates[i]).closest("li");
                $(row).addClass("hidden-xs");
            }
        },

        InstallEventHandlers: function () {
            $('a[data-id="rateToUseLink"]').off();
            $('a[data-id="rateToUseLink"]').on("click", CurrencyTranslation.CalculateForeignCurrencyAmount);
        },

        MarkSelectedRow: function (row) {
            if (selectedRowId === $(row).attr("data-id")) {
                $(row).addClass("selected");
            }
        },

        Setup: function (options) {
            Common.InitializeTooltips();
            $(window).on("resize", function () {
                Common.InitializeTooltips();
            });
            DataTablePlugins.Init();

            // init default datatable
            setupDataTable(options);
        }
    };
})();;
var CryptoUtil = (function () {
    var cryptoObj = window.crypto || window.msCrypto; // for IE 11

    return {
        Base64ToArrayBuffer: function(base64) {
            var binaryString = window.atob(base64);
            var len = binaryString.length;
            var bytes = new Uint8Array(len);
            for (var i = 0; i < len; i++) {
                bytes[i] = binaryString.charCodeAt(i);
            }
            return bytes;
        },

        StringToArrayBuffer: function(text) {
            var buf = new ArrayBuffer(text.length);
            var bufView = new Uint8Array(buf);
            for (var i = 0, strLen = text.length; i < strLen; i++) {
                bufView[i] = text.charCodeAt(i);
            }
            return buf;
        },

        ArrayBufferToString: function(arrayBuffer) {
            return String.fromCharCode.apply(null, new Uint8Array(arrayBuffer));
        },

        GenerateKeyPair: function() {
            return cryptoObj.subtle.generateKey({
                    name: "RSA-OAEP",
                    modulusLength: 2048,
                    publicExponent: new Uint8Array([1, 0, 1]),
                    hash: "SHA-256"
                },
                true,
                ["encrypt", "decrypt"]);
        },

        ExportPublicKey: function(publicKey) {
            return cryptoObj.subtle.exportKey(
                "spki", // = Simple public-key infrastructure
                publicKey
            );
        },

        FormatPublicKey: function(publicKey) {
            const exportedAsString = CryptoUtil.ArrayBufferToString(publicKey);
            const exportedAsBase64 = window.btoa(exportedAsString);

            return "-----BEGIN PUBLIC KEY-----\n" + exportedAsBase64 + "\n-----END PUBLIC KEY-----";
        },

        ExportPrivateKey: function(privateKey) {
            return cryptoObj.subtle.exportKey(
                "pkcs8", // PKCS #8 is a standard syntax for storing private key information
                privateKey
            );
        },

        FormatPrivateKey: function(privateKey) {
            const exportedAsString = CryptoUtil.ArrayBufferToString(privateKey);
            const exportedAsBase64 = window.btoa(exportedAsString);

            return "-----BEGIN PRIVATE KEY-----\n" + exportedAsBase64 + "\n-----END PRIVATE KEY-----";
        },

        EncryptDataWithPublicKey: function(plainText, publicKey) {
            plainText = CryptoUtil.StringToArrayBuffer(plainText);
            return cryptoObj.subtle.encrypt({
                    name: "RSA-OAEP"
                },
                publicKey,
                plainText
            );
        },

        DecryptDataWithPrivateKey: function(cipherTextArrayBuffer, privateKey) {
            return cryptoObj.subtle.decrypt({
                    name: "RSA-OAEP",
                    hash: { name: "SHA-256" }
                },
                privateKey,
                cipherTextArrayBuffer
            );
        },

        ImportPrivateKey: function(pem) {
            // fetch the part of the PEM string between header and footer
            const pemHeader = "-----BEGIN PRIVATE KEY-----";
            const pemFooter = "-----END PRIVATE KEY-----";
            const pemContents = pem.substring(pemHeader.length, pem.length - pemFooter.length);
            // base64 decode the string to get the binary data
            const binaryDerString = window.atob(pemContents);
            // convert from a binary string to an ArrayBuffer
            const binaryDer = CryptoUtil.StringToArrayBuffer(binaryDerString);

            return cryptoObj.subtle.importKey(
                "pkcs8",
                binaryDer,
                {
                    name: "RSA-OAEP",
                    modulusLength: 2048,
                    publicExponent: new Uint8Array([1, 0, 1]),
                    hash: "SHA-256"
                },
                true,
                ["decrypt"]
            );
        },

        ImportPublicKey: function(pem) {
            // fetch the part of the PEM string between header and footer
            const pemHeader = "-----BEGIN PUBLIC KEY-----";
            const pemFooter = "-----END PUBLIC KEY-----";
            const pemContents = pem.substring(pemHeader.length, pem.length - pemFooter.length);
            // base64 decode the string to get the binary data
            const binaryDerString = window.atob(pemContents);
            // convert from a binary string to an ArrayBuffer
            const binaryDer = CryptoUtil.StringToArrayBuffer(binaryDerString);

            return cryptoObj.subtle.importKey(
                "spki",
                binaryDer,
                {
                    name: "RSA-OAEP",
                    hash: "SHA-256"
                },
                true,
                ["encrypt"]
            );
        },

        ConvertToHex: function(cipherText, numberOfBytes) {
            var bufView = new Uint8Array(cipherText);

            var hex = "";
            for (var i = 0; i < numberOfBytes; i++) {
                if (bufView[i] < 10) {
                    hex += "0" + bufView[i].toString(16);
                } else {
                    hex += bufView[i].toString(16);
                }
            }

            return hex;
        }
    };
})();;
var ExchangeRateHistory = (function () {
    var responsiveHelper = null;
    var table = null;
    var mobileSorting = new MobileSorting();
    var paging;
    var entriesPerPage = Common.DISPLAY_LENGTH;

    function setSearchParametersFn() {
        var currencyCode = $('#currencyCodeInput').val();
        var validOnDateFrom = $('#dateFromInput').val();
        var validOnDateTo = $('#dateToInput').val();

        table.fnSettings().aoPreSearchCols[0].sSearch = currencyCode;
        table.fnSettings().aoPreSearchCols[1].sSearch = validOnDateFrom;
        table.fnSettings().aoPreSearchCols[2].sSearch = validOnDateTo;
    }

    function applySearchFn() {
        if ($('#dateFromInput').valid() && $('#dateToInput').valid()) {
            setSearchParametersFn();
            table.fnFilter('');
            return true;
        }

        return false;
    }

    function scrollToTop() {
        $('html, body').animate({
            scrollTop: $('#exchangeRateHistoryTable').offset().top
        }, 500);
    }

    function handleScrollToTop() {
        var currentEntriesPerPage = table.fnSettings()._iDisplayLength;
        if (entriesPerPage !== currentEntriesPerPage) {
            entriesPerPage = currentEntriesPerPage;
            scrollToTop();
        }
    }

    var setupDataTable = function (options) {
        table = $('#exchangeRateHistoryTable').dataTable({
            sDom: "<'row clearfix'<'col-xs-12 visible-xs dataTables_sort'>>t<'row clearfix'<'col-md-4'l><'col-md-6'p><'col-md-2'<'spinner-container'>>'>",
            iDisplayLength: Common.DISPLAY_LENGTH,
            bStateSave: false,
            bFilter: true,
            bProcessing: false,
            bAutoWidth: false,
            sPaginationType: 'full_numbers',
            bServerSide: true,
            sAjaxSource: options.filterUri,
            aLengthMenu: [[15, -1], [15, options.allEntriesLabel]],
            aaSorting: options.sorting,
            aoColumns: [
                { bSortable: false, bVisible: false }, // Complete table row
                { bSortable: true }, // Valid-on date
                { bSortable: true }, // Rate to use
                { bSortable: false }, // Deviation
                { bSortable: false } // Column used as main column in collapsed state
            ],
            oLanguage: {
                "sUrl": Common.DataTable.LOCALIZATION_URL_PREFIX + options.twoLetterLanguageCode + Common.DataTable.LOCALIZATION_URL_POSTFIX
            },
            fnPreDrawCallback: function () {
                setSearchParametersFn();
                if (!responsiveHelper) {
                    responsiveHelper = new ResponsiveDatatablesHelper(table, Common.DataTable.BREAKPOINT_DEFINITION);
                    responsiveHelper.expandAllCallback = function () {
                        CurrencyTranslation.InstallEventHandlers();
                        CurrencyTranslation.HideEmptyExchangeRates();
                    }
                }
                $('.spinner-container').show();
            },
            fnRowCallback: function (nRow) {
                responsiveHelper.createExpandIcon(nRow);
            },
            fnDrawCallback: function () {
                responsiveHelper.expandAll();
                responsiveHelper.respond();
                Common.TableResizer(table);
                mobileSorting.UpdateMobileSorting();
                CurrencyTranslation.InstallEventHandlers();
                handleScrollToTop();
                table.show();
            },
            fnServerData: function (url, data, callback) {
                $('#exchangeRateHistoryTableContainer').removeClass('hidden');

                Common.AjaxWithErrorHandling(url, data, function (result) {
                    if (result.RedirectTo) {
                        location.href = result.RedirectTo;
                    }

                    callback(result);
                    $('.spinner-container').hide();

                    if (paging) {
                        scrollToTop();
                    }
                }, '.spinner-container');
            },
            fnCreatedRow: function (nRow, aData) {
                Common.PostprocessTableRow(table, nRow, aData[0]);
                CurrencyTranslation.MarkSelectedRow(nRow);
            },
            fnInitComplete: function () {
                $('#exchangeRateHistoryTable th div').on("click", function (e) {
                    $(this).closest('th').trigger("click");
                    e.stopPropagation();
                });
                mobileSorting.InitializeMobileSorting(table);
                table.on('page', function () {
                    paging = true;
                });
            }
        });
    };

    function setupDatePickers(options) {
        var form = $('#exchangeRateFilterForm');

        var datePickerOptions = {
            language: options.cultureCode,
            autoclose: true,
            forceParse: false,
            todayHighlight: true,
            todayBtn: "linked",
            clearBtn: true,
            startDate: '01/01/2012',
            keyboardNavigation: false,
            format: options.dateFormat.replace(/M/g, "m")
        };

        $('#dateFromDatePicker').datepicker(datePickerOptions);
        $('#dateToDatePicker').datepicker(datePickerOptions);

        $('[data-type="date"]').off();
        $('[data-type="date"]').on("click", function (e) {
            e.preventDefault();
        });

        $('[data-type="date"]').closest(".date").datepicker().on('changeDate', function () {
            var input = $(this).find('input');

            if (!input.is(":focus")) {
                form.validate().element(input);
            }
        });
    }
    
    function filterDataTableByDate() {
        $('#searchButton').on('click',function() {
            table.fnDraw();
        });
    }
    
    return {
        InitializeFilter: function (options) {
            setupDatePickers(options);
        },

        Setup: function (options) {
            Common.InitializeTooltips();
            $(window).on("resize", function () {
                Common.InitializeTooltips();
            });
            DataTablePlugins.Init();

            // init default datatable
            setupDataTable(options);
            filterDataTableByDate();
        }
    };
})();;
var Faq = function() {

    return {
        InitializeFaq: function() {
            var anchor = window.location.hash;

            if (anchor) {
                // remove any encoded whitespaces in the anchor part of the URL
                anchor = decodeURIComponent(anchor).replace(/\s/, '');
            }

            var anchorElement = $(anchor);

            if (anchorElement.length <= 0) {
                return;
            }

            var type = anchorElement.attr('data-type');

            if (type !== 'category' && type !== 'question') {
                return;
            }

            anchorElement.removeClass('collapsed');

            var categoryBody;
            if (type === 'category') {
                categoryBody = $(anchor + "-Category");

                categoryBody.collapse({
                    toggle: true,
                    parent: '#faqContent'
                });
            } else if (type === 'question') {
                categoryBody = anchorElement.parents('.category-body');

                var categoryId = categoryBody.attr('id').replace('-Category', '');
                var categoryHeader = $('#' + categoryId);

                categoryHeader.removeClass('collapsed');
                categoryBody.collapse({
                    toggle: true,
                    parent: '#faqContent'
                });


                var answer = $(anchor + "-Answer");
                answer.collapse({
                    toggle: true
                });
            }
        }
    };
}();;
var DateFormatter = (function() {

    (function($) {
        $.fn.getCursorPosition = function() {
            var input = this.get(0);
            if (!input) {
                return 0; // No (input) element found
            }
            if (document.selection) {
                // IE
                input.focus();
            }

            return "selectionStart" in input
                ? input.selectionStart
                : "" ||
                Math.abs(document.selection.createRange().moveStart("character", -input.value.length));
        };
    })(jQuery);

    return {
        SetupDate: function(dateSelector) {
            $(dateSelector).on("keyup",
                function(e) {
                    if (e.keyCode === 8 || e.keyCode === 46) {
                        return;
                    }

                    var s = e.target.value;
                    var caretPos = $(e.target).getCursorPosition();

                    // date
                    if (caretPos === 2) {
                        s.length > 2 ? e.target.value[3] = "." : e.target.value += ".";
                    }
                    if (caretPos === 5) {
                        s.length > 5 ? e.target.value[5] = "." : e.target.value += ".";
                    }
                });
        },

        SetupExpiryDate: function(dateSelector) {
            $(dateSelector).on("keyup",
                function(e) {
                    if (e.keyCode === 8 || e.keyCode === 46) {
                        return;
                    }

                    var s = e.target.value;
                    var caretPos = $(e.target).getCursorPosition();

                    // expiry date
                    if (caretPos === 2) {
                        s.length > 2 ? e.target.value[3] = "/" : e.target.value += "/";
                    }
                });
        },

        SetupDateAndTime: function(dateSelector) {
            $(dateSelector).on("keyup",
                function(e) {
                    if (e.keyCode === 8 || e.keyCode === 46) {
                        return;
                    }

                    var s = e.target.value;
                    var caretPos = $(e.target).getCursorPosition();

                    // date
                    if (caretPos === 2) {
                        s.length > 2 ? e.target.value[3] = "." : e.target.value += ".";
                    }
                    if (caretPos === 5) {
                        s.length > 5 ? e.target.value[5] = "." : e.target.value += ".";
                    }

                    // time
                    if (caretPos === 10) {
                        s.length > 10 ? e.target.value[11] = " " : e.target.value += " ";
                    }
                    if (caretPos === 13) {
                        s.length > 13 ? e.target.value[10] = ":" : e.target.value += ":";
                    }
                });
        }
    };
})();;
FileUploadStorage = new Array();

function FileUpload() {

    var fileUploadContainer = null;
    var fileUploadSuccessCallback = null;
    var fileUploadErrorCallback = null;
    var spinner = null;

    function uploadAttachment(id) {
        // Disable upload button
        $("#spanAttachment" + id).attr('disabled', 'disabled');

        // Show spinner
        var spinnerTarget = $("#spanSpinner" + id).get(0);
        spinner = new Spinner({ corners: 0.5, hwaccel: true, length: 5, lines: 11, radius: 7, rotate: 0, speed: 1, trail: 40, width: 3 }).spin(spinnerTarget);

        var formData = new FormData();
        var attachment = document.getElementById("fileChooser" + id).files[0];
        formData.append("attachment", attachment);
        var xhr = new XMLHttpRequest();
        var form = document.getElementById("formFileUpload" + id);
        xhr.open(form.method, form.action, true);
        xhr.addEventListener("load", function (evt) {
            uploadAttachmentCompleted(id, evt.target.response);
        }, false);
        xhr.addEventListener("error", function (evt) {
            uploadAttachmentCompleted(id, evt.target.response);
        }, false);
        xhr.send(formData);
    }

    function uploadAttachmentCompleted(id, response) {
        var uploadResult = undefined;

        // Hide spinner
        if (spinner) {
            spinner.stop();
            spinner = null;
        }
        // Enable upload button
        $("#spanAttachment" + id).removeAttr('disabled');

        // Reset the image form so the file won't get uploaded again
        document.getElementById("formFileUpload" + id).reset();
        try {
            //If filesize exeeds a configured value no data may be sent to the server, so we have to deal with a 404 error
            try {
                uploadResult = JSON.parse($(response)[0].innerHTML);
            } catch (e) {
                console.debug(e.message);
            }

            //If there was an error, display it to the user
            if (!uploadResult && fileUploadErrorCallback) {
                fileUploadErrorCallback();
                return;
            }

            if (uploadResult && uploadResult.IsValid === false) {
                if (uploadResult.Message && fileUploadErrorCallback) {
                    fileUploadErrorCallback(uploadResult.Message);
                }
                return;
            }

            if (fileUploadSuccessCallback) {
                fileUploadSuccessCallback(uploadResult);
            }
        } catch (error) {
            if (fileUploadErrorCallback && $("#uploadTarget" + id).contents().text() != "") {
                fileUploadErrorCallback();
            }
        }
    };

    this.Initialize = function (paramFileUploadContainer, paramFileUploadSuccessCallback, paramFileUploadErrorCallback, id) {
        fileUploadContainer = paramFileUploadContainer;
        fileUploadSuccessCallback = paramFileUploadSuccessCallback;
        fileUploadErrorCallback = paramFileUploadErrorCallback;

        $('#fileChooser' + id).on('change', function () {
            var input = $(this),
                numFiles = input.get(0).files ? input.get(0).files.length : 1,
                label = input.val().replace(/\\/g, '/').replace(/.*\//, '');
            input.trigger('fileselect' + id, [numFiles, label]);
        });

        $('#fileChooser' + id).on('fileselect' + id, function () {
            uploadAttachment(id);
        });
        $("#" + fileUploadContainer).append($("#divFileUpload" + id));

        $(window).on("resize", function () {
            $("#" + fileUploadContainer).append($("#divFileUpload" + id));
        });
    }
};;
var FinancialTransactions = (function() {
    var transactionDateFromDp, transactionDateToDp, postingDateFromDp, postingDateToDp, cycleDateFromDp, cycleDateToDp;
    var selectedTransactions = new Array();

    var init = true;
    var filterOptions;
    var displayStart = 0;
    var simple2EnhancedColMapping = { "1": 1, "2": 2, "3": 3, "4": 4, "5": 9, "6": 5, "7": 10, "8": 11, "9": 6, "10": 7, "11": 8 };
    var enhanced2SimpleColMapping = { "1": 1, "2": 2, "3": 3, "4": 4, "5": 6, "6": 9, "7": 10, "8": 11, "9": 5, "10": 7, "11": 8 };

    var paging = false;
    var spinnerContainer;
    var simpleTableInfo, enhancedTableInfo;

    var ALL = "ALL";

    var originalTimescale;
    var hasClearSearchLinkBeenClicked;
    var hasOriginalTimescaleAutomaticallyBeenSetToAll;

    // -------------------------------------------------------------------------------------------------------------------
    // Common functions
    // -------------------------------------------------------------------------------------------------------------------
    
    function intializePeriodChange(inputFieldSelector, datePickerSelector) {
        $(inputFieldSelector).on("keyup", function () {
            automaticallySetPeriodAndShowHint();
        });

        $(datePickerSelector).datepicker().on("changeDate", function () {
            automaticallySetPeriodAndShowHint();
        });
    }

    function manuallySetPeriod() {
        originalTimescale = $('#quarterSelectionDropDown').val();
        $('#undoPeriodChangeContainer').addClass('hidden');
        hasOriginalTimescaleAutomaticallyBeenSetToAll = false;
    }

    function automaticallySetPeriodAndShowHint() {
        if (originalTimescale && $('#quarterSelectionDropDown').val() !== ALL) {
            originalTimescale = $('#quarterSelectionDropDown').val();
        }

        if ($('#quarterSelectionDropDown').val() !== ALL) {
            $('#quarterSelectionDropDown').val(ALL);
            $('#undoPeriodChangeContainer').removeClass('hidden');
            hasOriginalTimescaleAutomaticallyBeenSetToAll = true;
        }
    }

    function resetOriginalPeriod() {
        if (originalTimescale) {
            $('#quarterSelectionDropDown').val(originalTimescale);
        }
        $('#undoPeriodChangeContainer').addClass('hidden');
        hasOriginalTimescaleAutomaticallyBeenSetToAll = false;
        applySearch();
    }
    
    function addTransactionCallbackButton(containerSelector) {
        var html = $('#callbackButtonTemplate').html();
        var datatableContainer = $(containerSelector + ' .callback_container');
        datatableContainer.append(html);
    }

    function addIconButtons(containerSelector, toggleButtonSelector) {
        var html = $('#iconButtonTemplate').html();
        var datatableMenu = $(containerSelector + " .dataTables_add");
        datatableMenu.append(html);

        $(containerSelector + " " + toggleButtonSelector).removeClass('hidden');
    }

    function addItemsToDatatable(table, containerSelector, toggleButtonSelector) {
        addIconButtons(containerSelector, toggleButtonSelector);
        addTransactionCallbackButton(containerSelector);

        installEventHandlers(table, containerSelector);
    }

    function applyTableSorting(table, selector) {
        var sortArray = table.fnSettings().aaSorting;

        if (sortArray && sortArray[0]) {
            var sortCol = sortArray[0][0];
            var colSpan = $('th ' + selector + '[data-sort="' + sortCol + '"]');

            if (colSpan.length > 0) {
                var asc = sortArray[0][1] === 'asc';

                $('th ' + selector).removeClass();
                colSpan.addClass('sorting');
                colSpan.addClass(asc ? 'sorting_asc' : 'sorting_desc');
            }

            $('td ' + selector + '[data-sort="' + sortCol + '"]').parent('td').addClass('sorting_1');
        }
    }

    function createCallBackOptions() {
        return {
            type: Common.FORM_METHOD_POST,
            url: filterOptions.transactionCallBackUri,
            formData: {
                isPrepaidContext: filterOptions.isPrepaidContext,
                transactions: selectedTransactions
            },
            spinner: { noSpinner: true },
            traditional: true
        };
    }

    function createTableOptions(filterUrl, getTableFn, applyColumnStylesFn, preDrawCallbackFn, containerSelector, getSpinnerContainerFn,
        isFilteringAllowedFn, mobileSorting) {
        var responsiveHelper = null;

        return {
            sDom: "<'row'<'col-md-9 col-xs-10 dataTables_header'l><'col-md-3 col-xs-2 dataTables_add'>r><'row clearfix'<'col-xs-12 visible-xs dataTables_sort'>>t<'row'<'col-md-3'i><'col-md-5 col-lg-6'p><'col-md-4 col-lg-3'<'callback_container spinner-container'>>'>",
            iDisplayLength: Common.DISPLAY_LENGTH,
            bLengthChange: false,
            bStateSave: false,
            bFilter: true,
            bProcessing: false,
            bAutoWidth: false,
            sPaginationType: 'full_numbers',
            bServerSide: true,
            sAjaxSource: filterUrl,
            oLanguage: {
                "sUrl": Common.DataTable.LOCALIZATION
            },
            fnPreDrawCallback: function() {
                var table = getTableFn();

                // This is required so that the first table view toggle keeps the correct page numer. Due to a weird bahviour of the datatable, this is not done automatically (altough
                // everything is set up correctly).
                if (displayStart) {
                    table.fnSettings()._iDisplayStart = displayStart;
                    displayStart = 0;
                }

                preDrawCallbackFn();

                if (!responsiveHelper) {
                    responsiveHelper = new ResponsiveDatatablesHelper(table, Common.DataTable.BREAKPOINT_DEFINITION);
                    responsiveHelper.respondCallback = applyColumnStylesFn;
                    responsiveHelper.parentSelector = containerSelector;
                }
            },
            fnRowCallback: function(nRow) {
                responsiveHelper.createExpandIcon(nRow);
            },
            fnDrawCallback: function() {
                var table = getTableFn();
                responsiveHelper.expandAll();
                responsiveHelper.respond();

                if (!init) {
                    if (filterOptions.isEnhanced) {
                        $('#simpleTableContainer').addClass('hidden');
                        $('#enhancedTableContainer').removeClass('hidden');
                    } else {
                        $('#enhancedTableContainer').addClass('hidden');
                        $('#simpleTableContainer').removeClass('hidden');
                    }
                }

                Common.TableResizer(table, containerSelector);
                table.show();
                applyColumnStylesFn();
                mobileSorting.UpdateMobileSorting();
                FinancialTransactions.CheckPreviouslyCheckedCheckboxes();
                FinancialTransactions.AddTransactionCallBackEventHandler();
            },
            fnServerData: function(url, data, callback) {
                if (init && isFilteringAllowedFn()) {
                    $(containerSelector).removeClass('hidden');
                    init = false;
                }

                spinnerContainer = getSpinnerContainerFn() || containerSelector;

                Common.AjaxWithErrorHandling(url, data, function(result) {
                    if (result.RedirectTo) {
                        location.href = result.RedirectTo;
                    }

                    displayBalanceInformation(result.oAdditionalData);
                    callback(result);

                    if (paging) {
                        paging = false;
                        $('html, body').animate({
                            scrollTop: $(containerSelector).offset().top
                        }, 500);
                    }
                }, spinnerContainer + ' .spinner-container', { hideElements: true });

                spinnerContainer = null;
                return true;
            },
            fnCreatedRow: function(nRow, aData) {
                var table = getTableFn();
                Common.PostprocessTableRow(table, nRow, aData[0]);
            }
        };
    }

    function displayBalanceInformation(additionalData) {
        if (additionalData && additionalData.Balance != undefined) {
            var balanceContainer = $('#balanceInformationContainer');

            $('#balanceValue').text(additionalData.Balance);
            $('#balanceCurrency').text(additionalData.Currency);
            $('.dataTables_header').html(balanceContainer.html());
        }
    }

    function handleTransactionCallBack(e) {
        if (selectedTransactions.length > 0) {
            Common.AjaxGetPartialView(createCallBackOptions());
        } else {
            $('[data-id="callbackError"]').removeClass('hidden');
        }

        e.preventDefault();
    }

    function installEventHandlers(table, containerSelector) {
        $(containerSelector + ' [data-id="toggleTableView"]').on("click", toggleTableView);
        $(containerSelector + ' [data-id="downloadCsvData"]').on("click", downloadCsvData);
        $(containerSelector + ' [data-id="callbackButton"]').on("click", handleTransactionCallBack);

        function downloadCsvData(e) {
            var settings, aoData, downloadUri;
            if (filterOptions.isEnhanced) {
                settings = enhancedTableInfo.table.fnSettings();
                aoData = enhancedTableInfo.table._fnAjaxParameters(settings);
                downloadUri = filterOptions.downloadEnhancedCsvUri;
            } else {
                settings = simpleTableInfo.table.fnSettings();
                aoData = simpleTableInfo.table._fnAjaxParameters(settings);
                downloadUri = filterOptions.downloadCsvUri;
            }

            // use the aoData array and append a hidden input field with the correct name/value, so that the data will be posted to the server
            // when doing a form.submit
            var form = $(containerSelector + ' [data-id="downloadCsvDataForm"]');
            form.empty();
            form.attr('action', downloadUri);

            for (var i = 0; i < aoData.length; i++) {
                form.append('<input type="hidden" name="' + aoData[i].name + '" value="' + aoData[i].value + '" />');
            }
            
            form.submit();
            e.preventDefault();
        }
    }

    function setupResizeBehavior() {
        resizeForm();
        $(window).on('resize', function() {
            resizeForm();
            Common.Delay(resizeForm, 100);
        });
    }

    function toggleTableView() {
        Common.AjaxGetPartialView({
            type: 'GET',
            url: filterOptions.toggleUri,
            preventUpdate: true,
            callback: function() {
                var page, sorting;

                filterOptions.isEnhanced = !filterOptions.isEnhanced;

                if (filterOptions.isEnhanced) {
                    spinnerContainer = '#simpleTableContainer';
                    page = simpleTableInfo.table.fnPagingInfo().iPage;
                    sorting = simpleTableInfo.table.fnSettings().aaSorting;
                    displayStart = simpleTableInfo.table.fnPagingInfo().iStart;

                    enhancedTableInfo.table.fnSettings().aaSorting = [[simple2EnhancedColMapping[sorting[0][0]], sorting[0][1]]];
                    enhancedTableInfo.table.fnPageChange(page);
                } else {
                    spinnerContainer = '#enhancedTableContainer';
                    page = enhancedTableInfo.table.fnPagingInfo().iPage;
                    sorting = enhancedTableInfo.table.fnSettings().aaSorting;
                    displayStart = enhancedTableInfo.table.fnPagingInfo().iStart;

                    simpleTableInfo.table.fnSettings().aaSorting = [[enhanced2SimpleColMapping[sorting[0][0]], sorting[0][1]]];
                    simpleTableInfo.table.fnPageChange(page);
                }
            },
            spinner: { noSpinner: true }
        });

        return false;
    }

    // -------------------------------------------------------------------------------------------------------------------
    // Functions used for the filter and search form
    // -------------------------------------------------------------------------------------------------------------------

    function applySearch() {
        if (filterOptions.isEnhanced) {
            enhancedTableInfo.search();
        } else {
            simpleTableInfo.search();
        }
    };

    function clearSearchForm(e) {
        $('#resetFormInput').val('True');
        //Common.AjaxGetPartialView(createSearchFormOptions());

        $('#TransactionDateFrom').val('');
        $('#TransactionDateTo').val('');
        $('#PostingDateFrom').val('');
        $('#PostingDateTo').val('');
        $('#CycleDateFrom').val('');
        $('#CycleDateTo').val('');
        $('#AmountFrom').val('');
        $('#AmountTo').val('');
        $('#SelectedCurrency').val('');
        $('#SelectedTransactionType').val('');

        closeDatePickers();
        e.preventDefault();
    };

    function closeDatePickers() {
        var pickers = [transactionDateFromDp, transactionDateToDp, postingDateFromDp, postingDateToDp, cycleDateFromDp, cycleDateToDp];

        for (var index in pickers) {
            if (pickers.hasOwnProperty(index)) {
                pickers[index].datepicker('hide');
            }
        }
    };

    function openSearchIfRequired() {
        var isOpen = $('#searchContent[data-open]').length > 0;
        if (!isOpen) {
            // This needs to be done in order to close the search container after the input sizes habe been set correctly
            $('#searchContent').removeClass('in');
        }
    };

    function resizeForm() {
        var shown = $('#searchContent').hasClass('in');

        $('#searchContent').addClass('in');
        $('#searchContent').css('height', 'auto');
        setCorrectSizes();
        if (!shown) {
            $('#searchContent').removeClass('in');
            $('#searchContent').css('height', '0');
        }
    };

    function setCorrectSizes() {
        $('[data-fill-up-space]').each(function() {
            var field = $(this);

            if (field.closest('#searchContent').length > 0 && !$('#searchContent').hasClass('in')) {
                return;
            }
            var container = $(field).closest('[data-container]');
            var label = $('.control-label', container);

            field.width(container.width() - label.outerWidth(true) - Common.HorizontalAdditionalSpace(field) - 2);
        });
    };

    function setupDatePickers(options) {
        var form = $('#searchForm');

        var datePickerOptions = {
            language: options.cultureCode,
            autoclose: true,
            forceParse: false,
            todayHighlight: true,
            todayBtn: "linked",
            clearBtn: true,
            startDate: "-13m",
            endDate: "0d",
            keyboardNavigation: false,
            format: options.dateFormat.replace(/M/g, "m")
        };

        transactionDateFromDp = $('#transactionDateFromDatePicker').datepicker(datePickerOptions);
        postingDateFromDp = $('#postingDateFromDatePicker').datepicker(datePickerOptions);
        cycleDateFromDp = $('#cycleDateFromDatePicker').datepicker(datePickerOptions);

        if (options.isMobileAppContext) {
            datePickerOptions["orientation"] = "right";
        }
        transactionDateToDp = $('#transactionDateToDatePicker').datepicker(datePickerOptions);
        postingDateToDp = $('#postingDateToDatePicker').datepicker(datePickerOptions);
        cycleDateToDp = $('#cycleDateToDatePicker').datepicker(datePickerOptions);

        $('[data-type="date"]').off();
        $('[data-type="date"]').on("click", function(e) {
            e.preventDefault();
        });

        /*if (jQuery.browser.mobile) {
            $('[data-type="date"]').prop('type', 'date');
        }*/

        $('[data-type="date"]').closest(".date").datepicker().on('changeDate', function() {
            var input = $(this).find('input');

            if (!input.is(":focus")) {
                form.validate().element(input);
            }
        });
    };

    // -------------------------------------------------------------------------------------------------------------------
    // Functions used for the SIMPLE datatable
    // -------------------------------------------------------------------------------------------------------------------

    function setupSimpleTable() {
        var mobileSorting = new MobileSorting('#simpleTableContainer', '#simpleMobileSortingContainer');
        var table;

        function applyColumnStylesFn() {
            $('#simpleTableContainer th').off('click');
            $('#simpleTableContainer th div').off('click');
            $('#simpleTableContainer th div').on("click", sortColumnFn);

            DataTableHelper.TruncateCellContent();
            applyTableSorting(table, 'div:first-child');
            Common.InitializeTooltips();
            DataTableHelper.ApplyTooltips();
            FinancialTransactions.AddTransactionCallBackEventHandler();
            FinancialTransactions.CheckPreviouslyCheckedCheckboxes();
        }

        function getSpinnerContainerFn() { return spinnerContainer; }

        function getTableFn() { return table; }

        function isFilteringAllowedFn() { return !filterOptions.isEnhanced; }

        function preDrawCallbackFn() { setSearchParametersFn(); }

        function setSearchParametersFn() {
            var transactionDescription = $('#descriptionInput').val();

            if (transactionDescription.length < 3) {
                transactionDescription = '';
            }

            table.fnSettings().aoPreSearchCols[3].sSearch = $('#quarterSelectionDropDown').val();
            table.fnSettings().aoPreSearchCols[4].sSearch = transactionDescription;

            if ($('#cardNumberSelectionDropDown').length > 0) {
                table.fnSettings().aoPreSearchCols[11].sSearch = $('#cardNumberSelectionDropDown').val();
            }

            // extended search form
            table.fnSettings().aoPreSearchCols[5].sSearch = $('#TransactionDateFrom').val() + "|" + $('#TransactionDateTo').val();
            table.fnSettings().aoPreSearchCols[6].sSearch = $('#PostingDateFrom').val() + "|" + $('#PostingDateTo').val();
            table.fnSettings().aoPreSearchCols[7].sSearch = $('#CycleDateFrom').val() + "|" + $('#CycleDateTo').val();

            table.fnSettings().aoPreSearchCols[8].sSearch = $('#AmountFrom').val() + "|" + $('#AmountTo').val();

            table.fnSettings().aoPreSearchCols[9].sSearch = $('#SelectedCurrency').val();
            table.fnSettings().aoPreSearchCols[10].sSearch = $('#SelectedTransactionType').val();
        }

        function sortColumnFn() {
            var asc = $(this).hasClass('sorting_asc');
            var sortCol = $(this).attr('data-sort');

            if (sortCol !== undefined) {
                var sortArray = [[sortCol, asc ? 'desc' : 'asc']];
                table.fnSettings().aaSorting = sortArray;
                table.fnSort(sortArray);
            }
        }

        table = $('#simpleTransactionsTable').dataTable($.extend(
            createTableOptions(filterOptions.simpleFilterUri, getTableFn, applyColumnStylesFn, preDrawCallbackFn,
                '#simpleTableContainer', getSpinnerContainerFn, isFilteringAllowedFn, mobileSorting), {
                aaSorting: filterOptions.simpleSorting,
                aoColumns: [
                    { bSortable: false, bVisible: false }, // Complete table row
                    { bSortable: true }, // Buchungsdatum
                    { bSortable: true }, // Transaktionsdatum 
                    { bSortable: true }, // Abrechnungsdatum
                    { bSortable: true }, // Rechnungstext
                    { bSortable: true }, // Betrag 
                    { bSortable: true }, // Kartenummer   
                    { bSortable: false }, // Umsatzrckfrage
                    { bSortable: false, bVisible: false }, // Accountnummer
                    { bSortable: false, bVisible: false }, // Manipulationsgebhr
                    { bSortable: false, bVisible: false }, // Fremdwhrungsbetrag
                    { bSortable: false, bVisible: false }, // Fremdwhrungskurs
                    { bSortable: false } // Column used as main column in collapsed state
                ],
                fnInitComplete: function() {
                    mobileSorting.InitializeMobileSorting(table);
                    addItemsToDatatable(table, '#simpleTableContainer', 'a[data-simple]');
                    applyColumnStylesFn();

                    table.on('page', function() {
                        paging = true;
                    });
                }
            }));

        return {
            table: table,
            search: function() {
                setSearchParametersFn();
                table.fnFilter('');
            }
        };
    }

    // -------------------------------------------------------------------------------------------------------------------
    // Functions used for the ENHANCED datatable
    // -------------------------------------------------------------------------------------------------------------------

    function setupEnhancedTable() {
        var mobileSorting = new MobileSorting('#enhancedTableContainer', '#enhancedMobileSortingContainer');
        var table;

        function applyColumnStylesFn() {
            $('#enhancedTableContainer th').off('click');
            $('#enhancedTableContainer th span').off('click');
            $('#enhancedTableContainer th span').on("click", sortColumnFn);

            applyTableSorting(table, 'span');
            DataTableHelper.TruncateCellContent();
            Common.InitializeTooltips();
            DataTableHelper.ApplyTooltips();
            FinancialTransactions.AddTransactionCallBackEventHandler();
            FinancialTransactions.CheckPreviouslyCheckedCheckboxes();
        }

        function getSpinnerContainerFn() { return spinnerContainer; }

        function getTableFn() { return table; }

        function isFilteringAllowedFn() { return filterOptions.isEnhanced; }

        function preDrawCallbackFn() {
            applyColumnStylesFn();
            setSearchParametersFn();
        }

        function setSearchParametersFn() {
            var transactionDescription = $('#descriptionInput').val();

            if (transactionDescription.length < 3) {
                transactionDescription = '';
            }

            table.fnSettings().aoPreSearchCols[3].sSearch = $('#quarterSelectionDropDown').val();
            table.fnSettings().aoPreSearchCols[4].sSearch = transactionDescription;

            if ($('#cardNumberSelectionDropDown').length > 0) {
                table.fnSettings().aoPreSearchCols[11].sSearch = $('#cardNumberSelectionDropDown').val();
            }

            // extended search form
            table.fnSettings().aoPreSearchCols[5].sSearch = $('#TransactionDateFrom').val() + "|" + $('#TransactionDateTo').val();
            table.fnSettings().aoPreSearchCols[6].sSearch = $('#PostingDateFrom').val() + "|" + $('#PostingDateTo').val();
            table.fnSettings().aoPreSearchCols[7].sSearch = $('#CycleDateFrom').val() + "|" + $('#CycleDateTo').val();

            table.fnSettings().aoPreSearchCols[8].sSearch = $('#AmountFrom').val() + "|" + $('#AmountTo').val();

            table.fnSettings().aoPreSearchCols[9].sSearch = $('#SelectedCurrency').val();
            table.fnSettings().aoPreSearchCols[10].sSearch = $('#SelectedTransactionType').val();
        }

        function sortColumnFn() {
            var asc = $(this).hasClass('sorting_asc');
            var sortCol = $(this).attr('data-sort');

            if (sortCol !== undefined) {
                var sortArray = [[sortCol, asc ? 'desc' : 'asc']];
                table.fnSettings().aaSorting = sortArray;
                table.fnSort(sortArray);
            }
        }

        table = $('#enhancedTransactionsTable').dataTable($.extend(
            createTableOptions(filterOptions.enhancedFilterUri, getTableFn, applyColumnStylesFn, preDrawCallbackFn,
                '#enhancedTableContainer', getSpinnerContainerFn, isFilteringAllowedFn, mobileSorting), {
                aaSorting: filterOptions.enhancedSorting,
                aoColumns: [
                    { bSortable: false, bVisible: false }, // Complete table row
                    { bSortable: true }, // Buchungsdatum
                    { bSortable: true, bVisible: false }, // Transaktionsdatum
                    { bSortable: true }, // Abrechnungsdatum
                    { bSortable: true, bVisible: false }, // Rechnungstext 
                    { bSortable: true }, // Kartennummer
                    { bSortable: true, bVisible: false }, // Manipulationsgebhr
                    { bSortable: true }, // Fremdwhrungskurs   
                    { bSortable: true, bVisible: false }, // Fremdwhrungsbetrag   
                    { bSortable: true }, // Betrag   
                    { bSortable: false, bVisible: false }, // Umsatzrckfrage
                    { bSortable: false, bVisible: false }, // Accountnummer
                    { bSortable: false } // Column used as main column in collapsed state
                ],
                fnInitComplete: function() {
                    mobileSorting.InitializeMobileSorting(table);
                    addItemsToDatatable(table, '#enhancedTableContainer', 'a[data-enhanced]');
                    applyColumnStylesFn();

                    table.on('page', function() {
                        paging = true;
                    });
                }
            }));

        return {
            table: table,
            search: function() {
                setSearchParametersFn();
                table.fnFilter('');
            }
        };
    }


    // -------------------------------------------------------------------------------------------------------------------
    // PUBLIC interface for financial transactions
    // -------------------------------------------------------------------------------------------------------------------

    return {
        InitializeFilter: function () {
            // live filtering
            $('#quarterSelectionDropDown').on("change", function() {
                applySearch();
                manuallySetPeriod();
            });

            $('#cardNumberSelectionDropDown').on("change", function () {
                applySearch();
            });

            $('#descriptionInput').on("keyup", function() {
                Common.Delay(function() {
                    applySearch();
                }, Common.DEBOUNCE_TIME_IN_MS);
            });

            $('#applySearch').on("click", function () {
                applySearch();
            });            
        },
        
        InitializeSearch: function (options) {
            openSearchIfRequired();
            setupDatePickers(options);
            resizeForm();

            $('input[data-type="number"]').autoNumeric('init');
            $('#clearSearchLink').on("click", clearSearchForm);

            closeDatePickers();

            if (options.triggerSearch) {
                applySearch();
            }
        },

        InitializeUndoAutomaticPeriodChange: function (options) {
            if (!options.triggerSearch) {
                // initial page call
                originalTimescale = $('#quarterSelectionDropDown').val();
            } else if (originalTimescale) {
                if (hasClearSearchLinkBeenClicked) {
                    originalTimescale = $('#quarterSelectionDropDown').val();
                    $('#undoPeriodChangeContainer').addClass('hidden');
                    hasClearSearchLinkBeenClicked = false;
                } else if (hasOriginalTimescaleAutomaticallyBeenSetToAll) {
                    $('#undoPeriodChangeContainer').removeClass('hidden');
                }
            }

            // auto correction of selected timescale
            intializePeriodChange('#TransactionDateFrom', '#transactionDateFromDatePicker');
            intializePeriodChange('#TransactionDateTo', '#transactionDateToDatePicker');
            intializePeriodChange('#PostingDateFrom', '#postingDateFromDatePicker');
            intializePeriodChange('#PostingDateTo', '#postingDateToDatePicker');
            intializePeriodChange('#CycleDateFrom', '#cycleDateFromDatePicker');
            intializePeriodChange('#CycleDateTo', '#cycleDateToDatePicker');
            
            $('#undoPeriodChangeLink').on("click", function () {
                resetOriginalPeriod();
            });

            $('#clearSearchLink').on("click", function () {
                resetOriginalPeriod();
                hasClearSearchLinkBeenClicked = true;
            });
        },

        SetSelectedTransactions: function(selectedTrx) {
            selectedTransactions = selectedTrx;
        },

        CheckPreviouslyCheckedCheckboxes: function() {
            $("input:checkbox[name=cbTransactionCallBack]").each(function() {
                if ($.inArray($(this).val(), selectedTransactions) !== -1) {
                    $(this).prop("checked", true);
                }
            });
        },

        AddTransactionCallBackEventHandler: function() {
            var checkboxes = $("input:checkbox[name=cbTransactionCallBack]");
            var links = $('[data-id="callbackLink"]');

            checkboxes.off('click');
            checkboxes.on("click", function() {
                var transaction = $(this).val();
                if ($(this).is(":checked")) {
                    if ($.inArray(transaction, selectedTransactions) === -1) {
                        selectedTransactions.push(transaction);
                    }
                } else {
                    selectedTransactions.splice($.inArray(transaction, selectedTransactions), 1);
                }

                $('[data-id="callbackError"]').addClass('hidden');
            });

            links.off('click');
            links.on("click", function(e) {
                var transaction = $(this).attr('data-transaction');
                selectedTransactions = [transaction];
                handleTransactionCallBack(e);
            });
        },

        Initialize: function(options) {
            filterOptions = options;

            if (filterOptions.simpleSorting[0][0] < 0) {
                filterOptions.simpleSorting = [[]];
            }

            if (filterOptions.enhancedSorting[0][0] < 0) {
                filterOptions.enhancedSorting = [[]];
            }

            DataTablePlugins.Init();
            setupResizeBehavior();

            simpleTableInfo = setupSimpleTable();
            enhancedTableInfo = setupEnhancedTable();

            $(window).on("resize", function() {
                Common.InitializeTooltips();
            });
        },

		InitializeTransactionCallback: function() {
			Common.PrepareForm('#transactionCallBackForm', '#submitTransactionCallBackButton');
		}
    };
})();;
var Home = (function () {
    function showBadge(clickableBadgeSelector, teaserUriSelector) {
        $(".async-badge").removeClass('hidden');

        if (clickableBadgeSelector) {
            $(clickableBadgeSelector).on("click", function () {
                var uri = $(teaserUriSelector).attr('href');
                window.location = decodeURIComponent(uri);
            });
        }
    }

    return {
        CreateAjaxOptionsForHomeTeaser: function (contentId, loadTeaserContentUri, isMobileContext, teaserContainerSelector, teaserUriSelector, clickableBadgeSelector) {
            var options = {
                formId: contentId,
                containerId: contentId,
                type: Common.FORM_METHOD_GET,
                url: loadTeaserContentUri
            };

            if (isMobileContext) {
                options.callback = function() {
                    // move link to container and hide button in teaser
                    $(teaserUriSelector).addClass('hidden');

                    $(teaserContainerSelector).on("click", function() {
                        var uri = $(teaserUriSelector).attr('href');
                        window.location = decodeURIComponent(uri);
                    });

                    showBadge(clickableBadgeSelector, teaserUriSelector);
                }
            } else {
                options.callback = function() {
                    showBadge(clickableBadgeSelector, teaserUriSelector);
                }
            }

            return options;
        },

        PrepareTeaserTables: function() {
            $('[data-toggle="tooltip"]').each(function() {
                $(this).tooltip('destroy');
                if (!Common.IsPhoneMode()) {
                    if (Common.IsEllipsisActive(this)) {
                        $(this).tooltip();
                    } else {
                        $(this).on('show.bs.tooltip', function() {
                            return false;
                        });
                    }
                }
            });
        },

        InitializeEventHandler: function() {
            $(window).on("resize", Home.PrepareTeaserTables);
        },
        
        InitializeOptionalStartPage: function (isConfirmationRequired, isConfirmationDateExceeded, submitFormSelector) {
            var checkbox = $('#confirmationCheckbox');
            var errorContainer = $('#validationErrorContainer');
            var errorText = $('#confirmationError');

            function displayError(display) {
                if (display) {
                    errorContainer.removeClass('hidden');
                    errorText.removeClass('hidden');
                } else {
                    errorContainer.addClass('hidden');
                    errorText.addClass('hidden');
                }
            }
            
            // checkbox is optional
            if (checkbox.length !== 0) {
                checkbox.on("click", function () {
                    if ($(this).is(':checked')) {
                        displayError(false);
                    }
                });
            }

            // prevent duplicate form submit caused by double clicks
            var clicked = false;

            $('#continueButton').on("click", function (e) {
                if (clicked) {
                    console.log("double click ignored");
                    return false;
                }

                if (checkbox.length === 0 || $(checkbox).is(':checked') || !isConfirmationRequired && !isConfirmationDateExceeded) {
                    $(submitFormSelector).submit();
                } else {
                    displayError(true);

                    // make button clickable again, such that form can be submitted when checked
                    clicked = false;
                }

                e.preventDefault();

                // ensure button will be clickable again after a specific time
                clicked = true;
                Common.Delay(function () {
                    clicked = false;
                }, Common.DOUBLECLICK_TIME_IN_MS);
            });
        },

        InitializeDynamicTeaser: function(options) {
            var defaultActiveColor = "black";
            var defaultPassiveColor = "black";

            var activeDotsSelector = ".slick-dots li.slick-active button:before";
            var passiveDotsSelector = ".slick-dots li button:before";

            $('<style slick>' +
                passiveDotsSelector +
                ' {color: ' +
                defaultPassiveColor +
                '} ' +
                activeDotsSelector +
                ' {color: ' +
                defaultActiveColor +
                '}</style>').appendTo('head');

            $("#dynamicTeaserContainerContent").slick({
                    autoplay: true,
                    adaptiveHeight: true,
                    autoplaySpeed: options.autoplaySpeed,
                    dots: true,
                    infinite: true,
                    initialSlide: options.initialSlide,
                    speed: options.speed,
                    fade: options.fadePortrait,
                    responsive: [
                            {
                                breakpoint: 741, // ~ less: @screen-xs
                                settings: {
                                        fade: options.fadeLandscape
                                    }
                            }
                        ]
                });

            $("#dynamicTeaserContainerContent").on('afterChange', function(event, slick, direction) {
                var activeColorOfCurrentTeaser = options.dotsColor[direction] && options.dotsColor[direction].ActiveHexColor || defaultActiveColor;
                var passiveColorOfCurrentTeaser = options.dotsColor[direction] && options.dotsColor[direction].PassiveHexColor || defaultPassiveColor;

                $("style[slick]").text(passiveDotsSelector +
                    ' {color: ' +
                    passiveColorOfCurrentTeaser +
                    '} ' +
                    activeDotsSelector +
                    ' {color: ' +
                    activeColorOfCurrentTeaser +
                    '}');
            });
        },

        Dismiss3DsStatusBoxContainer: function(options) {
            $('#dismiss3DsStatusBox').on('click', function () {
                $.ajax({ url: options.url, type: 'GET', dataType: 'text', data: options.data });
            });
        },

        Dismiss3DsCardsNotRegisteredInfoBoxContainer: function(options) {
            $('#dismiss3DsCardsNotRegisteredInfoBox').on('click', function () {
                $.ajax({ url: options.url, type: 'GET', dataType: 'text', data: options.data });
            });
        },

        Dismiss3DsSwitchToNewDeviceInfoBoxContainer: function(options) {
            $('#dismiss3DsSwitchToNewDeviceInfoBox').on('click', function () {
                $.ajax({ url: options.url, type: 'GET', dataType: 'text', data: options.data });
            });
        }
    };
})();;
var Invoice = (function() {
    var responsiveHelper = null;
    var table = null;
    var mobileSorting = new MobileSorting();
    var paging;

    function applyColumnStyles() {
        DataTableHelper.TruncateCellContent(1);
        DataTableHelper.ApplyTooltips();
    }

    function addInvoiceArchiveButton(show) {
        var html = $('#invoiceArchiveTemplate').html();
        var dataTableContainer = $('.spinner-container');
        
        dataTableContainer.empty();
        if (show) {
            dataTableContainer.append(html);
        }
    }

    function addInvoiceButton() {
        var html = $('#invoiceTemplate').html();
        var dataTableContainer = $('.spinner-container');
        dataTableContainer.append(html);
    }

    var setupDataTable = function (isInvoiceArchiveAvailableForPublic, ajaxSource, invoiceLinkTextAfterDownload, isMobileContext, isArchiveContext) {

        var aoColumnsSettings = isArchiveContext ? [
            { bSortable: false, bVisible: false }, // Complete table row
            { bSortable: true }, // Datum
            { bSortable: true }, // Rechnungsnummer 
            { bSortable: true }, // PayLife Kundenkonto
            { bSortable: false }, // Abrechnung kontrollieren   
            { bSortable: false } // Column used as main column in collapsed state
        ] : [
            { bSortable: false, bVisible: false }, // Complete table row
            { bSortable: true }, // Datum
            { bSortable: true }, // Rechnungsnummer 
            { bSortable: true }, // PayLife Kundenkonto
            { bSortable: true }, // Whrung/ Saldo  
            { bSortable: false }, // Abrechnung kontrollieren   
            { bSortable: false } // Column used as main column in collapsed state
        ];

        table = $('#invoiceTable').dataTable({
            sDom: Common.DataTable.TABLE_DOM,
            iDisplayLength: Common.DISPLAY_LENGTH,
            bLengthChange: false,
            bStateSave: false,
            bFilter: true,
            bProcessing: false,
            bAutoWidth: false,
            sPaginationType: 'full_numbers',
            bServerSide: true,
            sAjaxSource: ajaxSource,
            aaSorting: [[1, 'desc']],
            aoColumns: aoColumnsSettings,
            oLanguage: {
                "sUrl": Common.DataTable.LOCALIZATION
            },
            fnPreDrawCallback: function () {
                if (!responsiveHelper) {
                    responsiveHelper = new ResponsiveDatatablesHelper(table, Common.DataTable.BREAKPOINT_DEFINITION);
                    responsiveHelper.respondCallback = applyColumnStyles;
                }

                if (!isArchiveContext) {
                    // when last page of invoices is open, display archive button
                    var pageInfo = this.fnPagingInfo();
                    var onLastPage = (pageInfo.iTotalPages - pageInfo.iPage) === 1;
                    if (onLastPage) {
                        addInvoiceArchiveButton(isInvoiceArchiveAvailableForPublic);
                    } else {
                        addInvoiceArchiveButton();
                    }
                }
            },
            fnRowCallback: function (nRow) {
                responsiveHelper.createExpandIcon(nRow);
            },
            fnDrawCallback: function () {
                responsiveHelper.expandAll();
                responsiveHelper.respond();
                Common.TableResizer(table);
                mobileSorting.UpdateMobileSorting();
                table.show();

                // download pdf - submit the form, which is generated per row when user clicks on the link.
                $("a", $('#invoiceTable')).on("click", function (e) {
                    // since we do not know if the download worked, we just guess it worked and
                    // remove the css class, because we have 2 different links, depending if the user already downloaded the file or not.
                    // and we do not want to reload the whole page (because of paging etc).
                    $(this).removeClass("btn btn-primary btn-download-invoice").text(invoiceLinkTextAfterDownload);
                    $(this).closest('tr').removeClass('unread');
                });
            },
            fnServerParams: function (data) {
                if (isArchiveContext) {
                    data.push({
                        name: "sArchiveInvoiceIndexDate",
                        value: $('#archiveInvoiceIndexDate').data("idx-date")
                    });
                }
            },
            fnServerData: function(url, data, callback) {
                $('#invoiceTableContainer').removeClass('hidden');

                Common.AjaxWithErrorHandling(url, data, function(result) {
                    if (result.RedirectTo) {
                        location.href = result.RedirectTo;
                    }

                    if (isArchiveContext) {
                        $('#archiveInvoiceIndexDate').data("idx-date", result.oAdditionalData);
                    }

                    callback(result);

                    if (paging) {
                        paging = false;
                        $('html, body').animate({
                            scrollTop: $('#invoiceTableContainer').offset().top
                        }, 500);
                    }
                }, '.spinner-container');
            },
            fnCreatedRow: function(nRow, aData) {
                Common.PostprocessTableRow(table, nRow, aData[0]);
            },
            fnInitComplete: function() {
                $('#invoiceTable th div').on("click", function(e) {
                    $(this).closest('th').trigger("click");
                    e.stopPropagation();
                });
                mobileSorting.InitializeMobileSorting(table);
                applyColumnStyles();
                table.on('page', function() {
                    paging = true;
                });
                if (isArchiveContext) {
                    addInvoiceButton();
                }
            }
        });
    };

    return {
        Setup: function (isInvoiceArchiveAvailableForPublic, ajaxSource, invoiceLinkTextAfterDownload, isMobileContext, isArchiveContext)  {
            Common.InitializeTooltips();
            $(window).on("resize", function() {
                Common.InitializeTooltips();
            });
            DataTablePlugins.Init();

            // init default datatable
            setupDataTable(isInvoiceArchiveAvailableForPublic, ajaxSource, invoiceLinkTextAfterDownload, isMobileContext, isArchiveContext);
        },

        Search: function() {
            table.fnFilter(''); // submits the form
        }
    };
})();;
var Messaging = (function () {
    var responsiveHelper = null;
    var table = null;
    var mobileSorting = new MobileSorting();
    var paging = false;

    function expandTableCells() {
        Messaging.InstallEventHandlers();
        DataTableHelper.SetColumnValueWidths();
    }

    function preventXssInMessageBody(validateXssButtonSelector, submitMessageButtonSelector, formSelector, bodySelector, errorMessageSelector) {
        $(submitMessageButtonSelector).hide();
        $(validateXssButtonSelector).on("click", function () {
            var messageBody = $(formSelector).find(bodySelector).val();

            var reXSS = /.*<[^\s]/;
            var validateBodyXSS = messageBody.match(reXSS);

            $("[data-cloak]").on("click", function () {
                $("." + $(this).attr("data-cloak")).hide();
            });

            if (validateBodyXSS !== null) {
                $(errorMessageSelector).show();
            } else {
                $(errorMessageSelector).hide();
                $(submitMessageButtonSelector).trigger("click");
            }
        });
    }

    return {
        SetupDataTable: function(ajaxSource) {
            table = $('#messagesTable').dataTable({
                    sDom: Common.DataTable.TABLE_DOM,
                    iDisplayLength: Common.DISPLAY_LENGTH,
                    bLengthChange: false,
                    bStateSave: false,
                    bFilter: true,
                    bSort: true,
                    bProcessing: false,
                    bAutoWidth: false,
                    sPaginationType: 'full_numbers',
                    bServerSide: true,
                    sAjaxSource: ajaxSource,
                    aaSorting: [[4, 'desc']],
                    aoColumns: [
                            { bSortable: false, bVisible: false }, // Complete table row
                            { bSortable: true }, // BusinessCaseNumber
                            { bSortable: true }, // Subject
                            { bSortable: true }, // MessageType
                            { bSortable: true }, // SubmissionDateTime
                            { bSortable: false } // Column used as main column in collapsed state
                        ],
                    oLanguage: {
                            "sUrl": Common.DataTable.LOCALIZATION
                        },
                    fnPreDrawCallback: function() {
                        if (!responsiveHelper) {
                            responsiveHelper = new ResponsiveDatatablesHelper(table, Common.DataTable.BREAKPOINT_DEFINITION, expandTableCells);
                        }
                    },
                    fnRowCallback: function(nRow) {
                        responsiveHelper.createExpandIcon(nRow);
                    },
                    fnDrawCallback: function() {
                        responsiveHelper.expandAll();
                        responsiveHelper.respond();
                        Common.TableResizer(table);
                        Messaging.InstallEventHandlers();
                        mobileSorting.UpdateMobileSorting();
                        table.show();
                    },
                    fnServerData: function(url, data, callback) {
                        $('#messageTableContainer').removeClass('hidden');

                        Common.AjaxWithErrorHandling(url, data, function(result) {
                            if (result.RedirectTo) {
                                location.href = result.RedirectTo;
                            }

                            callback(result);

                            if (paging) {
                                paging = false;
                                $('html, body').animate({
                                        scrollTop: $('#messageTableContainer').offset().top
                                    }, 500);
                            }
                        }, '.spinner-container');
                    },
                    fnCreatedRow: function(nRow, aData) {
                        Common.PostprocessTableRow(table, nRow, aData[0]);
                    },
                    fnInitComplete: function() {
                        $('#messagesTable th div').on("click", function(e) {
                            $(this).closest('th').trigger("click");
                            e.stopPropagation();
                        });
                        mobileSorting.InitializeMobileSorting(table);
                        table.on('page', function() {
                            paging = true;
                        });
                    }
                });
        },

        InstallEventHandlers: function() {
            $('#messagesTable tr').off();
            $('#messagesTable tr').on("click", function(e) {
                var linkUrl = $(this).attr('data-href');

                if (linkUrl) {
                    location.href = linkUrl;
                }
                e.preventDefault();
            });
        },

        PrepareAnswerMessage: function () {
            preventXssInMessageBody('#submitMessageValidateXSSButton', '#submitMessageButton', '#answerMessageForm', '#Body', '#validateXSS');

            Common.PrepareForm('#answerMessageForm', '#submitMessageButton', null, { noFocus: true });
        },

        PrepareMessageOverview: function(filterMessagesUri) {
            Common.InitializeTooltips();
            $(window).on("resize", function() {
                Common.InitializeTooltips();
            });

            DataTablePlugins.Init();
            Messaging.SetupDataTable(filterMessagesUri);
        },

        PrepareNewMessage: function (transactionQuestionSubject, frameChangeQuestionSubject) {
            preventXssInMessageBody('#submitMessageValidateXSSButton', '#submitMessageButton', '#newMessageForm', '#Body', '#validateXSS');

            var modifyClassAttributes = function() {
                $('#messageFormContainer').removeClass('hidden');
                $('#submitMessageButton').removeClass('hidden');
                $('#transactionLinkContainer').addClass('hidden');
            };

            function updateFormForSubject() {
                if ($('#subjectDropDown').val() === transactionQuestionSubject) {
                    $('#messageFormContainer').toggleClass('hidden');
                    $('#submitMessageButton').toggleClass('hidden');
                    $('#transactionLinkContainer').toggleClass('hidden');
                    $('#creditLine').addClass('hidden');
                } else if ($('#subjectDropDown').val() === frameChangeQuestionSubject) {
                    modifyClassAttributes();
                    $('#creditLine').removeClass('hidden');
                } else {
                    modifyClassAttributes();
                    $('#creditLine').addClass('hidden');
                }
            }

            $('#subjectDropDown').on("change", updateFormForSubject);
            updateFormForSubject();

            $('#subjectCategoryDropDown').on("change", function() {
                if ($('#subjectCategoryDropDown').val() === 'RELATED_TO_PREPAID_CARDS') {
                    $('#subjectRelatedToPrepaidCardsContainer').removeClass('hidden');
                    $('#subjectRelatedToCreditCardsContainer').addClass('hidden');
                } else {
                    $('#subjectRelatedToCreditCardsContainer').removeClass('hidden');
                    $('#subjectRelatedToPrepaidCardsContainer').addClass('hidden');
                }
            });

            Common.PrepareForm('#newMessageForm', '#submitMessageButton', null, { noFocus: true });
        },

        PrepareViewMessage: function() {
            $('#messageBody').html(
                    Common.ParseTextLinks(
                        $('#messageBody', false).text()
                    )
                );
        }
    };
})();;
var MobileDevice = (function() {
    var responsiveHelper = null;
    var table = null;
    var mobileSorting = new MobileSorting();
    var paging = false;
    var options = null;
    var deactivateFingerprintUri = null;

    function installEventHandlers() {
        // activate fingerprint
        var activateFingerprintSelector = $('[data-id="activateFingerprintLink"]');

        activateFingerprintSelector.on("click", function(e) {
            var form = $(this).closest("form");
            form.submit();
        });

        // deactivate fingerprint
        var deactivateFingerprintPopoverSelector = $('a[data-id="deactivateFingerprintLink"]');
        var confirmDeactivateFingerprintLinkSelector = 'button[data-id="deactivateFingerprintButton"]';
        var cancelDeactivateFingerprintLinkSelector = 'a[data-id="closeDeactivateFingerprintPopoverButton"]';

        deactivateFingerprintPopoverSelector.on("click", function(e) {
            e.preventDefault();
        });

        $(deactivateFingerprintPopoverSelector).popover({
            html: true,
            content: function() {
                return $("#deactivateFingerprintForDevicePopoverContent").html();
            },
            title: function() {
                return $("#deactivateFingerprintForDevicePopoverTitle").html();
            },
            trigger: "click",
            container: "body",
            toggle: "popover",
            placement: "bottom"
        });

        $(deactivateFingerprintPopoverSelector).on("shown.bs.popover",
            function() {
                // closest ravels up the DOM tree beginning with the deactivation link until it finds a match for the supplied form selector
                var deactivateFingerprintForm = $(this).closest("form");
                var submitDeactivateFingerprintFn = function () {
                    deactivateFingerprintForm.submit();
                };

                Global.SetCurrentPopover(deactivateFingerprintPopoverSelector);
                Common.InitializeConfirmationBoxHandlers(confirmDeactivateFingerprintLinkSelector,
                    cancelDeactivateFingerprintLinkSelector,
                    deactivateFingerprintPopoverSelector,
                    false,
                    submitDeactivateFingerprintFn);
            });

        // delete device
        var deleteDevicePopoverSelector = $('a[data-id="deleteMobileDeviceLink"]');
        var confirmLinkSelector = 'button[data-id="deleteDeviceButton"]';
        var cancelLinkSelector = 'a[data-id="closeDeleteDevicePopoverButton"]';

        deleteDevicePopoverSelector.on("click", function(e) {
            e.preventDefault();
        });

        $(deleteDevicePopoverSelector).popover({
            html: true,
            content: function() {
                var isDeviceUsedFor3dSecureApproval = $(this).attr(options.isUsedFor3dSecureDataAttr);

                if (JSON.parse(isDeviceUsedFor3dSecureApproval)) {
                    return $("#deleteDeviceRegisteredFor3dSecurePopoverContent").html();
                } else {
                    return $("#deleteDeviceNotRegisteredFor3dSecurePopoverContent").html();
                }
            },
            title: function() {
                return $("#deleteDevicePopoverTitle").html();
            },
            trigger: "click",
            container: "body",
            toggle: "popover",
            placement: "bottom"
        });

        $(deleteDevicePopoverSelector).on("shown.bs.popover",
            function() {
                // closest ravels up the DOM tree beginning with the delete link until it finds a match for the supplied form selector
                var form = $(this).closest("form");
                var submitFn = function() {
                    form.submit();
                };
                var cancelFn = function () {
                    Tracker.ExecuteTrackEvents(options.eventTrackingUri, { eventId: options.trackingEvent });
                };
                Global.SetCurrentPopover(deleteDevicePopoverSelector);
                Common.InitializeConfirmationBoxHandlers(confirmLinkSelector,
                    cancelLinkSelector,
                    deleteDevicePopoverSelector,
                    false,
                    submitFn,
                    cancelFn
                );
            });
    }

    function expandTableCells() {
        DataTableHelper.SetColumnValueWidths();
        reorderVariables("[data-device-name]", "div.replace-with-device-name");
        reorderVariables("[data-fingerprint-status]", "div.replace-with-fingerprint-status");
        installEventHandlers();
    }

    function addDeviceInfoButton() {
        var html = $("#deviceInfoTemplate").html();
        var datatableContainer = $(".spinner-container");
        datatableContainer.append(html);
    }

    function reorderVariables(sourceSelector, targetSelector) {
        var deviceNameElements = $(sourceSelector);
        if (deviceNameElements && deviceNameElements.length > 0) {
            for (var i = 0; i < deviceNameElements.length; i++) {
                var deviceNameElement = deviceNameElements[i];
                var deviceName = deviceNameElement.dataset.deviceName;

                var parent = $(deviceNameElement).closest("ul");
                var label = $(parent).find(targetSelector);
                $(label).text(deviceName);
            }
        }
    }

    function setupDataTable(ajaxSource) {
        table = $("#deviceTable").dataTable({
            sDom:
                "<'row clearfix'<'col-xs-12 visible-xs dataTables_sort'>>t<'row clearfix'<'col-md-4'i><'col-md-4'p><'col-md-4'<'spinner-container'>>'>",
            iDisplayLength: Common.DISPLAY_LENGTH,
            bLengthChange: false,
            bStateSave: false,
            bFilter: true,
            bSort: true,
            bProcessing: false,
            bAutoWidth: false,
            sPaginationType: "full_numbers",
            bServerSide: true,
            sAjaxSource: ajaxSource,
            aaSorting: [[1, "asc"]],
            aoColumns: options.isMobileAppContext
                    ? [
                        { bSortable: false, bVisible: false }, // Complete table row
                        { bSortable: true }, // Desktop + Tablet: Name
                        { bSortable: false }, // Mobile: Name + delete device link
                        { bSortable: true }, // Registration date
                        { bSortable: true }, // Last login date
                        { bSortable: false },
                        { bSortable: false }, // Delete device link
                        { bSortable: false } // Column used as main column in collapsed state
                    ]
                    : [
                        { bSortable: false, bVisible: false }, // Complete table row
                        { bSortable: true }, // Desktop + Tablet: Name
                        { bSortable: false }, // Mobile: Name + delete device link
                        { bSortable: true }, // Registration date
                        { bSortable: true }, // Last login date
                        { bSortable: false },
                        { bSortable: false },
                        { bSortable: false }, // Delete device link
                        { bSortable: false } // Column used as main column in collapsed state
                    ],
            oLanguage: {
                "sUrl": Common.DataTable.LOCALIZATION
            },
            fnPreDrawCallback: function() {
                if (!responsiveHelper) {
                    responsiveHelper =
                        new ResponsiveDatatablesHelper(table, Common.DataTable.BREAKPOINT_DEFINITION, expandTableCells);
                }
            },
            fnRowCallback: function(nRow) {
                responsiveHelper.createExpandIcon(nRow);
            },
            fnDrawCallback: function () {
                responsiveHelper.expandAll();
                responsiveHelper.respond();
                Common.TableResizer(table);
                installEventHandlers();
                mobileSorting.UpdateMobileSorting();
                table.show();
            },
            fnServerData: function(url, data, callback) {
                $("#deviceTableContainer").removeClass("hidden");

                Common.AjaxWithErrorHandling(url,
                    data,
                    function(result) {
                        if (result.RedirectTo) {
                            location.href = result.RedirectTo;
                        }

                        callback(result);

                        if (paging) {
                            paging = false;
                            $("html, body").animate({
                                    scrollTop: $("#deviceTableContainer").offset().top
                                },
                                500);
                        }
                    },
                    ".spinner-container",
                    { hideElements: true });
            },
            fnCreatedRow: function(nRow, aData) {
                Common.PostprocessTableRow(table, nRow, aData[0]);
            },
            fnInitComplete: function() {
                $("#deviceTable th div").on("click", function(e) {
                    $(this).closest("th").trigger("click");
                    e.stopPropagation();
                });
                mobileSorting.InitializeMobileSorting(table);
                table.on("page",
                    function() {
                        paging = true;
                    });
                addDeviceInfoButton();
            }
        });
    }

    return {
        PrepareDeviceOverview: function (filterDevicesUri, settings) {
            options = settings;
            deactivateFingerprintUri = options.deactivateFingerprintUri;

            Common.InitializeTooltips();
            $(window).on("resize", function() {
                Common.InitializeTooltips();
            });

            DataTablePlugins.Init();
            setupDataTable(filterDevicesUri);
        },

        InitializeEditAppCode: function(options) {
            var control = $(options.control);
            var field = $(options.field);

            if (control.attr("data-show") === "true") {
                control.text(options.hideText);
            } else {
                control.text(options.showText);
                field.val(options.initValue);
            }

            Common.PrepareForm("#editAppCodeForm", "#submitButton");
        }
    };
})();;
var MobileWallet = function () {
    var missingConfirmationIds = [];
    var mobileNumberVerificationAlreadyPrepared = false;

    function showConfirmationError(errorMessageId, show) {
        if (show) {
            $(errorMessageId).show();
        } else {
            $(errorMessageId).hide();
        }
    }

    function showConfirmationProcessError(errorConfirmationMissingSelector, show) {
        if (show) {
            $(errorConfirmationMissingSelector).removeClass("hidden");
        } else {
            $(errorConfirmationMissingSelector).addClass("hidden");
        }
    }


    function prepareCheckbox(checkboxId, errorMessageId, errorConfirmationMissingSelector) {
        $(checkboxId).on("click", function () {
            if (isChecked(checkboxId)) {
                // checkbox error
                showConfirmationError(errorMessageId, false);

                // process error
                if (containsError(checkboxId)) {
                    removeError(checkboxId);
                }

                if (!hasErrors()) {
                    showConfirmationProcessError(errorConfirmationMissingSelector, false);
                }
            }
        });
    }

    function prepareCardChange(cardSelectionId, selectedCardId, availableCards, creditLineAmountTextTemplate, creditLineAmountId) {
        $(cardSelectionId).on('change', function () {
            $(selectedCardId).val(this.value);
            setSelectedCard(selectedCardId, this.value);
            displayLimitText(selectedCardId, availableCards, creditLineAmountTextTemplate, creditLineAmountId);
        });
    }

    function displayLimitText(selectedCardId, availableCards, creditLineAmountTextTemplate, creditLineAmountId) {
        var currentlySelectedCardId = $(selectedCardId).val();

        for (var i = 0; i < availableCards.length; i++) {
            if (availableCards[i].cardId === currentlySelectedCardId) {
                var productName = availableCards[i].productName;
                var limit = availableCards[i].limit;
                var limitText = StringUtil.Format(creditLineAmountTextTemplate, productName, limit);

                $(creditLineAmountId).html(limitText);
                break;
            }
        }
    }

    function setSelectedCard(selectedCardId, cardId) {
        $(selectedCardId).val(cardId);
    }

    function isChecked(checkboxId) {
        return $(checkboxId).is(':checked');
    }

    function registerError(errorId) {
        if (!containsError(errorId)) {
            missingConfirmationIds.push(errorId);
        }
    }

    function containsError(errorId) {
        return missingConfirmationIds.indexOf(errorId) >= 0;
    }

    function removeError(errorId) {
        if (!containsError(errorId)) {
            return;
        }

        var errorPosition = missingConfirmationIds.indexOf(errorId);
        missingConfirmationIds.splice(errorPosition, 1);
    }

    function hasErrors() {
        return missingConfirmationIds.length > 0;
    }

    var prepareMobileNumberConfirmationPopover = function (options) {
        $(options.submitButtonId).attr("disabled", true);

        if (mobileNumberVerificationAlreadyPrepared) {
            return;
        }

        $(options.submitButtonId).popover({
            html: true,
            content: function () {
                return $(options.popoverMobileNumberContentSelector).html();
            },
            title: function () {
                return $(options.popoverMobileNumberTitleSelector).html();
            },
            trigger: "manual",
            container: "body",
            toggle: "popover",
            placement: "top"
        });
    };

    var showMobileNumberConfirmationPopover = function (options) {
        $(options.submitButtonId).popover('show');
    };

    var hideMobileNumberConfirmationPopover = function (options) {
        $(options.submitButtonId).popover('hide');
        $(options.submitButtonId).attr("disabled", false);
    };

    var checkForErrorsForStepOne = function (e, successFunction) {
        e.preventDefault();

        if (!isChecked(options.confirmationCheckboxId)) {
            showConfirmationError(options.errorMessageId, true);
            registerError(options.confirmationCheckboxId);
        }

        if (hasErrors()) {
            showConfirmationProcessError(options.errorConfirmationMissingSelector, true);
            return;
        }

        if (successFunction) {
            successFunction();
        }
    }

    return {
        InitializeOrderWizardStepOne: function () {
            showConfirmationError(options.errorMessageId, false);
            prepareCheckbox(options.confirmationCheckboxId, options.errorMessageId, options.errorConfirmationMissingSelector);

            $(options.submitButtonId).on("click", function (e) {
                checkForErrorsForStepOne(e, function () {
                    prepareMobileNumberConfirmationPopover(options);
                    showMobileNumberConfirmationPopover(options);
                });
            });

            $(document.body).on("click", options.confirmNumberSelector, function (e) {
                hideMobileNumberConfirmationPopover(options);
                checkForErrorsForStepOne(e, function () {
                    $(options.submitFormId).submit();
                });
            });
        },

        InitializeOrderWizardStepTwo: function (options) {
            setSelectedCard(options.selectedCardId, options.availableCards[0].cardId);
            displayLimitText(options.selectedCardId, options.availableCards, options.creditLineAmountTextTemplate, options.creditLineAmountId);

            showConfirmationError(options.conditionsErrorMessageId, false);
            prepareCheckbox(options.conditionsCheckboxId, options.conditionsErrorMessageId, options.errorConfirmationMissingSelector);

            showConfirmationError(options.confirmationErrorMessageId, false);
            prepareCheckbox(options.confirmationCheckboxId, options.confirmationErrorMessageId, options.errorConfirmationMissingSelector);

            if (options.availableCards.length > 1) {
                prepareCardChange(options.cardSelectionId, options.selectedCardId, options.availableCards, options.creditLineAmountTextTemplate, options.creditLineAmountId);
            }

            // prevent duplicate form submit caused by double clicks
            var clicked = false;

            $(options.submitButtonId).on("click", function (e) {
                if (clicked) {
                    console.log("double click ignored");
                    return;
                }

                if (!isChecked(options.conditionsCheckboxId)) {
                    showConfirmationError(options.conditionsErrorMessageId, true);
                    registerError(options.conditionsCheckboxId);
                }

                if (!isChecked(options.confirmationCheckboxId)) {
                    showConfirmationError(options.confirmationErrorMessageId, true);
                    registerError(options.confirmationCheckboxId);
                }

                if (!hasErrors()) {
                    $(options.submitFormId).submit();
                } else {
                    showConfirmationProcessError(options.errorConfirmationMissingSelector, true);

                    // make button clickable again, such that form can be submitted when checked
                    clicked = false;
                }

                e.preventDefault();

                // ensure button will be clickable again after a specific time
                clicked = true;
                Common.Delay(function () {
                    clicked = false;
                }, Common.DOUBLECLICK_TIME_IN_MS);
            });
        }
    };
}();;
var MyControl = (function () {
    var validFromDp, validToDp;

    function setupDatePickers(options) {
        var form = $('#submitForm');

        var datePickerOptions = {
            language: options.cultureCode,
            autoclose: true,
            forceParse: false,
            todayHighlight: true,
            todayBtn: "linked",
            clearBtn: true,
            startDate: 'd',
            minDate: 0,
            endDate: new Date($('#ValidToMaxDate').val()),
            keyboardNavigation: false,
            format: options.dateFormat.replace(/M/g, "m")
        };

        validFromDp = $('#validFromDatePicker').datepicker(datePickerOptions);
        validToDp = $('#validToDatePicker').datepicker(datePickerOptions);

        if (options.isMobileAppContext) {
            datePickerOptions["orientation"] = "right";
        }

        $('[data-type="date"]').off();
        $('[data-type="date"]').on("click", function (e) {
            e.preventDefault();
        });

        $('[data-type="date"]').closest(".date").datepicker().on('changeDate', function () {
            var input = $(this).find('input');

            if (!input.is(":focus")) {
                form.validate().element(input);
            }
        });
    }

    function closeDatePickers() {
        var pickers = [validFromDp, validToDp];

        for (var index in pickers) {
            if (pickers.hasOwnProperty(index)) {
                pickers[index].datepicker('hide');
            }
        }
    }

    function displayMyControlStatus(serviceSelector, status, cardId, disableLinkForSupport) {
        var service = $(serviceSelector);
        service.find('[data-my-control="loading"]').attr('hidden', true);

        var serviceStatus = service.find('[data-my-control="status"]');

        if (status.Icon.toLowerCase() === "activated") {
            serviceStatus.find('.icon-enabled').removeAttr('hidden');
        } else if (status.Icon.toLowerCase() === "deactivated") {
            serviceStatus.find('.icon-disabled').removeAttr('hidden');
        }

        serviceStatus.find('.status').text(status.StatusText);

        serviceStatus.removeAttr('hidden');

        var serviceAction = service.find('[data-my-control="action"]');
        serviceAction.find('.action.link').text(status.ActionText);
        serviceAction.find('.action.link').attr('href', status.ActionLink + "?CardId=" + cardId);
        if(disableLinkForSupport) {
            // disable link only when action is triggered immediately, i.e. no SCA
            serviceAction.find('.action.link').attr('class', 'disableSupport ' + serviceAction.find('.action.link').attr('class'));
        }
        serviceAction.find('.action.link').removeAttr('hidden');
        serviceAction.removeAttr('hidden');

        Common.PreventMultipleClicksOnElement(serviceAction.find('.action.link'));
    }

    function displayTemporaryCardLockDescription(status) {
        var cardActiveOrLockedText = "";

        $('[data-id="spinner-card-status"]').trigger("detailsCardStatus", [status.Icon.toLowerCase()]);

        if (status.Icon.toLowerCase() === "activated") {
            cardActiveOrLockedText = $('[data-id="popoverInfoTemporaryCardLock"]').attr('data-content-card-active');
            $('[data-id="card-active"]').removeClass("hidden");
            $('[data-id="card-locked"]').addClass("hidden");
        } else if (status.Icon.toLowerCase() === "deactivated") {
            cardActiveOrLockedText = $('[data-id="popoverInfoTemporaryCardLock"]').attr('data-content-card-locked');
            $('[data-id="card-locked"]').removeClass("hidden");
            $('[data-id="card-active"]').addClass("hidden");
        }

        $('[data-id="popoverInfoTemporaryCardLock"]').attr('data-content', cardActiveOrLockedText);
    }

    function createFormOptions(formId, containerId, spinnerOptions) {
        if (formId === "#") {
            return {
                containerId: containerId,
                formId: formId,
                spinner: $.extend({
                        radius: 3,
                        width: 2
                    },
                    spinnerOptions)
            };
        } else {
            return {
                type: $(formId).attr("method"),
                url: $(formId).attr("action"),
                formData: $(formId).serialize(),
                containerId: containerId,
                formId: formId,
                spinner: $.extend({
                        radius: 3,
                        width: 2
                    },
                    spinnerOptions)
            };
        }
    }

    return {
        SetupMyControl: function(options) {
            // start spinner for all related my control widgets
            Common.StartSpinner(createFormOptions("#", '[data-id="myControlGeoControlContent"]', { containerSelector: '[data-id="geoControlContentSpinnerContainer"]' }));
            Common.StartSpinner(createFormOptions("#", '[data-id="myControlTemporaryCardLockContent"]', { containerSelector: '[data-id="temporaryCardLockContentSpinnerContainer"]' }));
            Common.StartSpinner(createFormOptions("#", '[data-id="myControlElectronicCommerceContent"]', { containerSelector: '[data-id="electronicCommerceContentSpinnerContainer"]' }));
            Common.StartSpinner(createFormOptions("#", '[data-id="myControlCashWithdrawalContent"]', { containerSelector: '[data-id="cashWithdrawalContentSpinnerContainer"]' }));
            Common.StartSpinner(createFormOptions("#", '[data-id="exchangeRateInfoContent"]', { containerSelector: '[data-id="exchangeRateInfoSpinnerContainer"]' }));

            // load status once and distribute it over all my control widgets
            var data = { cardId: options.selectedCardId };
            Common.AjaxWithErrorHandling(options.loadMyControlStatusUri,
                data,
                function(result) {
                    if (result.isValid) {
                        displayMyControlStatus('[data-id="myControlGeoControlContent"]', result.status.GeoBlocking, 
                            options.selectedCardId, result.status.GeoBlocking.Icon.toLowerCase() === "deactivated" && options.isSupportMode);
                        displayMyControlStatus('[data-id="myControlTemporaryCardLockContent"]', result.status.AllAuthorizations,
                            options.selectedCardId, result.status.AllAuthorizations.Icon.toLowerCase() === "activated" && options.isSupportMode);
                        displayMyControlStatus('[data-id="myControlElectronicCommerceContent"]', result.status.ECommerceAndMoto,
                            options.selectedCardId, result.status.ECommerceAndMoto.Icon.toLowerCase() === "activated" && options.isSupportMode);
                        displayMyControlStatus('[data-id="myControlCashWithdrawalContent"]', result.status.CashWithdrawal,
                            options.selectedCardId, result.status.CashWithdrawal.Icon.toLowerCase() === "activated" && options.isSupportMode);
                        displayTemporaryCardLockDescription(result.status.AllAuthorizations);

                    } else {
                        $('[data-my-control="loading"]').each(function() {
                            $(this).attr('hidden', true);
                        });

                        $('[data-my-control="status"]').each(function() {
                            $(this).find(".status").text(result.status);
                            $(this).removeAttr('hidden');
                        });

                        $('[data-my-control="action"]').each(function() {
                            $(this).find(".action.no-link").text(result.action);
                            $(this).find(".action.no-link").removeAttr('hidden');
                            $(this).removeAttr('hidden');
                        });

                        $('[data-id="popoverInfoTemporaryCardLock"]').attr('data-content',
                            $('[data-id="popoverInfoTemporaryCardLock"]').attr('data-content-card-active'));
                        $('[data-id="card-active"]').removeClass("hidden");
                        $('[data-id="card-locked"]').addClass("hidden");

                        $('[data-id="spinner-card-status"]').trigger("detailsCardStatusFailed");
                    }
                });
        },

        SetupMyControlPin: function (options) {
            if (/MSIE \d|Trident.*rv:/.test(navigator.userAgent)) {
                DecryptPinIE11.Initialize(options);
            } else {
                DecryptPin.Initialize(options);
            }
        },

        SetupAllowWorldwide: function(options) {
            setupDatePickers(options);
            closeDatePickers();

            Common.PrepareForm(options.formSelector, options.submitButtonSelector);
        },

        SetupSwitchControls: function(options) {
            Common.PrepareForm(options.formSelector, options.submitButtonSelector);
        },

        SetupCardStatus: function (options) {
            $(options.containerId).removeClass("hidden");
            var spinnerOptions = { containerId: options.containerId, spinner: { radius: 1, width: 1 } };
            var spinner = Common.StartSpinner(spinnerOptions);

            $(options.containerId).on("detailsCardStatus", function (event, status) {
                Common.StopSpinner(spinner, spinnerOptions);
                $(options.containerId).addClass("hidden");
                $('.temporary-lock-tag').addClass("hidden");
                if (status === "deactivated") {
                    $('.temporary-lock-tag').removeClass("hidden");
                    $('[data-id="details-card-status"]').text(options.temporaryLockStatus);
                }
                $('[data-id="details-card-status"]').removeClass("hidden");
            });

            $(options.containerId).on("detailsCardStatusFailed", function (event) {
                Common.StopSpinner(spinner, spinnerOptions);
                $(options.containerId).addClass("hidden");
                $('[data-id="details-card-status"]').removeClass("hidden");
            });
        }
};
})();;
var OsaAuthentication = (function () {
	var validatedBirthdayValue;
	var validatedAccountNumber;

    return {
        InitializeLogin: function (options) {
            Common.PrepareForm(options.formId, options.confirmButtonId, function () {
                Common.AjaxGetPartialView({
                    type: Common.FORM_METHOD_POST,
                    url: $(options.formId).attr('action'),
                    formData: $(options.formId).serialize(),
                    containerId: "#loginContainer",
                    formId: options.formId,
                    hideElements: true,
                    spinner: {
                        keepSpinner: true
                    }
                });
            });

            $('#birthdayDateInput').off('blur');
            var helpLink = $(options.specialHelpLinkSelector);
            if (helpLink.length) {
                helpLink.attr('href', options.specialHelpLinkUri);
            }
        },

		InitializeAccountNumber: function(options) {
			var accountNumberField = $(options.accountInputSelector);
			validatedAccountNumber = accountNumberField.val();

			function createRule() {
				return {
					alwaysinvalid: {
						depends: function () {
							var currentAccountNumber = accountNumberField.val();
							return validatedAccountNumber === currentAccountNumber;
						}
					},
					messages: {
						alwaysinvalid: function () {
							return options.invalidAccountNumberErrorText;
						}
					}
				};
			};

			if (validatedAccountNumber) {
				accountNumberField.rules("add", createRule());
			}

			Common.MarkAsNumeric(options.accountInputSelector);
		},

        InitializeDatePicker: function (options) {
        	var form = $(options.formId);
	        var birthDayField = $(options.birthdayInputSelector);

            var datePickerOptions = {
                language: options.cultureCode,
                autoclose: true,
                forceParse: false,
                todayHighlight: true,
                todayBtn: false,
                clearBtn: true,
                startDate: '01/01/1900',
                keyboardNavigation: false,
                format: options.dateFormat.replace(/M/g, "m")
            };

            validatedBirthdayValue = birthDayField.val();
            $(options.datePickerId).datepicker(datePickerOptions);

	        var datepickerField = $('[data-type="date"]');

	        datepickerField.off('focus');
            $('[data-type="date"]').on("click", function (e) {
                e.preventDefault();
            });

	        var validation = form.validate();

            function createRule() {
            	return {
            		alwaysinvalid: {
            			depends: function () {
				            var currentBirthdayValue = birthDayField.val();
            				return validatedBirthdayValue === currentBirthdayValue;
			            }
            		},
            		messages: {
            			alwaysinvalid: function () {
            				return options.loginWillBeLockedErrorText
            					? options.loginWillBeLockedErrorText
            					: validation.settings.messages[options.birthDayNameSelector].dateformat;
            			}
            		}
            	};
            };

            if (options.loginWillBeLockedErrorText) {
            	validation.settings.messages[options.birthDayNameSelector].dateformat = options.loginWillBeLockedErrorText;
            }

            if (validatedBirthdayValue) {
				birthDayField.rules("add", createRule());
            }

            $('[data-type="date"]').closest(".date").datepicker().on('changeDate', function () {
                var input = $(this).find('input');

                if (!input.is(":focus")) {
                    form.validate().element(input);
                }
            });
        }
    };
})();;
/* !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
 * Do not add features that are unsupported by IE11, e.g. async await.
 * This is because when IE11 accesses the live portal,
 * the bundler delivers all the js files and IE 11 tries
 * to parse the file regardless of whether it is actually needed or not.
 */
var DecryptPin = (function () {

    function hidePin() {
        $('#first').text("X");
        $('#second').text("X");
        $('#third').text("X");
        $('#fourth').text("X");
    }

    function getEncryptedPin(options, privateKey) {

        $('#decrypt-pin-button').removeClass("hidden");

        var timer;
        $('#decrypt-pin-button').on('mousedown touchstart', function () {

            $('#digitsPinDisplay').addClass("hidden");

            $(window).on('mouseup touchend', function () {
                $('#decrypt-pin-button').prop('disabled', true);
                hidePin();
                clearInterval(timer);
                window.open(options.successDisplayPinUri, "_self");
            });

            Common.AjaxWithErrorHandling(options.encryptedPinUri,
                    { cardId: options.selectedCardId },
                    function(result) {
                        if (result.isValid) {

                            $('#digitsPinDisplay').removeClass("hidden");

                            $('.horizontal-line-static').addClass("hidden");
                            $('.horizontal-line-animated').removeClass("hidden");

                            var timeLeft = 6;
                            timer = setInterval(function () {
                                if (timeLeft <= 0) {
                                    hidePin();
                                    clearInterval(timer);
                                    $('#countdownTimer').text(timeLeft);
                                    $('.horizontal-line-animated').addClass("hidden");
                                    window.open(options.successDisplayPinUri, "_self");
                                } else {
                                    $('#countdownTimer').text(timeLeft);
                                }
                                timeLeft -= 1;
                            }, 1000);

                            var encryptedPinArrayBuffer = CryptoUtil.Base64ToArrayBuffer(result.encryptedPin);

                            CryptoUtil.DecryptDataWithPrivateKey(encryptedPinArrayBuffer, privateKey)
                                .then(function(decryptedPinArrayBuffer) {
                                    var decryptedPin = CryptoUtil.ConvertToHex(decryptedPinArrayBuffer, 2);
                                    $('#first').text(decryptedPin[0]);
                                    $('#second').text(decryptedPin[1]);
                                    $('#third').text(decryptedPin[2]);
                                    $('#fourth').text(decryptedPin[3]);
                                }).catch(function() {
                                    window.open(options.errorDisplayPinUri, "_self");
                                });

                        } else {
                            window.open(options.errorDisplayPinUri, "_self");
                        }
                    },
                    '#spinnerSetupPinDisplay',
                    options);
            });
    }

    return {
        Initialize: function(options) {

            var spinner = Common.StartSpinner({ containerId: '#spinnerSetupPinDisplay'});

            var keyPair;
            CryptoUtil.GenerateKeyPair().then(function (generatedKeyPair) {

                keyPair = generatedKeyPair;
                return CryptoUtil.ExportPublicKey(keyPair.publicKey);

            }).then(function(publicKey) {

                var exportedPublicKey = CryptoUtil.FormatPublicKey(publicKey);
                return new Promise(function(resolve, reject) {
                    Common.AjaxWithErrorHandling(options.displayPinUri,
                        { cardId: options.selectedCardId, publicKey: exportedPublicKey },
                        function(result) {
                            if (result.isValid) {
                                resolve(result);
                            } else {
                                reject(result);
                            }
                        });
                });

            }).then(function () {
                Common.StopSpinner(spinner, { containerId: '#spinnerSetupPinDisplay' });
                $('#digitsPinDisplay').removeClass("hidden");
                $('.horizontal-line-static').removeClass("hidden");
                getEncryptedPin(options, keyPair.privateKey);
            }).catch(function() {
                window.open(options.errorDisplayPinUri, "_self");
            });
        }
    };
})();
;
var DecryptPinIE11 = (function () {

    function hidePin() {
        $('#first').text("X");
        $('#second').text("X");
        $('#third').text("X");
        $('#fourth').text("X");
    }

    function exportPublicKey(publicKey, callback) {
        var op = CryptoUtil.ExportPublicKey(publicKey);

        op.oncomplete = function (e) {
            var exportedPublicKey = CryptoUtil.FormatPublicKey(e.target.result);
            callback(exportedPublicKey);
        };

        op.onerror = function (error) {
            console.error(error);
        };
    }

    function exportPrivateKey(privateKey, callback) {
        var op = CryptoUtil.ExportPrivateKey(privateKey);

        op.oncomplete = function (e) {
            var exportedPrivateKey = CryptoUtil.FormatPrivateKey(e.target.result);
            callback(exportedPrivateKey);
        };

        op.onerror = function (error) {
            console.error(error);
        };
    }

    function getEncryptedPin(options, privateKey) {
        $('#decrypt-pin-button').removeClass("hidden");

        var timer;
        $('#decrypt-pin-button').on('mousedown', function () {

            $('#digitsPinDisplay').addClass("hidden");

            $(window).on('mouseup', function () {
                $('#decrypt-pin-button').prop('disabled', true);
                hidePin();
                clearInterval(timer);
                window.open(options.successDisplayPinUri, "_self");
            });

            Common.AjaxWithErrorHandling(options.encryptedPinUri,
                { cardId: options.selectedCardId },
                function (result) {
                    if (result.isValid) {
                        $('#digitsPinDisplay').removeClass("hidden");
                        $('.horizontal-line-static').addClass("hidden");
                        $('.horizontal-line-animated').removeClass("hidden");

                        $('#pinDisplayTimeText').removeClass("hidden");
                        var timeLeft = 6;
                        timer = setInterval(function () {
                            if (timeLeft <= 0) {
                                hidePin();
                                clearInterval(timer);
                                $('#countdownTimer').text(timeLeft);
                                $('.horizontal-line-animated').addClass("hidden");
                                window.open(options.successDisplayPinUri, "_self");
                            } else {
                                $('#countdownTimer').text(timeLeft);
                            }
                            timeLeft -= 1;
                        }, 1000);

                        var encryptedPinArrayBuffer = CryptoUtil.Base64ToArrayBuffer(result.encryptedPin);

                        var op = CryptoUtil.DecryptDataWithPrivateKey(encryptedPinArrayBuffer, privateKey);

                        op.oncomplete = function (evt) {
                            var decryptedPinArrayBuffer = evt.target.result;
                            var decryptedPin = CryptoUtil.ConvertToHex(decryptedPinArrayBuffer, 2);
                            $('#first').text(decryptedPin[0]);
                            $('#second').text(decryptedPin[1]);
                            $('#third').text(decryptedPin[2]);
                            $('#fourth').text(decryptedPin[3]);
                        };

                        op.onerror = function () {
                            window.open(options.errorDisplayPinUri, "_self");
                        };

                    } else {
                        window.open(options.errorDisplayPinUri, "_self");
                    }
                },
                '#spinnerSetupPinDisplay',
                options);
        });
    }

    return {
        Initialize: function(options) {
            try {

                var spinner = Common.StartSpinner({ containerId: '#spinnerSetupPinDisplay' });

                var op = CryptoUtil.GenerateKeyPair();
                op.oncomplete = function(e) {
                    var keyPair = e.target.result;

                    exportPublicKey(keyPair.publicKey, function(exportedPublicKey) {

                        Common.AjaxWithErrorHandling(options.displayPinUri, { cardId: options.selectedCardId, publicKey: exportedPublicKey },
                            function (result) {
                                if (result.isValid) {
                                    exportPrivateKey(keyPair.privateKey, function () {
                                        Common.StopSpinner(spinner, { containerId: '#spinnerSetupPinDisplay' });
                                        $('#digitsPinDisplay').removeClass("hidden");
                                        $('.horizontal-line-static').removeClass("hidden");
                                        getEncryptedPin(options, keyPair.privateKey);
                                    });
                                } else {
                                    window.open(options.errorDisplayPinUri, "_self");
                                }
                            }, '#spinnerSetupPinDisplay');
                    });

                };

                op.onerror = function(error) {
                    console.error(error);
                };
            } catch (error) {
                console.error(error);
            }
        }
    };
})();
;
var PasswordOrder = (function() {

    return {
        PrepareStepOne: function() {
            $("#creditCardNumberInput").numeric();
            Common.PrepareForm('#stepOneForm', '#proceedToStepTwoButton');
        },

        PrepareStepTwoPerLetter: function (options) {
            $("#cardVerificationCodeTextField").numeric();
            var form = $('#stepTwoLetterForm');

            var datePickerOptions = {
                language: options.cultureCode,
                autoclose: true,
                forceParse: false,
                todayHighlight: false,
                todayBtn: false,
                clearBtn: false,
                startDate: '01/01/1900',
                keyboardNavigation: false,
                format: options.dateFormat.replace(/M/g, "m")
            };

            $('#dateOfBirthDatePicker').datepicker(datePickerOptions);
            
            $('[data-type="date"]').off();
            $('[data-type="date"]').on("click", function (e) {
                e.preventDefault();
            });

            $('[data-type="date"]').closest(".date").datepicker().on('changeDate', function () {
                var input = $(this).find('input');

                if (!input.is(":focus")) {
                    form.validate().element(input);
                    input.focus();
                }
            });

            Common.PrepareForm('#stepTwoLetterForm', '#proceedToStepThreeButton');
        }
    };
})();;
var PersonalData = (function () {
    var originalValues = {};
    var verificationsVisited = {};
    var verificationCompleted = {};
    var verificationErrorsToShow = {};
    var wasEmailTokenVerificationSuccessful = false;
    var wasScaTokenVerificationSuccessful = false;

    function removeWhitespace(value) {
        if (value) {
            return value.replace(/\s+/g, "");
        } else {
            return value;
        }
    }

    function isFieldValid(fieldId) {
        var validator = $("#requestContactDataUpdateForm").validate();
    	return validator && validator.check($(fieldId)[0]);
    }

    function rememberOriginalValue(originalFieldId) {
        var originalValue = $(originalFieldId).val();

        originalValues[originalFieldId] = originalValue;
    }

    function resetOriginalValue(originalFieldId) {
        var originalValue = originalValues[originalFieldId];
        $(originalFieldId).val(originalValue);
    }

    function resetVerification(originalFieldId) {
        if (originalFieldId) {
            verificationCompleted[originalFieldId] = false;
        }
    }

    function setVerification(originalFieldId, isVerificationDialogShownToUser) {
        verificationCompleted[originalFieldId] = isVerificationDialogShownToUser;
    }

    function hideVerification(verificationGroupId) {
        $(verificationGroupId).hide();
    }

    function checkIfVerificationsAreCompleted() {
        var areAllRequiredVerificationLinksFollowed = true;
        var areAllElementsValid = true;

        for (var verifiedFiedId in verificationCompleted) {
            var element = $(verifiedFiedId);
            var isValid = element.valid();
            areAllElementsValid &= isValid;

            var groupElements = $("[data-input-group=" + element.attr("data-input-group") + "]");
            var areAllGroupedValueSameAsOriginal = $(verifiedFiedId).val() === originalValues[verifiedFiedId];

            if (groupElements.length > 0) {
                groupElements.each(function () {
                    var selector = "#" + $(this).attr("id");
                    areAllGroupedValueSameAsOriginal &= $(this).val() === originalValues[selector];
                });
            }

            if (!verificationCompleted.hasOwnProperty(verifiedFiedId) || areAllGroupedValueSameAsOriginal || !isValid) {
                continue;
            }

            var linkFollowed = verificationCompleted[verifiedFiedId];
            areAllRequiredVerificationLinksFollowed &= linkFollowed;

            toggleVerificationError(!linkFollowed, verifiedFiedId, true);
        }

        return areAllRequiredVerificationLinksFollowed && areAllElementsValid;
    }

    function isVerificationNecessary(currentFieldId, ignoreCase) {
        var isNecessary;
        var currentValue = $(currentFieldId).val();

        if (currentValue === "" || !(isFieldValid(currentFieldId) || verificationErrorsToShow[currentFieldId] === true)) {
            isNecessary = false;
        } else {
            var rememberedValue = originalValues[currentFieldId];

            if (ignoreCase) {
                isNecessary = (removeWhitespace(rememberedValue).toLowerCase() !== removeWhitespace(currentValue).toLowerCase());
            } else {
                isNecessary = (removeWhitespace(rememberedValue) !== removeWhitespace(currentValue));
            }
        }

        if (isNecessary) {
            setVerification(currentFieldId, false);
        } else {
            resetVerification(currentFieldId);
        }

        return isNecessary;
    }

    function clearVerificationInput() {
        $("#tokenEmailEnteredByUser").val("");
    }

    function prepareMobilePhoneVerficicationLink(countryIsoCodeId, phoneNumberId, verificationLinkId, cancelMessageId, ignoreCase) {
        rememberOriginalValue(countryIsoCodeId);
        rememberOriginalValue(phoneNumberId);

        $(countryIsoCodeId).on('change', function (e) {
            toggleVerificationError(false, phoneNumberId);
            showMobilePhoneVerificationLinkIfNecessary(countryIsoCodeId, phoneNumberId, ignoreCase, verificationLinkId, cancelMessageId);
        });

        (function () {
            var previous;

            $(phoneNumberId).on("keyup change", function (e) {
                if (Common.UNPRINTABLE_KEYCODES.indexOf(e.which) >= 0 || previous === this.value) {
                    return;
                }

                if (previous !== this.value) {
                    toggleVerificationError(false, phoneNumberId, true);
                    toggleVerificationError(false, countryIsoCodeId, true);
                }

                if ($(phoneNumberId).val().trim() === "") {
                    toggleVerificationError(false, phoneNumberId);
                    resetVerification(countryIsoCodeId);
                    resetVerification(phoneNumberId);
                    $(verificationLinkId).addClass("hidden");
                    return;
                }

                previous = this.value;

                showMobilePhoneVerificationLinkIfNecessary(countryIsoCodeId, phoneNumberId, ignoreCase, verificationLinkId, cancelMessageId);
            });
        })();
    }

    function showMobilePhoneVerificationLinkIfNecessary(countryIsoCodeId, phoneNumberId, ignoreCase, verificationLinkId, cancelMessageId) {
        var isVerificationOfIsoCodeNecessary = isVerificationNecessary(countryIsoCodeId, ignoreCase);
        var isVerificationOfPhoneNumberNecessary = isVerificationNecessary(phoneNumberId, ignoreCase);

        if (isVerificationOfIsoCodeNecessary || isVerificationOfPhoneNumberNecessary) {
            $(verificationLinkId).removeClass("hidden");
            $(verificationLinkId).show();
            $(cancelMessageId).addClass("hidden");
        } else {
            $(verificationLinkId).addClass("hidden");
            toggleVerificationError(false, countryIsoCodeId, true);
        }
    }

    function prepareEmailVerficicationLink(inputFieldId, verificationLinkId, cancelMessageId, ignoreCase) {
        rememberOriginalValue(inputFieldId);

        (function () {
            var previous;
            $(inputFieldId).on("keyup change", function (e) {
                if (Common.UNPRINTABLE_KEYCODES.indexOf(e.which) >= 0 || previous === this.value) {
                    return;
                }

                if (previous !== this.value) {
                    toggleVerificationError(false, inputFieldId);
                }

                if ($(inputFieldId).val().trim() === "") {
                    resetVerification(inputFieldId);
                    toggleVerificationError(false, inputFieldId);
                    $(verificationLinkId).addClass("hidden");
                    return;
                }

                if (isVerificationNecessary(inputFieldId, ignoreCase)) {
                    $(verificationLinkId).removeClass("hidden");
                    $(verificationLinkId).show();
                    $(cancelMessageId).addClass("hidden");
                } else {
                    $(verificationLinkId).addClass("hidden");
                }

                previous = this.value;
            });
        })();
    }

    function prepareMobilePhoneVerificationDialog(errorMessageId, isoCountryCodeId, phoneNumberId, verificationLinkId, cancelMessageId) {
        toggleVerificationError(false, isoCountryCodeId, true);
        toggleVerificationError(false, phoneNumberId, true);
        resetVerfication(errorMessageId);

        $('#verificationCancleOldMobilePhone, #verificationCancleOldMobilePhoneSms, #verificationCancleNewMobilePhone').on("click", function () {
            resetOriginalValue(isoCountryCodeId);
            resetOriginalValue(phoneNumberId);
            cancleVerification(verificationLinkId, cancelMessageId);
        });
    }

    function prepareEmailVerificationDialog(errorMessageScaTokenId, errorMessageId, inputFieldId, verificationLinkId, cancelMessageId) {
        toggleVerificationError(false, inputFieldId, true);
        resetVerfication(errorMessageId);
        resetVerfication(errorMessageScaTokenId);

        $('#verificationCancleEmail').on("click", function () {
            resetOriginalValue(inputFieldId);
            cancleVerification(verificationLinkId, cancelMessageId);
        });
    }

    function cancleVerification(verificationLinkId, cancelMessageId) {
        $(verificationLinkId).addClass("hidden");
        $(cancelMessageId).removeClass("hidden");
        $(cancelMessageId).show();
    }

    function resetVerfication(errorMessageId) {
        $(errorMessageId).hide();
        $('#tokenEnteredByUser').removeClass('input-validation-error');
        clearVerificationInput();

        //since we use the same partial view for different use cases, we have to eliminate all existing event handlers before the new ones are registered
        $('#verificationSubmit').off('click');
        $('#requestCodeAgain').off('click');
        $('[id^=verificationCancle]').off('click');
    }

    function verifyTokenOldMobileNumber(options, uri, verifiedTokenId, verifyLinkId) {
        var data = {
            token: $(options.tokenOldPhoneEnteredByUserSelector).val()
        };

        Common.AjaxWithErrorHandling(uri, data, function (result) {
            if (result.IsValid) {
                var userInput = $(options.tokenOldPhoneEnteredByUserSelector).val();
                $(verifiedTokenId).val(userInput);

                $(options.errorMessageOldPhoneId).hide();
                $(options.tokenOldPhoneEnteredByUserSelector).removeClass("input-validation-error");
                $(options.errorMessageOldPhoneId).addClass("hidden");

                $("#verificationForm").modal("hide");

                hideVerification(verifyLinkId);
                $(options.inputFieldId).removeAttr("readonly");
                $("#MobileNumber_IsoCountryCode").removeAttr("readonly");
                $("#MobileNumber_IsoCountryCode").prop("disabled", false);
                $(options.popoverSelector).addClass("hidden");

                $(options.verificationLinkId).removeClass("hidden");
            } else {
                $(verifiedTokenId).val("");

                $(options.errorMessageOldPhoneId).show();
                $(options.tokenOldPhoneEnteredByUserSelector).addClass("input-validation-error");
                $(options.errorMessageOldPhoneId).removeClass("hidden");
            }

            return result.IsValid;
        });
    }

    function verifyToken(tokenEnteredByUserSelector, errorMessageId, uri, verifiedTokenId, verifyLinkId, closeDialogOnSuccess, type) {
        var data = {
            token: $(tokenEnteredByUserSelector).val()
        };

        Common.AjaxWithErrorHandling(uri, data, function (result) {
            if (result.IsValid) {
                var userInput = $(tokenEnteredByUserSelector).val();
                $(verifiedTokenId).val(userInput);

                $(errorMessageId).hide();
                $(tokenEnteredByUserSelector).removeClass("input-validation-error");

                if (type === "email") {
                    wasEmailTokenVerificationSuccessful = true;
                } else if (type === "sca") {
                    wasScaTokenVerificationSuccessful = true;

                } else {
                    // to be continued ...
                }
                
                if (closeDialogOnSuccess
                    || wasScaTokenVerificationSuccessful && wasEmailTokenVerificationSuccessful) {
                    $("#verificationForm").modal("hide");

                    hideVerification(verifyLinkId); 
                }
            } else {
                $(verifiedTokenId).val("");

                $(errorMessageId).show();
                $(tokenEnteredByUserSelector).addClass("input-validation-error");
                $(errorMessageId).removeClass("hidden");

                ScaProcess.Reset();
            }

            return result.IsValid;
        });
    }

    function requestTokenEmail(options, data) {
        Common.AjaxWithErrorHandling(options.requestVerificationEmailTokenUri, data, function (result) {
            if (result.IsValid) {
                showDialog(options, options.checksumEmailSelector, result.Checksum);
            }
        });
    }

    function requestTokenNewPhone(options, data) {
        Common.AjaxWithErrorHandling(options.requestVerificationNewPhoneTokenUri, data, function (result) {
            if (result.IsValid) {
                showDialog(options, options.checksumNewPhoneSelector, result.Checksum);
            }
        });
    }

    function requestTokenOldPhone(options) {
        Common.AjaxWithErrorHandling(options.requestVerificationTokenOldPhoneUri, null, function (result) {
            if (result.IsValid) {
                showDialog(options, options.checksumOldPhoneSelector, result.Checksum);
            }
        });
    }

    function showDialog(options, checksumSelector, checksum) {
        $(checksumSelector).text(checksum);
        $("#verifyInputTitle").html(options.verifyInputTitle);
        $("#verifyInputText").html(options.verifyInputText);

        clearVerificationInput();
        $(options.modalDialogId).modal("show");
        resetVerification(options.inputFieldId, true);
        resetVerification(options.isoCountryCodeId, true);
        toggleVerificationError(false, options.inputFieldId);
    }

    function toggleVerificationError(isErrorShown, verifiedFieldSelector, immediatelyValidateField) {
		if (!isErrorShown || isFieldValid(verifiedFieldSelector)) {
			verificationErrorsToShow[verifiedFieldSelector] = isErrorShown;
		}

		if (immediatelyValidateField) {
			$(verifiedFieldSelector).valid();
		}
    }

    function initializeNameChangeEvent(firstNamePartId, secondNamePartId, specimenSignatureContainerId) {
        rememberOriginalValue(firstNamePartId);

        $(firstNamePartId).on("keyup change", function () {
            toggleSpecimenSignatureOnNameChange(firstNamePartId, secondNamePartId, specimenSignatureContainerId);
        });
    }

    function toggleSpecimenSignatureOnNameChange(givenNameId, surnameId, specimenSignatureContainerId) {
        if ($(givenNameId).val() === originalValues[givenNameId] && $(surnameId).val() === originalValues[surnameId]) {
            $(specimenSignatureContainerId).addClass("hidden");
        } else {
            $(specimenSignatureContainerId).removeClass("hidden");
        }
    }

    function toggleVerificationContainer(showVerificationContainer, verficationContainerSelector) {
        if (showVerificationContainer) {
            $(verficationContainerSelector).removeClass("hidden");
        } else {
            $(verficationContainerSelector).addClass("hidden");
        }

        return showVerificationContainer;
    }

    function toggleOnOriginalValueChange(fieldSelector) {
        var otherpartSelector = $(fieldSelector).attr("data-verification-otherpart");
        var verficationContainerSelector = $(fieldSelector).attr("data-verification-container");
        var fieldValue = $(fieldSelector).val();
        var originalValue = originalValues[fieldSelector];
        var otherpartValue = $(otherpartSelector).val();
        var otherpartOriginalValue = originalValues[otherpartSelector];

        var showVerificationContainer = (fieldValue !== "" && fieldValue !== originalValue) || (otherpartValue !== "" && otherpartValue !== otherpartOriginalValue);

        return toggleVerificationContainer(showVerificationContainer, verficationContainerSelector);
    }

    function toggleOnVerificationValueChange(countryCodeSelector, phoneNumberSelector) {
        var countryCodePartnerSelector = $(countryCodeSelector).attr("data-verification-partner");
        var phoneNumberPartnerSelector = $(phoneNumberSelector).attr("data-verification-partner");
        var verficationContainerSelector = $(countryCodePartnerSelector).attr("data-verification-container");

        var isCountryCodeValid = $(countryCodeSelector).val() === $(countryCodePartnerSelector).val();
        var isPhoneNumberValid = $(phoneNumberSelector).val() === $(phoneNumberPartnerSelector).val();

        var showVerificationContainer = Common.IsVisible(verficationContainerSelector) && (!isCountryCodeValid || !isPhoneNumberValid);

        return toggleVerificationContainer(showVerificationContainer, verficationContainerSelector);
    }

    function countryCodeChangeHandler(countryCodeSelector) {
        var currentValue = $(countryCodeSelector).val();
        var otherPartSelector = $(countryCodeSelector).attr("data-verification-otherpart");
        var verificationPartnerSelector = $(countryCodeSelector).attr("data-verification-partner");
        var verificationPartnerOtherpartSeletor = $(verificationPartnerSelector).attr("data-verification-otherpart");

        $(verificationPartnerSelector).val(currentValue);

        if (toggleOnOriginalValueChange(countryCodeSelector)) {
            // copy country code and clear phone number of verification partner
            verificationsVisited[verificationPartnerSelector] = false;
            verificationsVisited[verificationPartnerOtherpartSeletor] = false;

            $(verificationPartnerOtherpartSeletor).val("");

            $(verificationPartnerSelector).valid();
        } else {
            $(verificationPartnerOtherpartSeletor).val(originalValues[otherPartSelector]);
        }

        $(verificationPartnerOtherpartSeletor).valid();
    }

    function phoneNumberChangeHandler(phoneNumberSelector) {
        var verificationPartnerSelector = $(phoneNumberSelector).attr("data-verification-partner");

        // clear phone number of verification partner and set visited to false
        verificationsVisited[phoneNumberSelector] = false;
        verificationsVisited[verificationPartnerSelector] = false;

        var isVerificationShown = toggleOnOriginalValueChange(phoneNumberSelector);

        if (isVerificationShown) {
            $(verificationPartnerSelector).val("");
        } else {
            $(verificationPartnerSelector).val($(phoneNumberSelector).val());
        }

        $(verificationPartnerSelector).valid();
    }

    function changeMobilePhoneNumberWithoutOldPhone(options) {
        $(options.popoverSelector).popover("hide");
        $("#IsOldPhoneAvailable").val("false");
        $("#withoutOldPhoneContainer").removeClass("hidden");
        infoText = options.verifyMobileNumberWithOldPhoneInputText;
        $("#withOldPhoneContainer").addClass("hidden");
        $(options.modalDialogId).modal("show");
    }

    function extractMobilePhoneNumber(options) {
        var countryCodeWithAdditionalText = $("#MobileNumber_IsoCountryCode option:selected").text();
        var countryCode = countryCodeWithAdditionalText.replace(/[^\+0-9]/g, "");

        var phoneNumber = $(options.inputFieldId).val();

        return countryCode + " " + phoneNumber;
    }

    function hideAllVerficationContent(options) {
        $("#mobilePhoneValidationFileUpload").addClass("hidden");
        $("#mobilePhoneValidationOldPhoneNumber").addClass("hidden");
        $("#mobilePhoneValidationNewPhoneNumber").addClass("hidden");
        $("#emailValidation").addClass("hidden");
        toggleVerificationError(false, options.inputFieldId, true);
    }

    return {
        InitializeMobilePhoneVerification: function (options) {
            hideAllVerficationContent(options);
            $(options.errorMessageOldPhoneId).hide();
            $(options.errorMessageNewPhoneId).hide();

            prepareMobilePhoneVerficicationLink(options.isoCountryCodeId, options.inputFieldId, options.verificationLinkId, options.cancelMessageId, false);

            $(document.body).on("click", options.changeMobilePhoneNumberWithOldPhoneId, function () {
                hideAllVerficationContent(options);
                $("#mobilePhoneValidationOldPhoneNumber").removeClass("hidden");
            });

            // same behaviour as for changeMobilePhoneNumberWithoutOldPhoneId (file upload)
            $(document.body).on("click", '#verifyEmptyMobilePhoneNumber', function () {
                hideAllVerficationContent(options);
                $("#mobilePhoneValidationFileUpload").removeClass("hidden");
            });

            $(document.body).on("click", options.changeMobilePhoneNumberWithoutOldPhoneId, function () {
                hideAllVerficationContent(options);
                $("#mobilePhoneValidationFileUpload").removeClass("hidden");
            });

            $(document.body).on("click", "#verifyNewMobilePhoneNumber", function () {
                hideAllVerficationContent(options);
                $("#mobilePhoneValidationNewPhoneNumber").removeClass("hidden");
            });

            toggleVerificationError(false, options.isoCountryCodeId);
            toggleVerificationError(false, options.inputFieldId);

            (function () {
                var previous;
                $(options.inputFieldId).on('focus keyup', function (e) {
                    if (previous !== this.value && e.charCode !== Common.KEYCODE_ENTER) {
                        toggleVerificationError(false, options.isoCountryCodeId);
                        toggleVerificationError(false, options.inputFieldId);
                    }

                    previous = this.value;
                });

                $(options.inputFieldId + ", " + options.isoCountryCodeId).on("change", function () {
                    if (previous !== this.value) {
                        toggleVerificationError(false, options.isoCountryCodeId);
                        toggleVerificationError(false, options.inputFieldId);
                    }
                });
            })();

            if (!options.showVerificationLink) {
                $(options.verificationLinkId).hide();
            }

            $(options.popoverSelector).on("click", function (e) {
                e.preventDefault();
            });

            $(document.body).on("click", options.changeMobilePhoneNumberWithoutOldPhoneId, function () {
                changeMobilePhoneNumberWithoutOldPhone(options);
            });

            $(options.verifyEmptyMobileNumberSelector).on("click", function () {
                changeMobilePhoneNumberWithoutOldPhone(options);
            });

            $(document.body).on("click", options.changeMobilePhoneNumberWithOldPhoneId, function () {
                $(options.popoverSelector).popover("hide");
                $("#withOldPhoneContainer").removeClass("hidden");

                infoText = options.verifyMobileNumberWithOldPhoneInputText;
                var countryCode = $("#MobileNumber_IsoCountryCode option:selected").val();
                var phoneNumber = $(options.inputFieldId).val();

                if (countryCode.length <= 0 && phoneNumber.length <= 0) {
                    return;
                }

                $("#tokenOldPhoneEnteredByUser").val("");
                $(options.sourceMobilePhoneNumberSelector).text(extractMobilePhoneNumber(options));

                requestTokenOldPhone(options);

                $("#requestTokenOldPhoneAgain").on("click", function (e) {
                    if (options.isSupportMode) {
                        e.preventDefault();
                        return;
                    }

                    requestTokenOldPhone(options);
                });

                $("#verificationSubmitOldPhone").on("click", function () {
                    setVerification(options.inputFieldId, true);
                    setVerification(options.isoCountryCodeId, true);
                    verifyTokenOldMobileNumber(options, options.verifyTokenOldPhoneUri, "#TokenSms", options.verificationLinkId);
                    $("#IsOldPhoneAvailable").val("true");
                });
            });

            $(options.popoverSelector).popover({
                html: true,
                content: function () {
                    return $(options.popoverHtmlMobileNumberContent).html();
                },
                title: function () {
                    return $(options.popoverHtmlMobileNumberTitle).html();
                },
                trigger: "click",
                container: "body",
                toggle: "popover",
                placement: "bottom"
            });

            $(options.verificationLinkId).on("click", function () {
                prepareMobilePhoneVerificationDialog(options.errorMessageOldPhoneId, options.isoCountryCodeId, options.inputFieldId, options.verificationLinkId, options.cancelMessageId);

                var countryCode = $("#MobileNumber_IsoCountryCode option:selected").val();
                var phoneNumber = $(options.inputFieldId).val();

                if (countryCode.length <= 0 && phoneNumber.length <= 0) {
                    return;
                }

                $("#tokenNewPhoneEnteredByUser").val("");
                $(options.targetMobilePhoneNumberSelector).text(extractMobilePhoneNumber(options));

                var data = {
                    PhoneNumber: phoneNumber,
                    IsoCountryCode: countryCode
                };

                requestTokenNewPhone(options, data);

                $("#requestTokenNewPhoneAgain").off();
                $("#requestTokenNewPhoneAgain").on("click", function (e) {
                    if (options.isSupportMode) {
                        e.preventDefault();
                        return;
                    }

                    requestTokenNewPhone(options, data);
                });

                $("#verificationSubmitNewPhone").on("click", function () {
                    setVerification(options.inputFieldId, true);
                    setVerification(options.isoCountryCodeId, true);
                    verifyToken(options.tokenNewPhoneEnteredByUserSelector, options.errorMessageNewPhoneId, options.verifyTokenNewPhoneUri, "#TokenSms", options.verificationLinkId, true);
                    $(options.verificationLinkId).addClass("hidden");
                });
            });

            $(document.body).on("click", "#verificationCancleOldMobilePhone", function (e) {
                $.get(options.requestCancelEditContactDataOverlayUri);
                $("#uploadResultContainer").html(null);
            });

            function createRule(isCountryCode) {
                return {
                    alwaysinvalid: {
                        depends: function (element) {
                            var currentSelector = "#" + $(element).attr("id");
                            var errorSelector = "#" + $(element).attr("data-input-group") + "VerificationError";
                            var isInvalid = Common.IsVisible(options.verificationLinkId) && verificationErrorsToShow[currentSelector] === true;
                            var showError = Common.IsVisible(options.verificationLinkId) &&
                            (verificationErrorsToShow[options.isoCountryCodeId] === true || verificationErrorsToShow[options.inputFieldId] === true);

                            if (isCountryCode) {
                                $(options.inputFieldId).valid();
                            }

                            if (!isInvalid) {
                                $(element).removeClass("input-validation-error");
                            }

                            if (showError) {
                                $(errorSelector).removeClass("hidden");
                            } else {
                                $(errorSelector).addClass("hidden");
                            }

                            return isInvalid;
                        }
                    },
                    messages: {
                        alwaysinvalid: function () {
                            return "";
                        }
                    }
                };
            }

            $(options.isoCountryCodeId).rules("add", createRule(true));
            $(options.inputFieldId).rules("add", createRule(false));

            $(document.body).on("click", "#confirmAuthenticityButton", function (e) {
                e.preventDefault();

                if ($("#uploadResultContainer").html().trim().length > 0) {
                    $(options.inputFieldId).removeAttr("readonly");
                    $("#MobileNumber_IsoCountryCode").removeAttr("readonly");
                    $("#MobileNumber_IsoCountryCode").prop("disabled", false);

                    // hide initial link depending if mobile number was set or not
                    $("#toggleMobileNumberPopup").addClass("hidden");
                    $("#verifyEmptyMobilePhoneNumber").addClass("hidden");

                    // prepare verification; link will be shown when number is changed
                    $(options.verificationLinkId).removeClass("hidden");

                    $(options.modalDialogId).modal("hide");
                } else {
                    $("#uploadErrorContainer").show();
                }
            });
        },

        InitializeEmailAddressVerification: function (options) {
            $('#verifyEmailAddress').on('click',
                function() {
                    ScaProcess.Reset();
                });

            prepareEmailVerficicationLink(options.inputFieldId, options.verificationLinkId, options.cancelMessageId, true);

            (function () {
                var previous;

                $(options.inputFieldId).on('focus keyup', function (e) {
                    if (previous !== this.value && e.charCode !== Common.KEYCODE_ENTER) {
                        toggleVerificationError(false, options.inputFieldId);
                    }

                    previous = this.value;
                }).on("change", function () {
                    if (previous !== this.value) {
                        toggleVerificationError(false, options.inputFieldId);
                    }
                    previous = this.value;
                });
            })();

            if (!options.showVerificationLink) {
                $(options.verificationLinkId).addClass("hidden");
            }

            toggleVerificationError(false, options.inputFieldId);

            $(options.inputFieldId).rules("add", {
                alwaysinvalid: {
                    depends: function (element) {
                        var currentSelector = "#" + $(element).attr("id");
                        var errorSelector = currentSelector + "VerificationError";

                        var isInvalid = verificationErrorsToShow[currentSelector] === true;

                        if (isInvalid) {
                            $(errorSelector).removeClass("hidden");
                        } else {
                            $(errorSelector).addClass("hidden");
                        }

                        return isInvalid;
                    }
                },
                messages: {
                    alwaysinvalid: function () {
                        return "";
                    }
                }
            });

            $(options.verificationLinkId).on("click", function () {
                hideAllVerficationContent(options);
                clearVerificationInput();
                $("#emailValidation").removeClass("hidden");
                $(options.errorMessageEmailId).hide();

                prepareEmailVerificationDialog(options.errorMessageScaTokenId, options.errorMessageEmailId, options.inputFieldId, options.verificationLinkId, options.cancelMessageId);

                var emailAddress = $(options.inputFieldId).val();

                if (emailAddress.length <= 0) {
                    return;
                }

                var data = {
                    Email: emailAddress
                };

                requestTokenEmail(options, data);

                function submitToken() {
                    setVerification(options.inputFieldId, true);

                    // reset outcome of last token validation
                    wasEmailTokenVerificationSuccessful = false;
                    wasEmailTokenVerificationSuccessful = false;

                    verifyToken(options.tokenEmailEnteredByUserSelector, options.errorMessageEmailId, options.verifyTokenEmailUri, "#TokenEmail", options.verificationLinkId, false, "email");
                    verifyToken(options.tokenScaEnteredByUserSelector, options.errorMessageScaTokenId, options.verifyScaTokenUri, "#VerifiedScaToken", options.verificationLinkId, false, "sca");
                };

                $("#requestTokenEmailAgain").on("click", function () {
                    requestTokenEmail(options, data);
                });

                $("#tokenEmailEnteredByUser").on("keydown", function (e) {
                    if (e.keyCode === Common.KEYCODE_ENTER) {
                        submitToken();
                    }
                });

                $("#verificationSubmitEmail").on("click", function () {
                    submitToken();
                });
            });
        },

        InsertSeparatorInAllCountryLists: function () {
            $('select[id*="IsoCountryCode"]').each(function () {
                $(this).children("option:eq(2)").after('<option value="" disabled="disabled">-----------------------</option>');
            });
        },

        InitializeEditPersonalData: function () {
            // show container on server error
            if ($("span[data-valmsg-for='SpecimenSignatureFileUploadViewModel']").attr("class") === "field-validation-error") {
                $("#specimenSignatureContainer").removeClass("hidden");
            }

            initializeNameChangeEvent("#GivenName", "#Surname", "#specimenSignatureContainer");
            initializeNameChangeEvent("#Surname", "#GivenName", "#specimenSignatureContainer");

            Common.PrepareForm("#requestPersonalDataUpdateForm", "#submitButton");
        },

        ResetSpecimenSignature: function () {
            var errorMessageSelector = $("span[data-valmsg-for='SpecimenSignatureFileUploadViewModel']");
            if ($(errorMessageSelector).attr("class") === "field-validation-error") {
                $(errorMessageSelector).addClass("hidden");
            }
        },

        InitializeNotifyAddress: function () {
            Common.PrepareForm("#requestNotifyAddressUpdateForm", "#submitButton");
        },

        InitializeContactData: function (options) {
            if (options.isSupportMode) {
                Common.InitializeTooltips();
            }

            var fieldsRequireringVerification = [
                {
                    countryCode: "#HomePhoneNumber_IsoCountryCode",
                    phoneNumber: "#HomePhoneNumber_PhoneNumber"
                }, {
                    countryCode: "#FaxNumber_IsoCountryCode",
                    phoneNumber: "#FaxNumber_PhoneNumber"
                }, {
                    countryCode: "#WorkPhoneNumber_IsoCountryCode",
                    phoneNumber: "#WorkPhoneNumber_PhoneNumber"
                }
            ];

            Common.PrepareForm("#requestContactDataUpdateForm", "#requestContactDataUpdateButton", function () {
                var areAllRepetitionVerificationsCompleted = true;

                for (var i = 0; i < fieldsRequireringVerification.length; i++) {
                    var countryCodeVerificationPartner = $(fieldsRequireringVerification[i].countryCode).attr("data-verification-partner");
                    var phoneNumberVerificationPartner = $(fieldsRequireringVerification[i].phoneNumber).attr("data-verification-partner");

                    verificationsVisited[fieldsRequireringVerification[i].countryCode] = true;
                    verificationsVisited[countryCodeVerificationPartner] = true;
                    verificationsVisited[fieldsRequireringVerification[i].phoneNumber] = true;
                    verificationsVisited[phoneNumberVerificationPartner] = true;

                    $(countryCodeVerificationPartner).valid();
                    $(phoneNumberVerificationPartner).valid();

                    areAllRepetitionVerificationsCompleted &= !toggleOnVerificationValueChange(countryCodeVerificationPartner, phoneNumberVerificationPartner);
                }

                var areAllRequiredVerificationLinksFollowed = checkIfVerificationsAreCompleted();

                if (areAllRepetitionVerificationsCompleted && areAllRequiredVerificationLinksFollowed) {
                    //WORKAROUND -> DISABLED OF COUNTRY CODE MUST BE ELIMINATED BECAUSE IF NOT THE VALUE WILL NOT BE SEND
                    if ($("#MobileNumber_IsoCountryCode").attr("readonly") === "readonly") {
                        $("#MobileNumber_IsoCountryCode").attr("disabled", false);
                    }
                    $("#requestContactDataUpdateForm").submit();
                }
            });

            // Ensure that all fields are validated, even if the form is already invalid and thus not tried to be submitted any more
            $("#requestContactDataUpdateButton").on("click", checkIfVerificationsAreCompleted);
            $("input, select").on("keyup", function (e) {
                if (e.keyCode === Common.KEYCODE_ENTER) {
                    checkIfVerificationsAreCompleted();
                }
            });

            PersonalData.InsertSeparatorInAllCountryLists();

            for (var i = 0; i < fieldsRequireringVerification.length; i++) {
                (function () {
                    // country code
                    var countryCodeSelector = fieldsRequireringVerification[i].countryCode;
                    var countryCodeVerificationSelector = $(fieldsRequireringVerification[i].countryCode).attr("data-verification-partner");
                    rememberOriginalValue(fieldsRequireringVerification[i].countryCode);

                    $(fieldsRequireringVerification[i].countryCode).on("change", function () {
                        var currentFieldSelector = "#" + $(this).attr("id");
                        countryCodeChangeHandler(currentFieldSelector);
                    });

                    $(countryCodeSelector + ", " + countryCodeVerificationSelector).on("change", function (e) {
                        var currentFieldSelector = "#" + $(this).attr("id");
                        var otherFieldSelector = $(currentFieldSelector).attr("data-verification-otherpart");

                        toggleOnVerificationValueChange(currentFieldSelector, otherFieldSelector);
                        verificationsVisited[currentFieldSelector] = true;

                        e.stopPropagation();
                    });

                    // phone number
                    var phoneNumberSelector = fieldsRequireringVerification[i].phoneNumber;
                    var phoneNumberVerificationSelector = $(fieldsRequireringVerification[i].phoneNumber).attr("data-verification-partner");
                    rememberOriginalValue(fieldsRequireringVerification[i].phoneNumber);

                    (function () {
                        var previous;
                        var callback = function (e) {
                        	if (previous !== this.value
								&& e.which !== Common.KEYCODE_ENTER
								&& Common.UNPRINTABLE_KEYCODES.indexOf(e.which) < 0) {
                                var currentFieldSelector = "#" + $(this).attr("id");
                                phoneNumberChangeHandler(currentFieldSelector);
                            }

                            previous = this.value;
                        };
                        $(fieldsRequireringVerification[i].phoneNumber).on('focus', callback);
                        $(fieldsRequireringVerification[i].phoneNumber).on('keyup', function (e) {
                            Common.Delay(callback.on(this, e), Common.DEBOUNCE_TIME_IN_MS);
                            return false;
                        });
                    })();

                    var verificationCallback = function (e) {
                        var currentFieldSelector = "#" + $(this).attr("id");
                        var otherFieldSeletor = $(currentFieldSelector).attr("data-verification-otherpart");
                        verificationsVisited[currentFieldSelector] = true;

                        $(currentFieldSelector).valid();

                        toggleOnVerificationValueChange(otherFieldSeletor, currentFieldSelector);

                        e.stopPropagation();
                    };

                    $(phoneNumberVerificationSelector).on("focusout", function (e) {
                        verificationCallback.call(this, e);
                    });
                    $(phoneNumberVerificationSelector).on("keyup", function(e) {
                        Common.Delay(verificationCallback.on(this, e), Common.DEBOUNCE_TIME_IN_MS);
                        return false;
                    });

                    function createRule(isVerification) {
                        return {
                            alwaysinvalid: {
                                depends: function (element) {
                                    var currentSelector = "#" + $(element).attr("id");
                                    var verificationPartnerSelector = $(element).attr("data-verification-partner");
                                    var otherPartSelector = $(element).attr("data-verification-otherpart");
                                    var otherPartVerificationPartnerSelector = $(otherPartSelector).attr("data-verification-partner");
                                    var errorContainer = $("#" + $(element).attr("data-input-group") + "VerificationError");

                                    var value = $(element).val();
                                    var verificationPartnerValue = $(verificationPartnerSelector).val();
                                    var otherPartValue = $(otherPartSelector).val();
                                    var otherPartVerificationPartnerValue = $(otherPartVerificationPartnerSelector).val();

                                    if (isVerification) {
                                        $(verificationPartnerSelector).valid();
                                    }

                                    var visitedSelector = isVerification ? currentSelector : verificationPartnerSelector;
                                    var isInvalid = (verificationsVisited[visitedSelector] === true) && value !== verificationPartnerValue;

                                    if (!isVerification) {
                                        return isInvalid;
                                    }

                                    var showErrorContainer = isInvalid || (verificationsVisited[otherPartSelector] === true && otherPartValue !== otherPartVerificationPartnerValue);

                                    if (!isInvalid) {
                                        $(element).removeClass("input-validation-error");
                                    }

                                    if (showErrorContainer) {
                                        errorContainer.text($(phoneNumberVerificationSelector).attr("data-verification-errormessage"));
                                        errorContainer.show();
                                    } else {
                                        errorContainer.hide();
                                    }

                                    return isInvalid;
                                }
                            },
                            messages: {
                                alwaysinvalid: function () {
                                    return "";
                                }
                            }
                        };
                    };

                    $(countryCodeSelector).rules("add", createRule(false));
                    $(countryCodeVerificationSelector).rules("add", createRule(true));
                    $(phoneNumberSelector).rules("add", createRule(false));
                    $(phoneNumberVerificationSelector).rules("add", createRule(true));
                })();
            }

            Common.MarkAsNumeric("#MobileNumber_PhoneNumber");
            Common.MarkAsNumeric("#HomePhoneNumber_PhoneNumber");
            Common.MarkAsNumeric("#HomePhoneNumber_PhoneNumberVerification");
            Common.MarkAsNumeric("#FaxNumber_PhoneNumber");
            Common.MarkAsNumeric("#FaxNumber_PhoneNumberVerification");
            Common.MarkAsNumeric("#WorkPhoneNumber_PhoneNumber");
            Common.MarkAsNumeric("#WorkPhoneNumber_PhoneNumberVerification");
        }
    };
})();;
var ProductDetails = function () {
    var verificationFieldOriginalValues = [];
    var currentIbanValue;
    var responsiveHelper = null;
    var table = null;
    var mobileSorting = new MobileSorting();
    var paging = false;
    var addCardToWalletSpinner;
    var addCardToWalletSpinnerOptions = { containerId: '#addCardToWalletSpinner', spinner: { radius: 1, width: 1 } };

    function getMaxRetries(provisioningTimeoutInSec) {
        if (!provisioningTimeoutInSec) {
            return null; // = poll until finite state
        }
        return 5;

        return Math.ceil(provisioningTimeoutInSec * 1000 / Common.POLLING_WAIT_TIME_IN_MSEC);
    }

    function toggleMobilePaymentButton(buttonId, isAdditionalProvisioningPossible) {
        var addToWalletButton = $(buttonId);

        if (isAdditionalProvisioningPossible) {
            addToWalletButton.show();
        } else {
            addToWalletButton.hide();
        }
    }

    function rememberOriginalValue(originalFieldId) {
        var originalValue = $(originalFieldId).val();

        verificationFieldOriginalValues[originalFieldId] = originalValue;
    }

    function rememberOriginalValueInsensitive(originalFieldId) {
        var originalValue = $(originalFieldId).val().toUpperCase();

        verificationFieldOriginalValues[originalFieldId] = originalValue;
    }

    function rememberOriginalUsername(userNameId, originalUserName) {
        verificationFieldOriginalValues[userNameId] = originalUserName.toUpperCase();
    }

    function check3DsUsername(usernameVerificationUri, usernameId, cardId, spinnerContainerId, validFn) {
        var usernameSelector = "#" + usernameId;
        var cardIdSelector = "#" + cardId;

        if (!hasValueChanged(usernameSelector) || !$(usernameSelector).valid()) {
            if (validFn) {
                validFn();
            }
            return;
        }

        var username = $(usernameSelector).val();
        var selectedCard = $(cardIdSelector).val();
        Common.AjaxWithErrorHandling(
            usernameVerificationUri, {
            "username": username,
            "cardId": selectedCard
        },
            function (response) {
                var validator = $("#submitForm").validate();
                usernameError = response.isValid ? "" : username;
                if (response.isValid) {
                    validator.showErrors({});
                    if (validFn) {
                        validFn();
                    }
                } else {
                    var errorObj = {};
                    errorObj[usernameId] = response.errorMessage;
                    validator.showErrors(errorObj);
                }
            },
            spinnerContainerId, { spinner: { radius: 2, width: 2 } });
    }

    function prepareUsernameVerification(options, usernameId, cardId, spinnerContainerId) {
        var usernameSelector = "#" + usernameId;
        rememberOriginalUsername(usernameSelector, options.originalUsername);

        $(usernameSelector).focusout(function () {
            check3DsUsername(options.usernameVerificationUri, usernameId, cardId, spinnerContainerId, function () { });
        });
    }

    function preparePasswordRepetition(passwordId, repetitionGroupId, repetitionId) {
        rememberOriginalValue(passwordId);
        $(repetitionGroupId).hide();

        $(passwordId).on('keyup change', (function () {
            if (isRepetitionNecessary(passwordId)) {
                $(repetitionId).val("");
                $(repetitionGroupId).show();
            } else {
                $(repetitionGroupId).hide();
            }
        }));

        $(repetitionId).on('focusout', (function () {
            if ($(passwordId).valid() && $(repetitionId).valid()) {
                rememberOriginalValueInsensitive(passwordId);
                $(repetitionGroupId).show();
            }
        }));
    }

    function hasValueChanged(currentFieldId) {
        var currentValue = $(currentFieldId).val().toUpperCase();

        if (currentValue === "") {
            return false;
        }

        var rememberedValue = verificationFieldOriginalValues[currentFieldId];

        return (rememberedValue.toUpperCase() !== currentValue.toUpperCase());
    }

    function isRepetitionNecessary(currentFieldId) {
        return hasValueChanged(currentFieldId);
    }

    function toggleBicRow(ibanId, bicRow, bicId) {
        var ibanField = $(ibanId);
        var ibanValue = ibanField.val().trim();

        if (currentIbanValue === ibanValue) {
            // No change detected, hence do not hide/show BIC field
            return;
        }

        currentIbanValue = ibanValue;

        if (ibanValue.substring(0, 2).toUpperCase() === "AT" || ibanValue === "" || ibanValue.toUpperCase() === "A") {
            $(bicId).val("");
            $(bicRow).hide();
        } else {
            $(bicRow).show();
        }
    }

    function createFormOptions(formId, containerId, spinnerOptions) {
        return {
            type: $(formId).attr("method"),
            url: $(formId).attr("action"),
            formData: $(formId).serialize(),
            containerId: containerId,
            formId: formId,
            spinner: $.extend({
                radius: 3,
                width: 2
            }, spinnerOptions)
        };
    }

    function insertSeparatorInCountryList() {
        $("#countryDropDown").children("option:eq(2)").after('<option value="" disabled="disabled">-----------------------</option>');
    }

    function loadPartialViewOnClick(identifier, formSelector, confirmLinkSelector, containerSelector, supportMode, spinnerOptions) {
        if (supportMode) {
            Common.InitializeTooltips();

            $(confirmLinkSelector).on("click", function (e) {
                e.preventDefault();
            });
        } else {
            $(confirmLinkSelector).on("click", function (e) {
                // prevent duplicate form submit caused by double clicks 
                // (replacement of partial view overrides a potential guard flag as used anywhere else, so we have to use this workaround here)
                var millisecondsToWait = 500;
                $(confirmLinkSelector).attr("disabled", true);

                Common.Delay(function () {
                    Common.AjaxGetPartialView(createFormOptions(formSelector, containerSelector, spinnerOptions));
                    $(confirmLinkSelector).removeAttr("disabled");
                }, millisecondsToWait);

                e.preventDefault();
            });
        }
    }

    function showConfirmationError(show) {
        if (show) {
            $("#specialTradingConditionsNotConfirmed").removeClass("hidden");
            $("#confirmationError").show();
        } else {
            $("#specialTradingConditionsNotConfirmed").addClass("hidden");
            $("#confirmationError").hide();
        }
    }

    function expandTableCells() {
        DataTableHelper.SetColumnValueWidths();
        reorderVariables('[data-merchant-name]', "div.replace-with-merchant-name");
        installEventHandlers();
    }

    function reorderVariables(sourceSelector, targetSelector) {
        var merchantNameElements = $(sourceSelector);
        if (merchantNameElements && merchantNameElements.length > 0) {
            for (var i = 0; i < merchantNameElements.length; i++) {
                var merchantNameElement = merchantNameElements[i];
                var merchantName = merchantNameElement.dataset.merchantName;

                var parent = $(merchantNameElement).closest("ul");
                var label = $(parent).find(targetSelector);
                $(label).text(merchantName);
            }
        }
    }

    function installEventHandlers(submitFn, cancelFn) {
        // delete merchant
        var deleteMerchantPopoverSelector = $('a[data-id="deleteMobileDeviceLink"]');
        var confirmLinkSelector = 'button[data-id="deleteMerchantButton"]';
        var cancelLinkSelector = 'a[data-id="closeDeleteMerchantPopoverButton"]';

        deleteMerchantPopoverSelector.on("click", function (e) {
            e.preventDefault();
        });

        $(deleteMerchantPopoverSelector).popover({
            html: true,
            content: function () {
                return $('#deleteMerchantPopoverContent').html();
            },
            title: function () {
                return $('#deleteMerchantPopoverTitle').html();
            },
            trigger: "click",
            container: "body",
            toggle: "popover",
            placement: "bottom"
        });

        $(deleteMerchantPopoverSelector).on('shown.bs.popover', function () {
            // closest ravels up the DOM tree beginning with the delete link until it finds a match for the supplied form selector
            var form = $(this).closest('form');
            var that = this;

            var submitFnWrapper = function () {
                form.submit();
                var merchantName = $(that).closest('form').find('#MerchantName').val();
                submitFn(merchantName);
            };

            var cancelFnWrapper = function () {
                var merchantName = $(that).closest('form').find('#MerchantName').val();
                cancelFn(merchantName);
            };

            Global.SetCurrentPopover(deleteMerchantPopoverSelector);
            Common.InitializeConfirmationBoxHandlers(confirmLinkSelector, cancelLinkSelector, deleteMerchantPopoverSelector, false, submitFnWrapper, cancelFnWrapper);
        });
    }

    function hideMessage() {
        $('.alert').css('display', 'none');
    }

    function showMessage(message, className) {
        $('.alert').css('display', 'block');

        if (className) {
            $('.alert').removeClass('alert-danger');
            $('.alert').removeClass('alert-warning');
            $('.alert').addClass(className);
        }

        if (message) {
            $('#alert-message').text(message);
        }
    }

    function setupDataTable(ajaxSource, cardId, submitFn, cancelFn) {
        table = $('#merchantWhitelistTable').dataTable({
            sDom: "<'row clearfix'<'col-xs-12 visible-xs dataTables_sort'>>t<'row clearfix'<'col-md-4'i><'col-md-4'p><'col-md-4'<'spinner-container'>>'>",
            iDisplayLength: Common.DISPLAY_LENGTH,
            bLengthChange: false,
            bStateSave: false,
            bFilter: true,
            bSort: true,
            bProcessing: false,
            bAutoWidth: false,
            sPaginationType: 'full_numbers',
            bServerSide: true,
            sAjaxSource: ajaxSource,
            aaSorting: [[1, 'asc']],
            aoColumns: [
                { bSortable: false, bVisible: false }, // Complete table row
                { bSortable: true }, // Desktop + Tablet: Merchant Name
                { bSortable: false }, // Mobile: Merchant Name + delete device link
                { bSortable: true }, // date added
                { bSortable: false }, // Delete Merchant from whitelist link
                { bSortable: false } // Column used as main column in collapsed state
            ],
            oLanguage: {
                "sUrl": Common.DataTable.LOCALIZATION
            },
            fnPreDrawCallback: function () {
                if (!responsiveHelper) {
                    responsiveHelper = new ResponsiveDatatablesHelper(table, Common.DataTable.BREAKPOINT_DEFINITION, expandTableCells);
                }
            },
            fnRowCallback: function (nRow) {
                responsiveHelper.createExpandIcon(nRow);
            },
            fnDrawCallback: function () {
                responsiveHelper.expandAll();
                responsiveHelper.respond();
                Common.TableResizer(table);
                installEventHandlers(submitFn, cancelFn);
                mobileSorting.UpdateMobileSorting();
                table.show();
            },
            fnServerData: function (url, data, callback) {
                $('#merchantWhitelistTableContainer').removeClass('hidden');

                data.push({ "name": "sSearch", "value": cardId });

                Common.AjaxWithErrorHandling(url, data, function (result) {
                    if (result.RedirectTo) {
                        location.href = result.RedirectTo;
                    }

                    callback(result);

                    if (paging) {
                        paging = false;
                        $('html, body').animate({
                            scrollTop: $('#merchantWhitelistTableContainer').offset().top
                        }, 500);
                    }
                }, '.spinner-container', { hideElements: true });
            },
            fnCreatedRow: function (nRow, aData) {
                Common.PostprocessTableRow(table, nRow, aData[0]);
            },
            fnInitComplete: function () {
                $('#merchantWhitelistTable th div').on("click", function (e) {
                    $(this).closest('th').trigger("click");
                    e.stopPropagation();
                });
                mobileSorting.InitializeMobileSorting(table);
                table.on('page', function () {
                    paging = true;
                });
            }
        });
    }

    return {
        PrepareMerchantWhitelist: function (filterUri, cardId, submitFn, cancelFn) {
            Common.InitializeTooltips();
            $(window).on("resize", function () {
                Common.InitializeTooltips();
            });

            DataTablePlugins.Init();
            setupDataTable(filterUri, cardId, submitFn, cancelFn);
        },

        ShowAsynchronousProcessResult: function (sourceId, targetId) {
            $(targetId).html($(sourceId).html());
            window.scrollTo(0, 0);
        },

        InitializeToggleShowPassword: function (options) {
            var control = $(options.control);
            var field = $(options.field);

            if (control.attr("data-show") === "true") {
                control.text(options.hideText);
            } else {
                control.text(options.showText);
                field.val(options.initValue);
            }

            control.on("click", function (e) {
                if (control.attr("data-show") === "true") {
                    control.attr("data-show", "false");
                    control.text(options.showText);
                    field.attr("type", "password");
                } else {
                    control.attr("data-show", "true");
                    control.text(options.hideText);
                    field.attr("type", "text");
                }
                field.first().focus();
                e.preventDefault();
            });
        },

        InitializeToggleShowActivationCode: function (options) {
            var control = $(options.control);
            var element = $(options.element);

            control.on('click', function (e) {
                if (control.attr('data-show') === 'true') {
                    control.attr('data-show', 'false');
                    control.text(options.showText);
                    element.text(options.obfuscatedActivationCode);
                } else {
                    control.attr("data-show", "true");
                    control.text(options.hideText);
                    element.text(options.activationCode);
                }
                e.preventDefault();
            });
        },

        InitializeAddCorrespondenceAddress: function () {
            insertSeparatorInCountryList();
            Common.PrepareForm('#addCorrespondenceAddressForm', '#submitButton');
            $('#countryDropDown').on("change", function () {
                if ($('#zipInput').val()) {
                    $('#zipInput').valid();
                }
            });
        },

        InitializeRemoveCorrespondenceAddress: function () {
            insertSeparatorInCountryList();
            Common.PrepareForm('#removeCorrespondenceAddressForm', '#submitButton');
        },

        InitializeMobileCardOptions: function (redirectToCorrespondingCardSelector, formToSubmitSelector, targetTabName) {
            $(redirectToCorrespondingCardSelector).on("click", function (e) {
                $('#tab').val(targetTabName);
                $(formToSubmitSelector).submit();
                e.preventDefault();
            });
        },

        InitializeMailNotificationSetting: function (supportMode) {
            loadPartialViewOnClick('mailNotification', "#mailInvoiceNotificationSettingForm", '[data-id="changeMailInvoiceNotificationConfirmLink"]',
                '[data-id="mailNotificationSettingContent"]', supportMode, { noSpinner: true });
        },

        InitializeSmsNotificationSetting: function (supportMode) {
            loadPartialViewOnClick('smsNotification', '#smsInvoiceNotificationSettingForm', '[data-id="changeSmsInvoiceNotificationConfirmLink"]',
                '[data-id="smsNotificationSettingContent"]', supportMode, { noSpinner: true });
