                '[data-id="balanceInformationSettingContent"]', supportMode, { noSpinner: true });
        },

        InitializeToggleExchangeRateInfo: function (isSupportMode, submitForm, submitButton) {
            // prevent duplicate form submit caused by double clicks
            var clicked = false;

            $(submitButton).on("click", function (e) {
                if (isSupportMode) {
                    console.log("support mode");
                    e.preventDefault();
                    return;
                }

                if (clicked) {
                    console.log("double click ignored");
                    e.preventDefault();
                    return;
                }

                if ($(submitForm).valid()) {
                    $(submitForm).submit();
                } else {
                    showConfirmationError(true);
                    // make button clickable again, such that form can be submitted when checked
                    clicked = false;
                }

                e.preventDefault();

                // ensure button will be clickable again after a specific time
                clicked = true;
                Common.Delay(function () {
                    clicked = false;
                }, Common.DOUBLECLICK_TIME_IN_MS);
            });
        },

        InitializeOrderWizardStepOne: function (userMustConfirm, confirmationCheckbox, submitForm, submitButton) {
            if (userMustConfirm && confirmationCheckbox) {
                showConfirmationError(false);

                $(confirmationCheckbox).on("click", function () {
                    if ($(confirmationCheckbox).is(':checked')) {
                        showConfirmationError(false);
                    }
                });
            }

            // prevent duplicate form submit caused by double clicks
            var clicked = false;

            $(submitButton).on("click", function (e) {
                if (clicked) {
                    console.log("double click ignored");
                    e.preventDefault();
                    return;
                }

                if ($(submitForm).valid() && (userMustConfirm && $(confirmationCheckbox).is(':checked') || !userMustConfirm || !confirmationCheckbox)) {
                    $(submitForm).submit();
                } else {
                    if (userMustConfirm && !$(confirmationCheckbox).is(':checked')) {
                        showConfirmationError(true);
                    }
                    // make button clickable again, such that form can be submitted when checked
                    clicked = false;
                }

                e.preventDefault();

                // ensure button will be clickable again after a specific time
                clicked = true;
                Common.Delay(function () {
                    clicked = false;
                }, Common.DOUBLECLICK_TIME_IN_MS);
            });
        },

        InitializeReprintPin: function (reprintPinForm, reprintPinButton, redirectToOptionsForm, redirectToOptionsLink) {
            Common.PrepareForm(redirectToOptionsForm, redirectToOptionsLink);
            Common.PrepareForm(reprintPinForm, reprintPinButton);
        },

        InitializeChange: function (submitForm, submitButton) {
            Common.PrepareForm(submitForm, submitButton);
        },

        SetupConfirmationSetting: function (options, supportMode, showConfirmationDialog, formToSubmit, linkToFollow) {
            Common.InitializeConfirmationSetting(options,
                supportMode,
                showConfirmationDialog,
                formToSubmit,
                linkToFollow);
        },

        InitializeCounter: function (supportMode) {
            loadPartialViewOnClick('orderVoucher', '#orderVoucherForm', '[data-id="orderVoucherLink"]', '#creditCardCounterSection', supportMode, {
