{
  "name": "telegraf",
  "version": "3.38.0",
  "description": "Modern Telegram Bot Framework",
  "license": "MIT",
  "author": {
    "name": "Vitaly Domnikov",
    "email": "oss@vitaly.codes"
  },
  "main": "telegraf.js",
  "types": "./typings/index.d.ts",
  "type": "commonjs",
  "bin": {
    "telegraf": "bin/telegraf"
  },
  "files": [
